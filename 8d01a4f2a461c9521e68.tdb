$
 PAR  L(LIQUID,CU,W;0),,                +135590;,,                    N 12Hal !
$
 PAR  L(FCC_A1,CU,W:VA;0),,             +110920;,,                    N 12Hal !
 PAR  L(FCC_A1,CU,W:VA;1),,             +4640;,,                      N 12Hal !
$
 PAR  L(BCC_A2,CU,W:VA;0),,             +144650;,,                    N 12Hal !
 PAR  L(BCC_A2,CU,W:VA;1),,             -7620;,,                      N 12Hal !
$
$ metastable
