$
$ Database for Al-Ho from T. Tokunaga et al. 2013.
$
$ T. Tokunaga, H. Kominato, S. Iikubo, H. Ohtani,
$ Mater. Trans. (JIM), 54, 647-55(2013).
$
$ Dataset created 2019.05.26 by Bengt Hallstedt.
$ 2020.12.20: Modified for use with GES6.
$
$ Modified from 03Cac.
$
$ L(HCP_A3,AL,HO;2) is scary.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT AL   FCC_A1                     26.98154   4540.      28.30   ! 
 ELEMENT HO   HCP_A3                    164.9304    7995.624   75.0191 ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : AL HO : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : AL% HO : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : AL HO : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 %A 2 1 0.5 !
 CONST HCP_A3 : AL HO% : VA : !
$
$ Prototype Al3Ho (hR20, R-3m)
$ Prototype AuCu3 (cP4, Pm-3m) also possible
$
 PHASE AL3HO % 2 3 1 !
 CONST AL3HO : AL : HO : !
$
$ Prototype Cu2Mg (cF24, Fd-3m)
$
 PHASE C15_LAVES % 2 2 1 !
 CONST C15_LAVES : AL% HO : AL HO% : !
$
$ Prototype AlDy (oP16, Pbcm)
$
 PHASE ALHO % 2 1 1 !
 CONST ALHO : AL : HO : !
$
$ Prototype Al2Gd3 (tP20, P4_2nm)
$
 PHASE AL2HO3 % 2 2 3 !
 CONST AL2HO3 : AL : HO : !
$
$ Prototype Co2Si (oP12, Pnma)
$
 PHASE ALHO2_C37 % 2 1 2 !
 CONST ALHO2_C37 : AL : HO : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Al
$
$ BCT_A5 and DIAMOND_A4 added in unary 3.0
$
 PAR  G(FCC_A1,AL:VA),,                 +GHSERAL;                2900 N 91Din !
 PAR  G(BCC_A2,AL:VA),,                 +GHSERAL+10083-4.813*T;  2900 N 91Din !
 PAR  G(HCP_A3,AL:VA),,                 +GHSERAL+5481-1.8*T;     2900 N 91Din !
$PAR  G(CBCC_A12,AL:VA),,               +GHSERAL
$            +10083.4-4.813*T;                                   2900 N 91Din !
$PAR  G(CUB_A13,AL:VA),,                +GHSERAL
$            +10920.44-4.8116*T;                                 2900 N 91Din !
$PAR  G(BCT_A5,AL),,                    +GHSERAL+10083-4.813*T;  2900 N SGCOST !
$PAR  G(DIAMOND_A4,AL),,                +GHSERAL+30*T;           2900 N SGCOST !
 PAR  G(LIQUID,AL),,                    +GLIQAL;                 2900 N 91Din !
$
 PAR  G(C15_LAVES,AL:AL),,              +3*GHSERAL+15000;,,           N 03Cac !
$
 FUNCTION GHSERAL   298.15  -7976.15+137.093038*T-24.3671976*T*LN(T)
       -0.001884662*T**2-8.77664E-07*T**3+74092*T**(-1);
       700.00  Y  -11276.24+223.048446*T-38.5844296*T*LN(T)
       +0.018531982*T**2 -5.764227E-06*T**3+74092*T**(-1);
       933.47  Y  -11278.361+188.684136*T-31.748192*T*LN(T)
       -1.230622E+28*T**(-9);
      2900.00  N !
 FUNCTION GLIQAL    298.15  +11005.045-11.84185*T+GHSERAL+7.9337E-20*T**7;
       933.47  Y  -795.991+177.430209*T-31.748192*T*LN(T);
      2900.00  N !
 FUNCTION GBCCAL    298.15  +GHSERAL+10083-4.813*T;                    2900 N !
$ ------------------------------------------------------------------------------
$ Ho
$
$ BCC_A2 and LIQUID revised in unary 4.4
$ HCP_A3 modified in unary 5.0
$ (upper T interval added + trailing decimals added)
$
$ FCC_A1 from 03Cac not included in unary
$
 PAR  G(HCP_A3,HO:VA),,                 +GHSERHO;                3001 N 91Din !
 PAR  G(FCC_A1,HO:VA),,                 +GHSERHO
             +2000-0.831451*T;                                   3001 N 03Cac !
 PAR  G(BCC_A2,HO:VA),,                 +GHSERHO
             +4380-2.494353*T;                                   3001 N 00Nor1 !
 PAR  G(LIQUID,HO),,                    +GLIQHO;                 3001 N 00Nor1 !
$
 PAR  G(C15_LAVES,HO:HO),,              +3*GHSERHO+15000;,,           N 03Cac !
$
 FUNCTION GHSERHO   298.15  -7612.429+86.593171*T-23.4879*T*LN(T)
       -0.00827315*T**2+2.375467E-06*T**3;
       600.00  Y  -10917.688+182.475691*T-39.6932*T*LN(T)
       +0.01820065*T**2-4.829733E-06*T**3;
       900.00  Y  +46646.188-421.474473*T+48.0595*T*LN(T)
       -0.0424634*T**2+3.233133E-06*T**3-7185900*T**(-1);
      1200.00  Y  +27786.061-156.162846*T+8.28608*T*LN(T)
       -0.01082725*T**2-1.112352E-06*T**3-6183850*T**(-1);
      1703.00  Y  -825364.662+4248.379056*T-558.9506818*T*LN(T)
       +0.139111904*T**2-6.824652E-06*T**3+2.19952973E+08*T**(-1);
      3000.00  Y  -17149.229+235.898104*T-43.932*T*LN(T);
      3001.00  N !
 FUNCTION GLIQHO    298.15  +17383.4216-9.992194*T+GHSERHO;
      1000.00  Y  +124827.5326-994.683024*T+127.9577778*T*LN(T)
       -0.088196514*T**2+8.008222E-6*T**3-15727191*T**(-1);  
      1703.00  Y  -9688.5314+230.793918*T-43.932*T*LN(T);  
      3001.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ T. Tokunaga, H. Kominato, S. Iikubo, H. Ohtani,
$ Mater. Trans. (JIM), 54, 647-55(2013).
$
$ Modified from 03Cac.
$
$ Checked against paper. Checked at 6000 K.
$
$ L(HCP_A3,AL,HO;2) is scary.
$
 PAR  L(LIQUID,AL,HO;0),,               -150000+16*T;,,               N 13Tok !
 PAR  L(LIQUID,AL,HO;1),,               -20820;,,                     N 03Cac !
 PAR  L(LIQUID,AL,HO;2),,               +22050;,,                     N 03Cac !
$
 PAR  L(FCC_A1,AL,HO:VA;0),,            +UN_ASS;,,                    N !
$
 PAR  L(HCP_A3,AL,HO:VA;0),,            -132000+9.78*T;,,             N 13Tok !
 PAR  L(HCP_A3,AL,HO:VA;1),,            -78000-5*T;,,                 N 13Tok !
 PAR  L(HCP_A3,AL,HO:VA;2),,            +140000-100*T;,,              N 13Tok !
$
 PAR  G(AL3HO,AL:HO),,                  +3*GHSERAL+GHSERHO
             -172000+17*T;,,                                          N 13Tok !
$
 PAR  G(C15_LAVES,AL:HO),,              +2*GHSERAL+GHSERHO
             -168000+20*T;,,                                          N 13Tok !
 PAR  G(C15_LAVES,HO:AL),,              +2*GHSERHO+GHSERAL
             +198000-20*T;,,                                          N 13Tok !
$
 PAR  G(ALHO,AL:HO),,                   +GHSERAL+GHSERHO
             -101000+13.4*T;,,                                        N 13Tok !
 PAR  G(AL2HO3,AL:HO),,                 +2*GHSERAL+3*GHSERHO
             -222500+32.5*T;,,                                        N 13Tok !
 PAR  G(ALHO2_C37,AL:HO),,              +GHSERAL+2*GHSERHO
             -120000+18.5*T;,,                                        N 13Tok !
$ ------------------------------------------------------------------------------
$
 ASSESSED_SYSTEM
   AL-HO(TDB -* +LIQUID +FCC_A1 +HCP_A3 +C15_LAVES +AL2HO3 +AL3HO +ALHO
   +ALHO2_C37 ;P3 *) !
