$
 PAR  L(LIQUID,AG,GA;0),,               -19643.7987+63.6589658*T
             -8.62025618*T*LN(T);,,                                   N 11Gie !
 PAR  L(LIQUID,AG,GA;1),,               -38747.7601+140.716275*T
             -16.1115178*T*LN(T);,,                                   N 11Gie !
 PAR  L(LIQUID,AG,GA;2),,               -25745.6828+140.455355*T
             -17.4166652*T*LN(T);,,                                   N 11Gie !
$
 PAR  L(FCC_A1,AG,GA:VA;0),,            -19854.0439+22.305971*T;,,    N 11Gie !
 PAR  L(FCC_A1,AG,GA:VA;1),,            -10412.6067-22.5102088*T;,,   N 11Gie !
$
 PAR  L(HCP_A3,AG,GA:VA;0),,            -15203.2953+1.78639448*T;,,   N 11Gie !
 PAR  L(HCP_A3,AG,GA:VA;1),,            -22134.9653+7.17838494*T;,,   N 11Gie !
$
 PAR  G(AG2GA_C22,AG:GA),,              +2*GHCPAG+GHCPGA
             -32689.1668+12.5713314*T;,,                              N 11Gie !
 PAR  L(AG2GA_C22,AG:AG,GA;0),,         +6368.88797-14.8261554*T;,,   N 11Gie !
 PAR  L(AG2GA_C22,AG:AG,GA;1),,         +3641.22858;,,                N 11Gie !
$
 PAR  G(AG3GA2_B2,AG:GA),,              +3*GHSERAG+2*GHSERGA
             -49663.8526+14.6044545*T;,,                              N 11Gie !
