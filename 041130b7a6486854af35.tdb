 PAR  L(BCC_A2,AL,TH:VA;1),,            +219100-3*T;,,                N 10Li !
$
 PAR  G(AL7TH2,AL:TH),,                 +7*GHSERAL+2*GHSERTH
             -332199+60.282*T;,,                                      N 10Li !
 PAR  G(AL3TH_D019,AL:TH),,             +3*GHSERAL+GHSERTH
             -151304+19.22*T;,,                                       N 10Li !
 PAR  G(AL2TH_C32,AL:TH),,              +2*GHSERAL+GHSERTH
             -124554+7.779*T;,,                                       N 10Li !
 PAR  G(AL3TH2,AL:TH),,                 +3*GHSERAL+2*GHSERTH
             -194700+8.595*T;,,                                       N 10Li !
 PAR  G(ALTH_B33,AL:TH),,               +GHSERAL+GHSERTH
             -85900+12.254*T;,,                                       N 10Li !
 PAR  G(AL2TH3_D5A,AL:TH),,             +2*GHSERAL+3*GHSERTH
             -209850+39.995*T;,,                                      N 10Li !
 PAR  G(ALTH2_C16,AL:TH),,              +GHSERAL+2*GHSERTH
