 PAR  L(LIQUID,AL,TH;0),,               -93725+4.523*T;,,             N 10Li !
 PAR  L(LIQUID,AL,TH;1),,               -30000;,,                     N 10Li !
 PAR  L(LIQUID,AL,TH;2),,               -20156+9.264*T;,,             N 10Li !
