$
$ Database for Co-Zn from G.P. Vassilev and M. Jiang 2004
$ (included in LB Vol. 5)
$
$ G.P. Vassilev, M. Jiang, J. Phase Equilib. Diffus., 25, 259-68(2004).
$
$ Dataset created 2015.05.10 by Bengt Hallstedt.
$ 2020.07.31: Modified for use with GES6.
$
$ There is an inverse miscibility gap with a minimum at 2633 K and x(Zn)=0.75.
$
$ COZN_A13 is modelled as bcc in spite of being A13.
$
$ Magnetic parameters were not included for the bcc phase in the paper. In LB
$ bcc was included as a separate phase (COZN_A2). Here the normal bcc with
$ magnetic parameters and the interaction parameters from the paper is included.
$ This makes the bcc phase field somewhat larger.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT CO   HCP_A3                     58.9332    4765.567   30.0400 ! 
 ELEMENT ZN   HCP_A3                     65.38      5657.0     41.63   ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : CO ZN : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : CO% ZN : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : CO ZN : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 %A 2 1 0.5 !
 CONST HCP_A3 : CO% ZN% : VA : !
$
$ Prototype beta-Mn (cP20, P4_132)
$
 PHASE COZN_A13 %B 1 1 !
 CONST COZN_A13 : CO ZN : !
$
$ Prototype Al4Cu9 (cP52, P4-3m)
$
 PHASE CO5ZN21_D83 % 1 1 !
 CONST CO5ZN21_D83 : CO ZN : !
$
$ Prototype CoZn7 (hP60, P6_2)
$
 PHASE COZN7 % 2 1 7 !
 CONST COZN7 : CO : ZN : !
$
$ Prototype Co2Zn15 (mC28, C2/m)
$
 PHASE CO2ZN15 % 2 2 15 !
 CONST CO2ZN15 : CO : ZN : !
$
$ Prototype CoZn13 (mC28, C2/m)
$
 PHASE COZN13 % 2 1 13 !
 CONST COZN13 : CO : ZN : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Co
$
 PAR  G(HCP_A3,CO:VA),,                 +GHSERCO;,,                   N 91Din !
 PAR  TC(HCP_A3,CO:VA),,                 1396.00;,,                   N 91Din !
 PAR  BM(HCP_A3,CO:VA),,                    1.35;,,                   N 91Din !
 PAR  G(FCC_A1,CO:VA),,                 +GHSERCO
             +427.591-0.615248*T;,,                                   N 91Din !
 PAR  TC(FCC_A1,CO:VA),,                 1396.00;,,                   N 91Din !
 PAR  BM(FCC_A1,CO:VA),,                    1.35;,,                   N 91Din !
 PAR  G(BCC_A2,CO:VA),,                 +GHSERCO+2938-0.7138*T;,,     N 91Din !
 PAR  TC(BCC_A2,CO:VA),,                 1450.00;,,                   N 91Din !
 PAR  BM(BCC_A2,CO:VA),,                    1.35;,,                   N 91Din !
$PAR  G(CBCC_A12,CO:VA),,               +GHSERCO+4155;,,              N 91Din !
$PAR  G(CUB_A13,CO:VA),,                +GHSERCO+3155;,,              N 91Din !
 PAR  G(LIQUID,CO),,                    +GLIQCO;,,                    N 91Din !
$
 PAR  G(COZN_A13,CO),,                  +GBCCCO;,,                    N 04Vas !
 PAR  TC(COZN_A13,CO),,                  1450.00;,,                   N 04Vas !
 PAR  BM(COZN_A13,CO),,                     1.35;,,                   N 04Vas !
 PAR  G(CO5ZN21_D83,CO),,               +GHSERCO+10000;,,             N 04Vas !
$
 FUNCTION GHSERCO   298.15  +310.241+133.36601*T-25.0861*T*LN(T)
       -0.002654739*T**2-1.7348E-07*T**3+72527*T**(-1);
      1768.00  Y  -17197.666+253.28374*T-40.5*T*LN(T)+9.3488E+30*T**(-9);
      6000.00  N !
 FUNCTION GLIQCO    298.15  +15085.037-8.931932*T+GHSERCO-2.19801E-21*T**7;
      1768.00  Y  -846.61+243.599944*T-40.5*T*LN(T);
      6000.00  N !
 FUNCTION GFCCCO    298.15  +GHSERCO+427.59-0.615248*T;                6000 N !
 FUNCTION GBCCCO    298.15  +GHSERCO+2938-0.7138*T;                    6000 N !
$ ------------------------------------------------------------------------------
$ Zn
$
 PAR  G(HCP_A3,ZN:VA),,                 +GHSERZN;                1700 N 91Din !
$PAR  G(HCP_A3,ZN:VA),,                 +GHSERZN
$            +2969.82-1.56968*T;                                 1700 N 93Kow !
 PAR  G(FCC_A1,ZN:VA),,                 +GHSERZN
             +2969.82-1.56968*T;                                 1700 N 91Din !
 PAR  G(BCC_A2,ZN:VA),,                 +GHSERZN
             +2886.96-2.5104*T;                                  1700 N 91Din !
$PAR  G(DIAMOND_A4,ZN),,                +GHSERZN+30*T;           1700 N SGCOST !
$PAR  G(BCT_A5,ZN),,                    +GHSERZN
$            +2886.96-2.5104*T;                                  1700 N SGCOST !
$PAR  G(TETRAGONAL_A6,ZN),,             +GHSERZN+4184;           1700 N 96Lee !
$PAR  G(TETRAGONAL_A6,ZN),,             +GHSERZN
$            +2886.96-2.5104*T;                                  1700 N 99SGUN !
$PAR  G(RHOMBOHEDRAL_A7,ZN),,           +GHSERZN+2300+11.5*T;    1700 N 99SGUN !
$PAR  G(RHOMBO_A10,ZN),,                +GLIQZN-2000+10*T;       1700 N 98Han !
$PAR  G(CBCC_A12,ZN:VA),,               +GHSERZN+2000;           1700 N 01Mie !
$PAR  G(CUB_A13,ZN:VA),,                +GHSERZN+2000;           1700 N 01Mie !
 PAR  G(LIQUID,ZN),,                    +GLIQZN;                 1700 N 91Din !
$
 PAR  G(COZN_A13,ZN),,                  +GBCCZN;                 1700 N 04Vas !
 PAR  G(CO5ZN21_D83,ZN),,               +GHSERZN+10000;          1700 N 04Vas !
$
 FUNCTION GHSERZN   298.15  -7285.787+118.470069*T-23.701314*T*LN(T)
       -0.001712034*T**2-1.264963E-06*T**3;
       692.677 Y  -11070.546+172.345644*T-31.38*T*LN(T)+4.7047E+26*T**(-9);
      1700.00  N !
 FUNCTION GLIQZN    298.15  +7157.222-10.29305*T+GHSERZN-3.58949E-19*T**7;
       692.677 Y  -3620.385+161.60854*T-31.38*T*LN(T);
      1700.00  N !
 FUNCTION GBCCZN    298.15  +GHSERZN+2886.96-2.5104*T;                 6000 N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ From G.P. Vassilev and M. Jiang 2004 (included in LB Vol. 5)
$
$ G.P. Vassilev, M. Jiang, J. Phase Equilib. Diffus., 25, 259-68(2004).
$
$ Checked against LB and paper. Checked at 6000 K.
$
$ There is an inverse miscibility gap with a minimum at 2633 K and x(Zn)=0.75.
$
$ COZN_A13 is modelled as bcc in spite of being A13.
$
$ Magnetic parameters were not included for the bcc phase in the paper. In LB
$ bcc was included as a separate phase (COZN_A2). Here the normal bcc with
$ magnetic parameters and the interaction parameters from the paper is included.
$ This makes the bcc phase field somewhat larger.
$
 PAR  L(LIQUID,CO,ZN;0),,               -15017+12.735*T;,,            N 04Vas !
 PAR  L(LIQUID,CO,ZN;1),,               +51758-29.752*T;,,            N 04Vas !
$
 PAR  L(FCC_A1,CO,ZN:VA;0),,            -34275+23.006*T;,,            N 04Vas !
 PAR  L(FCC_A1,CO,ZN:VA;1),,            +18019-9.938*T;,,             N 04Vas !
 PAR  TC(FCC_A1,CO,ZN:VA;0),,            -332;,,                      N 04Vas !
$
 PAR  L(BCC_A2,CO,ZN:VA;0),,            -33588+19.099*T;,,            N 04Vas !
 PAR  L(BCC_A2,CO,ZN:VA;1),,            +15504-2.971*T;,,             N 04Vas !
$
 PAR  L(HCP_A3,CO,ZN:VA;0),,            -20500+22.5*T;,,              N 04Vas !
 PAR  TC(HCP_A3,CO,ZN:VA;0),,            -332;,,                      N 04Vas !
$
 PAR  L(COZN_A13,CO,ZN;0),,             -60380+41.475*T;,,            N 04Vas !
 PAR  L(COZN_A13,CO,ZN;1),,             +48088-30.451*T;,,            N 04Vas !
 PAR  TC(COZN_A13,CO,ZN;0),,             -861;,,                      N 04Vas !
$
 PAR  L(CO5ZN21_D83,CO,ZN;0),,          -75720+27.599*T;,,            N 04Vas !
 PAR  L(CO5ZN21_D83,CO,ZN;1),,          +61919-35*T;,,                N 04Vas !
 PAR  L(CO5ZN21_D83,CO,ZN;2),,          -122000+31.483*T;,,           N 04Vas !
$
 PAR  G(COZN7,CO:ZN),,                  +GHSERCO+7*GHSERZN
             -96408+36.16*T;,,                                        N 04Vas !
 PAR  G(CO2ZN15,CO:ZN),,                +2*GHSERCO+15*GHSERZN
             -127398;,,                                               N 04Vas !
 PAR  G(COZN13,CO:ZN),,                 +GHSERCO+13*GHSERZN
             -93940+21.462*T;,,                                       N 04Vas !
