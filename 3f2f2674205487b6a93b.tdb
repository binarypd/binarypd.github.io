$
$ Database for Ge-Hf from Y. Tang et al. 2014.
$
$ Y. Tang, B. Hu, J. Wang, Y. Du, K. Cheng, D. Liu, M. Yin, P. Nash,
$ J. Mater. Sci., 49, 1306-16(2014).
$
$ Dataset created 2018.07.29 by Bengt Hallstedt.
$ 2021.01.03: Modified for use with GES6.
$
$ The Gibbs energy function for Ge2Hf should be revised.
$
$ The Gibbs energy of Ge2Hf3 was slightly modified.
$
$ Invariant equilibria differ by up to 2 K.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT GE   DIAMOND_A4                 72.59      4636.0     31.09   ! 
 ELEMENT HF   HCP_A3                    178.49      5845.0     43.56   ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : GE HF : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : GE HF : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : GE HF% : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 % 2 1 0.5 !
 CONST HCP_A3 : GE HF% : VA : !
$
$ Prototype C (cF8, Fd-3m)
$
 PHASE DIAMOND_A4 % 1 1 !
 CONST DIAMOND_A4 : GE : !
$
$ Prototype Si2Zr (oC12, Cmcm)
$
 PHASE GE2HF_C49 % 2 2 1 !
 CONST GE2HF_C49 : GE : HF : !
$
$ Prototype Ge4Sm5 (oP36, Pnma)
$
 PHASE GE4HF5 % 2 4 5 !
 CONST GE4HF5 : GE : HF : !
$
$ Prototype Si2U3 (tP10, P4/mbm)
$
 PHASE GE2HF3_D5A % 2 2 3 !
 CONST GE2HF3_D5A : GE : HF : !
$
$ Prototype Mn5Si3 (hP16, P6_3/mcm)
$
 PHASE GE3HF5_D88 % 2 3 5 !
 CONST GE3HF5_D88 : GE : HF : !
$
$ Prototype Al2Cu (tI12, I4/mcm)
$
 PHASE GEHF2_C16 % 2 1 2 !
 CONST GEHF2_C16 : GE : HF : !
$
$ Prototype Ti3P (tP32, P4_2/n)
$
 PHASE GEHF3 % 2 1 3 !
 CONST GEHF3 : GE : HF : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Ge
$
$ BCT_A5 and RHOMBOHEDRAL_A7 added in unary 4.1
$
 PAR  G(DIAMOND_A4,GE),,                +GHSERGE;                3200 N 91Din !
 PAR  G(FCC_A1,GE:VA),,                 +GHSERGE+36000-22.3*T;   3200 N 91Din !
 PAR  G(BCC_A2,GE:VA),,                 +GHSERGE+34100-23.5*T;   3200 N 91Din !
 PAR  G(HCP_A3,GE:VA),,                 +GHSERGE+35000-21.5*T;   3200 N 91Din !
$PAR  G(BCT_A5,GE),,                    +GHSERGE+28800-16.5*T;   3600 N 99SGUN !
$PAR  G(RHOMBOHEDRAL_A7,GE),,           +GHSERGE+29800-16.5*T;   3600 N 99SGUN !
 PAR  G(LIQUID,GE),,                    +GLIQGE;                 3200 N 91Din !
$
 FUNCTION GHSERGE   298.15  -9486.153+165.635573*T-29.5337682*T*LN(T)
       +0.005568297*T**2-1.513694E-06*T**3+163298*T**(-1);
       900.00  Y  -5689.239+102.86087*T-19.8536239*T*LN(T)-0.003672527*T**2;
      1211.40  Y  -9548.204+156.708024*T-27.6144*T*LN(T)-8.59809E+28*T**(-9);
      3200.00  N !
 FUNCTION GLIQGE    298.15  +37141.489-30.687043*T+GHSERGE+8.56632E-21*T**7;
      1211.40  Y  +27243.473+126.324186*T-27.6144*T*LN(T);
      3200.00  N !
$ ------------------------------------------------------------------------------
$ Hf
$
$ HCP_A3 and BCC_A2 modified in unary 5.0
$ (upper T interval added + trailing decimals added)
$
 PAR  G(HCP_A3,HF:VA),,                 +GHSERHF;                3001 N 91Din !
 PAR  G(BCC_A2,HF:VA),,                 +GBCCHF;                 3001 N 91Din !
 PAR  G(FCC_A1,HF:VA),,                 +GHSERHF+10000-2.2*T;    3001 N 91Din !
 PAR  G(LIQUID,HF),,                    +GLIQHF;                 3001 N 91Din !
$
 FUNCTION GHSERHF   298.15  -6987.297+110.744026*T-22.7075*T*LN(T)
       -0.004146145*T**2-4.77E-10*T**3-22590*T**(-1);
      2506.00  Y  -1446776.329+6193.609991*T-787.5363829*T*LN(T)
       +0.1735215*T**2-7.575759E-06*T**3+5.01742495E+08*T**(-1);
      3000.00  Y  -34274.698+277.882368*T-44*T*LN(T);
      3001.00  N !
 FUNCTION GBCCHF    298.15  +5370.703+103.836026*T-22.8995*T*LN(T)
       -0.004206605*T**2+8.71923E-07*T**3-22590*T**(-1)-1.446E-10*T**4;
      2506.00  Y  +1912456.771-8624.20573*T+1087.6141247*T*LN(T)
       -0.286857065*T**2+1.3427829E-05*T**3-6.10085091E+08*T**(-1);
      3000.00  Y  -32498.178+276.723247*T-44*T*LN(T);
      3001.00  N !
 FUNCTION GLIQHF    298.15  +27402.256-10.953093*T+GHSERHF;
      1000.00  Y  +49731.499-149.91739*T+12.116812*T*LN(T)
       -0.021262021*T**2+1.376466E-06*T**3-4449699*T**(-1);
      2506.00  Y  -4247.217+265.470523*T-44*T*LN(T);
      3001.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ Y. Tang, B. Hu, J. Wang, Y. Du, K. Cheng, D. Liu, M. Yin, P. Nash,
$ J. Mater. Sci., 49, 1306-16(2014).
$
$ Checked against paper. Checked at 6000 K.
$
$ The Gibbs energy function for Ge2Hf should be revised.
$
$ The Gibbs energy of Ge2Hf3 was slightly modified.
$
$ Invariant equilibria differ by up to 2 K.
$
 PAR  L(LIQUID,GE,HF;0),,               -139568.71;,,                 N 14Tan !
 PAR  L(LIQUID,GE,HF;1),,               +16458.47;,,                  N 14Tan !
$
 PAR  L(BCC_A2,GE,HF:VA;0),,            +UN_ASS;,,                    N !
 PAR  L(HCP_A3,GE,HF:VA;0),,            +UN_ASS;,,                    N !
$
 PAR  G(GE2HF_C49,GE:HF),,              +3*GGE2HF;,,                  N 14Tan !
 PAR  G(GE4HF5,GE:HF),,                 +4*GHSERGE+5*GHSERHF
             -611344.08+10.81422*T;,,                                 N 14Tan !
$ Original parameter below
$PAR  G(GE2HF3_D5A,GE:HF),,             +2*GHSERGE+3*GHSERHF
$            -327561.45+4.66145*T;,,                                  N 14Tan !
$ Modified parameter below
 PAR  G(GE2HF3_D5A,GE:HF),,             +2*GHSERGE+3*GHSERHF
             -327290+4.66145*T;,,                                     N 14Tan !
 PAR  G(GE3HF5_D88,GE:HF),,             +3*GHSERGE+5*GHSERHF
             -515587.52+8.25032*T;,,                                  N 14Tan !
 PAR  G(GEHF2_C16,GE:HF),,              +GHSERGE+2*GHSERHF
             -187860.66+7.88391*T;,,                                  N 14Tan !
 PAR  G(GEHF3,GE:HF),,                  +4*GGEHF3;,,                  N 14Tan !
$
 FUNCTION GGE2HF    298.15  -76315.2+15.44025*T-0.960553*T*LN(T)
       -0.0364533*T**2+2.96606E-06*T**3+1198396.59*T**(-1);
      6000.00  N !
 FUNCTION GGEHF3    298.15  -68034.79+148.0148*T-24.6875*T*LN(T)
       -0.01050137*T**2+6.70765E-07*T**3+504545.63*T**(-1);
      6000.00  N !
