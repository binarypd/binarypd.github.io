 PAR  L(FCC_A1,CE,ZN:VA;0),,            -2000;,,                      N 08Wan !
 PAR  L(BCC_A2,CE,ZN:VA;0),,            +UN_ASS;,,                    N !
 PAR  L(HCP_A3,CE,ZN:VA;0),,            +UN_ASS;,,                    N !
