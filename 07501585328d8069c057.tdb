 PAR  L(LIQUID,CE,ZN;0),,               -116372.167+39.3*T;,,         N 08Wan !
 PAR  L(LIQUID,CE,ZN;1),,               +70000-19*T;,,                N 08Wan !
 PAR  L(LIQUID,CE,ZN;2),,               -42000+20*T;,,                N 08Wan !
