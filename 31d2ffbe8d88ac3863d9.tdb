$
$ Database for Am-Fe from M. Kurata 2010.
$
$ M. Kurata, IOP Conf. Ser.: Mater. Sci. Eng., 9, 012023(2010).
$
$ Dataset created 2019.08.31 by Bengt Hallstedt.
$ 2020.12.22: Modified for use with GES6.
$
$ There is nothing to compare with in the paper. The phase diagram is probably
$ unknown.
$
$ There is an inverse miscibility gap with a minimum at 2382 K and x(Fe)=0.60.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT AM   DHCP                      243.        6407.00    55.3960 ! 
 ELEMENT FE   BCC_A2                     55.847     4489.0     27.2797 ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : AM FE : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m), delta
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : AM% FE% : VA : !
$
$ Bcc (cI2, Im-3m), epsilon
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : AM% FE% : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 %A 2 1 0.5 !
 CONST HCP_A3 : AM FE : VA : !
$
$ Prototype alpha-La (hP4, P6_3/mmc)
$
 PHASE DHCP % 1 1 !
 CONST DHCP : AM% FE : !
$
$ Prototype MnU6 (tI28, I4/mcm)
$
 PHASE AM6FE_D2C % 2 6 1 !
 CONST AM6FE_D2C : AM : FE : !
$
$ Prototype Cu2Mg (cF24, Fd-3m)
$
 PHASE AMFE2_C15 % 2 1 2 !
 CONST AMFE2_C15 : AM : FE : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Am
$
$ HCP_A3, ORTHO_AC, TETRAG_AD, ALPHA_PU, BETA_PU, GAMMA_PU, TETRAGONAL_A6
$ ORTHORHOMBIC_A20 and TETRAGONAL_U from 10Kur2 not included in unary
$
 PAR  G(DHCP,AM),,                      +GHSERAM;                3000 N 91Din !
 PAR  G(FCC_A1,AM:VA),,                 +GFCCAM;                 3000 N 91Din !
 PAR  G(BCC_A2,AM:VA),,                 +GBCCAM;                 3000 N 91Din !
 PAR  G(HCP_A3,AM:VA),,                 +GHSERAM+5000;           3000 N 10Kur2 !
$PAR  G(ORTHO_AC,AM),,                  +GHSERAM+5000;           3000 N 10Kur2 !
$PAR  G(TETRAG_AD,AM),,                 +GHSERAM+5000;           3000 N 10Kur2 !
$PAR  G(ALPHA_PU,AM),,                  +GHSERAM+5000;           3000 N 10Kur2 !
$PAR  G(BETA_PU,AM),,                   +GHSERAM+5000;           3000 N 10Kur2 !
$PAR  G(GAMMA_PU,AM),,                  +GHSERAM+5000;           3000 N 10Kur2 !
$PAR  G(TETRAGONAL_A6,AM),,             +GHSERAM+5000;           3000 N 10Kur2 !
$PAR  G(ORTHORHOMBIC_A20,AM),,          +GHSERAM+5000;           3000 N 10Kur2 !
$PAR  G(TETRAGONAL_U,AM),,              +GHSERAM+5000;           3000 N 10Kur2 !
 PAR  G(LIQUID,AM),,                    +GHSERAM
             +19910.7-14.1205*T;                                 3000 N 91Din !
$
 FUNCTION GHSERAM   298.15  -6639.201+89.645685*T-21.1868*T*LN(T)
       -0.00559955*T**2-5.41038E-07*T**3-30424*T**(-1);
      1329.00  Y  -21702.938+241.107269*T-41.84*T*LN(T);
      3000.00  N !
 FUNCTION GFCCAM    298.15  -5224.899+99.204329*T-23.1377*T*LN(T)
       -0.00294694*T**2-6.64773E-07*T**3-18507*T**(-1);
      1018.00  Y  -2935.853+73.800069*T-19.4406*T*LN(T)
       -0.005418*T**2-3.75233E-07*T**3-260435*T**(-1);
      1548.70  Y  -476.655+0.246538*T+GHSERAM;
      3000.00  N !
 FUNCTION GBCCAM    298.15  +5973.805-4.531331*T+GHSERAM
       +5E-08*T**2+5E-12*T**3;
       999.00  Y  -7800.332+63.93115*T-15.8832*T*LN(T)
       -0.0190671*T**2+2.291117E-06*T**3+2287195*T**(-1);
      1339.00  Y  -13153.887+219.600832*T-39.748*T*LN(T);
      1449.00  Y  +70352.138-326.394464*T+33.413*T*LN(T)
       -0.02736485*T**2+1.801717E-06*T**3-17379450*T**(-1);
      2183.60  Y  +4777.694-3.740241*T+GHSERAM;
      3000.00  N !
 FUNCTION GLIQAM    298.15  +GHSERAM+19910.7-14.1205*T;                6000 N !
$ ------------------------------------------------------------------------------
$ Fe
$
$ ORTHORHOMBIC_A20 and TETRAGONAL_U added im unary 4.1
$
$ DHCP, TETRAGONAL_A6, ORTHORHOMBIC_A20, TETRAGONAL_U, ORTHO_AC, TETRAG_AD,
$ ALPHA_PU, BETA_PU and GAMMA_PU from 10Kur2 not included in unary
$
 PAR  G(BCC_A2,FE:VA),,                 +GHSERFE;,,                   N 91Din !
 PAR  TC(BCC_A2,FE:VA),,                 1043.00;,,                   N 91Din !
 PAR  BM(BCC_A2,FE:VA),,                    2.22;,,                   N 91Din !
 PAR  G(FCC_A1,FE:VA),,                 +GFCCFE;,,                    N 91Din !
 PAR  TC(FCC_A1,FE:VA),,                 -201.00;,,                   N 91Din !
 PAR  BM(FCC_A1,FE:VA),,                   -2.10;,,                   N 91Din !
 PAR  G(HCP_A3,FE:VA),,                 +GHCPFE;,,                    N 91Din !
 PAR  G(DHCP,FE),,                      +GFCCFE+10000;,,              N 10Kur2 !
$PAR  G(TETRAGONAL_A6,FE),,             +GFCCFE+10000;,,              N 10Kur2 !
$PAR  G(CBCC_A12,FE:VA),,               +GHSERFE+4745;,,              N 91Din !
$PAR  G(CUB_A13,FE:VA),,                +GHSERFE+3745;,,              N 91Din !
$PAR  G(ORTHORHOMBIC_A20,FE),,          +GHSERFE+5000;,,              N 99SGUN !
$PAR  G(ORTHORHOMBIC_A20,FE),,          +GFCCFE+10000;,,              N 10Kur2 !
$PAR  G(TETRAGONAL_U,FE),,              +GHSERFE+5000;,,              N 99SGUN !
$PAR  G(TETRAGONAL_U,FE),,              +GFCCFE+10000;,,              N 10Kur2 !
$PAR  G(ORTHO_AC,FE),,                  +GFCCFE+10000;,,              N 10Kur2 !
$PAR  G(TETRAG_AD,FE),,                 +GFCCFE+10000;,,              N 10Kur2 !
$PAR  G(ALPHA_PU,FE),,                  +GFCCFE+10000;,,              N 10Kur2 !
$PAR  G(BETA_PU,FE),,                   +GFCCFE+10000;,,              N 10Kur2 !
$PAR  G(GAMMA_PU,FE),,                  +GFCCFE+10000;,,              N 10Kur2 !
 PAR  G(LIQUID,FE),,                    +GLIQFE;,,                    N 91Din !
$
 FUNCTION GHSERFE   298.15  +1225.7+124.134*T-23.5143*T*LN(T)
       -0.00439752*T**2-5.8927E-08*T**3+77359*T**(-1);
      1811.00  Y  -25383.581+299.31255*T-46*T*LN(T)+2.29603E+31*T**(-9);
      6000.00  N !
 FUNCTION GFCCFE    298.15  -1462.4+8.282*T-1.15*T*LN(T)
       +6.4E-04*T**2+GHSERFE;
      1811.00  Y  -1713.815+0.940009*T+GHSERFE+4.9251E+30*T**(-9);
      6000.00  N !
 FUNCTION GHCPFE    298.15  -3705.78+12.591*T-1.15*T*LN(T)
       +6.4E-04*T**2+GHSERFE;
      1811.00  Y  -3957.195+5.249009*T+GHSERFE+4.9251E+30*T**(-9);
      6000.00  N !
 FUNCTION GLIQFE    298.15  +12040.17-6.55843*T+GHSERFE-3.67516E-21*T**7;
      1811.00  Y  -10838.83+291.302*T-46*T*LN(T);
      6000.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ M. Kurata, IOP Conf. Ser.: Mater. Sci. Eng., 9, 012023(2010).
$
$ Checked at 6000 K.
$
$ There is nothing to compare with in the paper. The phase diagram is probably
$ unknown.
$
$ There is an inverse miscibility gap with a minimum at 2382 K and x(Fe)=0.60.
$
 PAR  L(LIQUID,AM,FE;0),,               -40000+30*T;,,                N 10Kur2 !
 PAR  L(LIQUID,AM,FE;1),,               -10000;,,                     N 10Kur2 !
 PAR  L(LIQUID,AM,FE;2),,               -5000;,,                      N 10Kur2 !
$
 PAR  L(FCC_A1,AM,FE:VA;0),,            +30000;,,                     N 10Kur2 !
 PAR  L(BCC_A2,AM,FE:VA;0),,            +15000;,,                     N 10Kur2 !
 PAR  L(HCP_A3,AM,FE:VA;0),,            +30000;,,                     N 10Kur2 !
 PAR  L(DHCP,AM,FE;0),,                 +30000;,,                     N 10Kur2 !
$
 PAR  G(AM6FE_D2C,AM:FE),,              +6*GLIQAM+GLIQFE
             -141000+90*T;,,                                          N 10Kur2 !
 PAR  G(AMFE2_C15,AM:FE),,              +GLIQAM+2*GLIQFE
             -57000+26*T;,,                                           N 10Kur2 !
