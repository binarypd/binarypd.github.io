 PAR  L(LIQUID,NI,SM;0),,               -146014+24.836*T;,,           N 98Su !
 PAR  L(LIQUID,NI,SM;1),,               -98811+43.433*T;,,            N 98Su !
$
 PAR  G(NI17SM2,NI:SM),,                +17*GHSERNI+2*GHSERSM
             -476545+113.788*T;,,                                     N 98Su !
 PAR  G(NI5SM_D2D,NI:SM),,              +5*GHSERNI+GHSERSM
             -212777+42.174*T;,,                                      N 98Su !
