 PAR  L(LIQUID,GA,TL;0),,               +15460.67+0.61255863*T;,,     N 17Kim !
 PAR  L(LIQUID,GA,TL;1),,               +1550.8555-0.65678765*T;,,    N 17Kim !
 PAR  L(LIQUID,GA,TL;2),,               +1966.55+0.4753241*T;,,       N 17Kim !
$
 PAR  L(BCC_A2,GA,TL:VA;0),,            +50000;,,                     N 18Hal !
 PAR  L(HCP_A3,GA,TL:VA;0),,            +50000;,,                     N 18Hal !
