$
$ Database for Er-Ge from K. Achgar et al. 2017.
$
$ K. Achgar, S. Kardellass, N. Selhaoui, L. Bouirden,
$ J. Alloys Compd., 724, 1157-66(2017).
$
$ Dataset created 2018.06.24 by Bengt Hallstedt.
$ 2020.12.26: Modified for use with GES6.
$
$ Invariant equilibria differ by up to 16 K because of rounding errors.
$
$ There is also a version in the paper using the Kaptay model for the liquid.
$ This does not seem to bring any advantages and results are very similar.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT ER   HCP_A3                    167.26      7392.2912  73.17816! 
 ELEMENT GE   DIAMOND_A4                 72.59      4636.0     31.09   ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : ER GE : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : ER GE : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : ER GE : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 % 2 1 0.5 !
 CONST HCP_A3 : ER% GE : VA : !
$
$ Prototype C (cF8, Fd-3m)
$
 PHASE DIAMOND_A4 % 1 1 !
 CONST DIAMOND_A4 : GE : !
$
$ Prototype Mn5Si3 (hP16, P6_3/mcm)
$
 PHASE ER5GE3_D88 % 2 5 3 !
 CONST ER5GE3_D88 : ER% GE : GE : !
$
$ Prototype Ge4Sm5 (oP36, Pnma)
$
 PHASE ER5GE4 % 2 5 4 !
 CONST ER5GE4 : ER : GE : !
$
$ Prototype Ge10Ho11 (tI84, I4/mmm)
$
 PHASE ER11GE10 % 2 11 10 !
 CONST ER11GE10 : ER : GE : !
$
$ Prototype CrB (oC8, Cmcm)
$
 PHASE ERGE_B33 % 2 1 1 !
 CONST ERGE_B33 : ER : GE : !
$
$ Prototype Er3Ge4 (oC28, Cmcm, 63)
$
 PHASE ER3GE4 % 2 3 4 !
 CONST ER3GE4 : ER : GE : !
$
$ Prototype Er2Ge3 (mC6, C2/m, 12)
$
 PHASE ER2GE3_LT % 2 2 3 !
 CONST ER2GE3_LT : ER : GE : !
$
$ Prototype AlB2 (hP3, P6/mmm)
$
 PHASE ER2GE3_C32 % 2 2 3 !
 CONST ER2GE3_C32 : ER : GE : !
$
$ Prototype DyGe2 (oC24, Cmc2_1, 36)
$
 PHASE ERGE2_LT % 2 1 2 !
 CONST ERGE2_LT : ER : GE : !
$
$ unknown structure
$
 PHASE ERGE2_MT % 2 1 2 !
 CONST ERGE2_MT : ER : GE : !
$
$ unknown structure
$
 PHASE ERGE2_HT % 2 1 2 !
 CONST ERGE2_HT : ER : GE : !
$
$ Prototype DyGe3 (oC16, Cmcm)
$
 PHASE ER2GE5 % 2 2 5 !
 CONST ER2GE5 : ER : GE : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Er
$
$ BCC_A2 added in unary 4.4
$
$ FCC_A1 from 02Cac not included in unary (but it should be)
$ FCC_A1 from 99Du and 11Don not included in unary
$
 PAR  G(HCP_A3,ER:VA),,                 +GHSERER;                3200 N 91Din !
 PAR  G(FCC_A1,ER:VA),,                 +GHSERER+5000;           3200 N 99Du !
$PAR  G(FCC_A1,ER:VA),,                 +GHSERER
$            +1950-0.831451*T;                                   3200 N 02Cac !
$PAR  G(FCC_A1,ER:VA),,                 +GHSERER+2000;           3200 N 11Don !
 PAR  G(BCC_A2,ER:VA),,                 +GHSERER
             +4600-2.494353*T;                                   3200 N 00Nor1 !
 PAR  G(LIQUID,ER),,                    +GLIQER;                 3200 N 91Din !
$
 FUNCTION GHSERER   298.15  -8489.136+116.698964*T-28.3846744*T*LN(T)
       +9.95792E-04*T**2-9.52557E-07*T**3+9581*T**(-1);
      1802.00  Y  -445688.206+2233.10212*T-298.135131*T*LN(T)
       +0.065950553*T**2-3.041405E-06*T**3+1.23973199E+08*T**(-1);
      3200.00  N !
 FUNCTION GLIQER    298.15  +19382.102-10.241846*T+GHSERER;
       500.00  Y  +17912.678+0.355564*T-12.0761776*T*LN(T)
       -0.014414687*T**2+1.316517E-06*T**3-528122*T**(-1);
      1802.00  Y  +747.131+187.623024*T-38.702*T*LN(T);
      3200.00  N !
$ ------------------------------------------------------------------------------
$ Ge
$
$ BCT_A5 and RHOMBOHEDRAL_A7 added in unary 4.1
$ CUB_A13 from 14Ber1 not included in unary database
$
 PAR  G(DIAMOND_A4,GE),,                +GHSERGE;                3200 N 91Din !
 PAR  G(FCC_A1,GE:VA),,                 +GHSERGE+36000-22.3*T;   3200 N 91Din !
 PAR  G(BCC_A2,GE:VA),,                 +GHSERGE+34100-23.5*T;   3200 N 91Din !
 PAR  G(HCP_A3,GE:VA),,                 +GHSERGE+35000-21.5*T;   3200 N 91Din !
$PAR  G(BCT_A5,GE),,                    +GHSERGE+28800-16.5*T;   3600 N 99SGUN !
$PAR  G(RHOMBOHEDRAL_A7,GE),,           +GHSERGE+29800-16.5*T;   3600 N 99SGUN !
$PAR  G(CUB_A13,GE:VA),,                +GHSERGE+35000-23*T;     3200 N 14Ber1 !
 PAR  G(LIQUID,GE),,                    +GLIQGE;                 3200 N 91Din !
$
 PAR  G(ER5GE3_D88,GE:GE),,             +8*GHSERGE+196000;,,          N 17Ach !
$
 FUNCTION GHSERGE   298.15  -9486.153+165.635573*T-29.5337682*T*LN(T)
       +0.005568297*T**2-1.513694E-06*T**3+163298*T**(-1);
       900.00  Y  -5689.239+102.86087*T-19.8536239*T*LN(T)-0.003672527*T**2;
      1211.40  Y  -9548.204+156.708024*T-27.6144*T*LN(T)-8.59809E+28*T**(-9);
      3200.00  N !
 FUNCTION GLIQGE    298.15  +37141.489-30.687043*T+GHSERGE+8.56632E-21*T**7;
      1211.40  Y  +27243.473+126.324186*T-27.6144*T*LN(T);
      3200.00  N !
 FUNCTION GHCPGE    298.15  +GHSERGE+35000-21.5*T;                     6000 N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ K. Achgar, S. Kardellass, N. Selhaoui, L. Bouirden,
$ J. Alloys Compd., 724, 1157-66(2017).
$
$ Checked against paper. Checked at 6000 K.
$
$ Invariant equilibria differ by up to 16 K because of rounding errors.
$
$ There is also a version in the paper using the Kaptay model for the liquid.
$ This does not seem to bring any advantages and results are very similar.
$
 PAR  L(LIQUID,ER,GE;0),,               -332223.74+34.76*T;,,         N 17Ach !
 PAR  L(LIQUID,ER,GE;1),,               -1300.26;,,                   N 17Ach !
 PAR  L(LIQUID,ER,GE;2),,               +97616.01;,,                  N 17Ach !
$
 PAR  L(HCP_A3,ER,GE:VA;0),,            +UN_ASS;,,                    N !
$
 PAR  G(ER5GE3_D88,ER:GE),,             +5*GHSERER+3*GHSERGE
             -731600.96+23.68*T;,,                                    N 17Ach !
 PAR  L(ER5GE3_D88,ER,GE:GE;0),,        -928000-24*T;,,               N 17Ach !
$
 PAR  G(ER5GE4,ER:GE),,                 +5*GHSERER+4*GHSERGE
             -835353.36+10.26*T;,,                                    N 17Ach !
 PAR  G(ER11GE10,ER:GE),,               +11*GHSERER+10*GHSERGE
             -1960960.47+31.5*T;,,                                    N 17Ach !
 PAR  G(ERGE_B33,ER:GE),,               +GHSERER+GHSERGE
             -187334.7+4.98*T;,,                                      N 17Ach !
 PAR  G(ER3GE4,ER:GE),,                 +3*GHSERER+4*GHSERGE
             -616656.74+16.66*T;,,                                    N 17Ach !
 PAR  G(ER2GE3_LT,ER:GE),,              +2*GHSERER+3*GHSERGE
             -420297+5.9*T;,,                                         N 17Ach !
 PAR  G(ER2GE3_C32,ER:GE),,             +2*GHSERER+3*GHSERGE
             -419677.5+5.4*T;,,                                       N 17Ach !
 PAR  G(ERGE2_LT,ER:GE),,               +GHSERER+2*GHSERGE
             -221632.68+7.09246*T;,,                                  N 17Ach !
 PAR  G(ERGE2_MT,ER:GE),,               +GHSERER+2*GHSERGE
             -221617.98+7.08*T;,,                                     N 17Ach !
 PAR  G(ERGE2_HT,ER:GE),,               +GHSERER+2*GHSERGE
             -220544.01+6.21*T;,,                                     N 17Ach !
 PAR  G(ER2GE5,ER:GE),,                 +2*GHSERER+5*GHSERGE
             -457800+25.97*T;,,                                       N 17Ach !
