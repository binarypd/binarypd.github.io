 PAR  G(LIQUID,CU2MG),,                 +2*GLIQCU+GLIQMG
             -28312+9.595*T;,,                                        N 07Zho !
 PAR  G(LIQUID,CUMG2),,                 +GLIQCU+2*GLIQMG
             -108077+748.301*T-98.984*T*LN(T)+0.02258*T**2
             +2499901*T**(-1);,,                                      N 07Zho !
 PAR  L(LIQUID,CU,CU2MG;0),,            -20012;,,                     N 07Zho !
 PAR  L(LIQUID,CU,CUMG2;0),,            -24230;,,                     N 07Zho !
 PAR  L(LIQUID,CU,MG;0),,               -22611;,,                     N 07Zho !
 PAR  L(LIQUID,CU2MG,CUMG2;0),,         +ZERO;,,                      N 07Zho !
 PAR  L(LIQUID,CU2MG,MG;0),,            -25845;,,                     N 07Zho !
 PAR  L(LIQUID,CUMG2,MG;0),,            +ZERO;,,                      N 07Zho !
$
 PAR  L(FCC_A1,CU,MG:VA;0),,            -19345;,,                     N 07Zho !
$
 PAR  L(HCP_A3,CU,MG:VA;0),,            +39230;,,                     N 07Zho !
$
 PAR  G(C15_LAVES,CU:MG),,              +GC15CUMG;,,                  N 07Zho !
 PAR  G(C15_LAVES,MG:CU),,              +2*GHSERMG+GHSERCU+104160;,,  N 07Zho !
 PAR  L(C15_LAVES,CU,MG:MG;0),,         +3521;,,                      N 07Zho !
 PAR  L(C15_LAVES,CU:CU,MG;0),,         -27868;,,                     N 07Zho !
$
 PAR  G(CUMG2_CB,CU:MG),,               +GCBCUMG;,,                   N 07Zho !
 PAR  G(CUMG2_CB,MG:CU),,               +2*GHSERMG+GHSERCU+116820;,,  N 07Zho !
$
$ Metastable
$
 PAR  L(BCC_A2,CU,MG:VA;0),,            -6500;,,                      N 98Lia2 !
$PAR  L(BCC_A2,CU,MG:VA;0),,            -10000;,,                     N COST !
$
$PAR  G(C14_LAVES,CU:MG),,              -24690.99+364.73085*T
