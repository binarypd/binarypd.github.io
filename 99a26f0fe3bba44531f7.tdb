 PAR  L(LIQUID,CO,SN;0),,               -111091.08+557.90398*T
             -66.556343*T*LN(T);,,                                    N 04Jia !
 PAR  L(LIQUID,CO,SN;1),,               -44585.348+222.1312*T
             -27.092377*T*LN(T);,,                                    N 04Jia !
 PAR  L(LIQUID,CO,SN;2),,               +2252;,,                      N 04Jia !
