 PAR  G(AUHF_B11,AU:HF),,               +GHSERAU+GHSERHF
             -122405.6+19.7163*T;,,                                   N 03Du !
 PAR  L(AUHF_B11,AU:AU,HF;0),,          -191262.5+71.9945*T;,,        N 03Du !
$
 PAR  G(AUHF_BETA,AU:HF),,              +GHSERAU+GHSERHF
             -120169.7+18.1042*T;,,                                   N 03Du !
 PAR  L(AUHF_BETA,AU:AU,HF;0),,         -212086.7+104.2866*T;,,       N 03Du !
