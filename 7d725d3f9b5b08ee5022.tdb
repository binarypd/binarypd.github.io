             -646029+27.684*T;,,                                      N 14Idd !
 PAR  G(HOSN2_C49,HO:SN),,              +GHSERHO+2*GHSERSN
             -195987+15.834*T;,,                                      N 14Idd !
$PAR  G(HO2SN5,HO:SN),,                 +2*GHSERHO+5*GHSERSN
