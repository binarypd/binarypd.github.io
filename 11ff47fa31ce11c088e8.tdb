 PAR  G(BCC_A2,S:VA),,                  +0.5*GS2GAS-31000+50*T;  1300 N 81Fer !
$PAR  G(LIQUID,S),,                     +GLIQSS;                 1300 N 99SGUN !
 PAR  G(LIQUID,VA:S),,                  +0.5*GS2GAS
             -65357+165.396*T-13.513*T*LN(T);                    1300 N 81Fer !
$
 PAR  L(LIQUID,VA:S,VA;0),,             +100*T;,,                     N 81Fer !
 PAR  G(ION,S),,                        +0.5*GS2GAS
             -65357+165.396*T-13.513*T*LN(T);                    1300 N 81Fer !
$
 PAR  G(FES,VA:S),,                     +0.5*GS2GAS+6433+145.376*T;,, N 81Fer !
$
 FUNCTION GHSERSS   298.15  -5228.956+55.401762*T-11.007*T*LN(T)
