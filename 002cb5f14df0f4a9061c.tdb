 PAR  L(FCC_A1,NI,TI:VA;0),,            -105520.9+14.08255*T;,,       N 95Oh !
 PAR  L(FCC_A1,NI,TI:VA;1),,            -99282.7+56.85055*T;,,        N 95Oh !
 PAR  TC(FCC_A1,NI,TI:VA;0),,           -9549.1;,,                    N 95Oh !
 PAR  TC(FCC_A1,NI,TI:VA;1),,           +6612.8;,,                    N 95Oh !
