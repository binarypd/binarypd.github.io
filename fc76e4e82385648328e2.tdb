 PAR  L(LIQUID,MG,TL;0),,               -26000;,,                     N 08Liu !
 PAR  L(LIQUID,MG,TL;1),,               -10500;,,                     N 08Liu !
$
 PAR  L(HCP_A3,MG,TL:VA;0),,            -9723-7.9*T;,,                N 08Liu !
 PAR  L(HCP_A3,MG,TL:VA;1),,            -4000-12.4*T;,,               N 08Liu !
