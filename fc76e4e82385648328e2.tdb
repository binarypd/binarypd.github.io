 PAR  L(BCC_A2,MG,TL:VA;0),,            -8220;,,                      N 08Liu !
$
 PAR  G(MG5TL2_D8G,MG:TL),,             +5*GHSERMG+2*GHSERTL
             -50000-12.9*T;,,                                         N 08Liu !
 PAR  G(MG2TL_C22,MG:TL),,              +2*GHSERMG+GHSERTL
             -25000-2.2*T;,,                                          N 08Liu !
 PAR  G(MGTL_B2,MG:TL),,                +GHSERMG+GHSERTL
