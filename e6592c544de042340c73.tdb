 PAR  L(LIQUID,AG,HG;0),,               -12300.7+8.88*T;,,            N 12Liu !
 PAR  L(LIQUID,AG,HG;1),,               -10490.8;,,                   N 12Liu !
 PAR  L(LIQUID,AG,HG;2),,               -950.955;,,                   N 12Liu !
$
 PAR  L(FCC_A1,AG,HG:VA;0),,            -16000+13.9*T;,,              N 12Liu !
 PAR  L(FCC_A1,AG,HG:VA;1),,            -18622.9;,,                   N 12Liu !
$
 PAR  G(AG11HG9_A3,AG:HG),,             +11*GHSERAG+9*GRHOHG
             -85723.6-40.08*T;,,                                      N 12Liu !
 PAR  G(AG11HG15,AG:HG),,               +11*GHSERAG+15*GRHOHG
             -85239.5-65.47*T;,,                                      N 12Liu !
