             -260509.2+8.424*T;,,                                     N 15Otm !
 PAR  G(AU2DY_C11B,AU:DY),,             +2*GHSERAU+GHSERDY
             -219658.29+3.9*T;,,                                      N 15Otm !
 PAR  G(AUDY_B33,AU:DY),,               +GHSERAU+GHSERDY
             -180572.6+15.2478*T;,,                                   N 15Otm !
 PAR  G(AUDY_B2,AU:DY),,                +GHSERAU+GHSERDY
             -180569.2+15.2456*T;,,                                   N 15Otm !
 PAR  G(AUDY2_C37,AU:DY),,              +GHSERAU+2*GHSERDY
             -183758.04+9.204*T;,,                                    N 15Otm !
