      2157.00  Y  -738.383-4.905143*T+GHSERPU;
      3000.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ A. Perron, P.E.A. Turchi, A. Landa, B. Oudot, B. Ravat, F. Delaunay,
$ J. Nucl. Mater., 482, 187-200(2016).
$
$ Checked against paper. Checked at 6000 K.
$
$ Modified from 04Tur to remove a liquid miscibility gap.
$
$ This is option 1 from their paper.
$
$ The AlPu-A12 parameters had to be adjusted to reproduce the correct
$ stability range.
$
 PAR  L(LIQUID,AL,PU;0),,               -19000-40*T;,,                N 16Per !
 PAR  L(LIQUID,AL,PU;1),,               -1000-30*T;,,                 N 16Per !
 PAR  L(LIQUID,AL,PU;2),,               -30000+10*T;,,                N 16Per !
$
 PAR  L(FCC_A1,AL,PU:VA;0),,            -94000+18*T;,,                N 04Tur !
 PAR  L(FCC_A1,AL,PU:VA;1),,            -10000-8*T;,,                 N 04Tur !
$
 PAR  L(BCC_A2,AL,PU:VA;0),,            -98000+10*T;,,                N 04Tur !
 PAR  L(BCC_A2,AL,PU:VA;1),,            -3500-28*T;,,                 N 04Tur !
$
 PAR  L(TETRAGONAL_A6,AL,PU;0),,        +ZERO;,,                      N 04Tur !
 PAR  L(ALPHA_PU,AL,PU;0),,             +31500;,,                     N 04Tur !
 PAR  L(GAMMA_PU,AL,PU;0),,             +ZERO;,,                      N 04Tur !
$
 PAR  G(AL4PU_D1B,AL:PU),,              +4*GHSERAL+GHSERPU
             -180000+25*T;,,                                          N 04Tur !
 PAR  G(AL3PU_9HA,AL:PU),,              +3*GHSERAL+GHSERPU
             -180000+30*T;,,                                          N 04Tur !
 PAR  G(AL3PU_9HB,AL:PU),,              +3*GHSERAL+GHSERPU
             -112800-26*T;,,                                          N 04Tur !
 PAR  G(AL3PU_6H,AL:PU),,               +3*GHSERAL+GHSERPU
             -102400-34*T;,,                                          N 04Tur !
 PAR  G(AL3PU_L12,AL:PU),,              +3*GHSERAL+GHSERPU
             -96600-38*T;,,                                           N 04Tur !
 PAR  G(AL2PU_C15,AL:PU),,              +2*GHSERAL+GHSERPU
             -138000+0.2*T;,,                                         N 04Tur !
$ Original parameter below
$PAR  G(ALPU_A12,AL:PU),,               +GHSERAL+GHSERPU
$            -68256-4.318*T;,,                                        N 04Tur !
$ Modified parameter below
 PAR  G(ALPU_A12,AL:PU),,               +GHSERAL+GHSERPU
             -68291.54-4.21785*T;,,                                   N 04Tur !
 PAR  G(ALPU3,AL:PU),,                  +GHSERAL+3*GHSERPU
             -65500-21.4*T;,,                                         N 04Tur !
