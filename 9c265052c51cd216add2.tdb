 PAR  L(LIQUID,CU,GE;0),,               -25615.6-3.207895*T;,,        N Rand !
 PAR  L(LIQUID,CU,GE;1),,               -35030.68+7.92245*T;,,        N Rand !
 PAR  L(LIQUID,CU,GE;2),,               -11593.706;,,                 N Rand !
$
 PAR  L(FCC_A1,CU,GE:VA;0),,            -61997.497+49.8802*T;,,       N Rand !
 PAR  L(FCC_A1,CU,GE:VA;1),,            +2134.934-51.589*T;,,         N Rand !
$
 PAR  L(HCP_A3,CU,GE:VA;0),,            -52051.788+36.381*T;,,        N Rand !
 PAR  L(HCP_A3,CU,GE:VA;1),,            -18195.524-34.987*T;,,        N Rand !
$
 PAR  G(CU3GE,CU:GE),,                  -56603.863+510.72198*T
             -96.1200027*T*LN(T)-0.0113*T**2+1646320*T**(-1);,,       N Rand !
$
$ Metastable
$
 PAR  L(BCC_A2,CU,GE:VA;0),,            -61997.497+49.8802*T;,,       N Same !
 PAR  L(BCC_A2,CU,GE:VA;1),,            +2134.934-51.589*T;,,         N Same !
