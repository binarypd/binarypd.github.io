      1190.00  Y  -11728.229+273.487076*T-50.208*T*LN(T);
      2100.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ X.L. Chen, J. Wang, T.L. Chen, X.D. Lin, M.H. Rong, G.H. Rao, H.Y. Zhou,
$ Calphad, 58, 151-59(2017).
$
$ Checked against paper. Checked at 6000 K.
$
$ There is an inverse miscibility gap with a minimum at 3450K and x(Sm)=0.30.
$
$ BMAG for the intermetallic compounds are incorrect.
$
 PAR  L(LIQUID,FE,SM;0),,               -16522.953+16.8415*T;,,       N 17Che !
 PAR  L(LIQUID,FE,SM;1),,               -4350.289+7.7475*T;,,         N 17Che !
$
 PAR  L(FCC_A1,FE,SM:VA;0),,            +100000;,,                    N 17Che !
 PAR  L(BCC_A2,FE,SM:VA;0),,            +100000;,,                    N 17Che !
 PAR  L(HCP_A3,FE,SM:VA;0),,            +100000;,,                    N 17Che !
$
 PAR  G(FE17SM2,FE:SM),,                -210000+2585.0031*T
             -452.5*T*LN(T)-0.19*T**2+5.5E-6*T**3+1050000*T**(-1);,,  N 17Che !
 PAR  TC(FE17SM2,FE:SM),,                 394.00;,,                   N 17Che !
 PAR  BMAG(FE17SM2,FE:SM),,                34.20;,,                   N 17Che !
$
 PAR  G(FE3SM,FE:SM),,                  +3*GHSERFE+GHSERSM
             -38800+21.3482*T;,,                                      N 17Che !
 PAR  TC(FE3SM,FE:SM),,                   653.00;,,                   N 17Che !
 PAR  BMAG(FE3SM,FE:SM),,                   4.60;,,                   N 17Che !
$
 PAR  G(FE2SM_C15,FE:SM),,              +2*GHSERFE+GHSERSM
             -25500+12.8266*T;,,                                      N 17Che !
 PAR  TC(FE2SM_C15,FE:SM),,               682.00;,,                   N 17Che !
 PAR  BMAG(FE2SM_C15,FE:SM),,               2.68;,,                   N 17Che !
