 PAR  L(LIQUID,SR,TE;0),,               -171563-65.55*T;,,            N 22Yu !
 PAR  L(LIQUID,SR,TE;1),,               -17000;,,                     N 22Yu !
 PAR  L(LIQUID,SR,TE;2),,               +45441;,,                     N 22Yu !
