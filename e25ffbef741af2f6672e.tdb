 PAR  L(BCC_A2,PB,ZR:VA;0),,            -87870.84+43.17287*T;,,       N 98Dix !
 PAR  L(HCP_A3,PB,ZR:VA;0),,            -117853.01+63.46457*T;,,      N 98Dix !
$
 PAR  G(PB2ZR,PB:ZR),,                  -131570.49+367.64883*T
