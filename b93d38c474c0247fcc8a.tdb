$
$ Database for Ni-N from A. Fernandez Guillermet and K. Frisk 1991
$ (included in LB Vol. 4)
$
$ A. Fernandez Guillermet, K. Frisk, Int. J. Thermophys., 12, 417-31(1991).
$
$ Dataset created 2010.02.15 by Bengt Hallstedt.
$ 2021.02.02: Modified for use with GES6.
$
$ In LB this dataset is incorrectly attributed to 91Fri2.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT N    1/2_MOLE_N2(G)             14.007     4335.0     95.751  ! 
 ELEMENT NI   FCC_A1                     58.69      4787.0     29.7955 ! 
$ ------------------------------------------------------------------------------
$ Species
$
 SPECIE  N2                                 N2 !
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : NI N : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : NI : N VA% : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : NI : N VA% : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 % 2 1 0.5 !
 CONST HCP_A3 : NI : N VA% : !
$
$ Prototype Fe4N (cP5, Pm-3m, L'1)
$
 PHASE FE4N_L1 % 2 4 1 !
 CONST FE4N_L1 : NI : N :!
$
 PHASE N2GAS % 1 1 !
 CONST N2GAS : N2 : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
 FUNCTION RTLNP     298.15  +8.31451*T*LN(1E-05*P);                    6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ N
$
 PAR  G(N2GAS,N2),,                     +2*GHSERNN+RTLNP;,,           N 91Din !
 PAR  G(LIQUID,N),,                     +GHSERNN+29950+59.02*T;,,     N 91Din !
$
 FUNCTION GHSERNN   298.15  -3750.675-9.45425*T-12.7819*T*LN(T)
       -0.00176686*T**2+2.681E-09*T**3-32374*T**(-1);
       950.00  Y  -7358.85+17.2003*T-16.3699*T*LN(T)
       -6.5107E-04*T**2+3.0097E-08*T**3+563070*T**(-1);
      3350.00  Y  -16392.8+50.26*T-20.4695*T*LN(T)
       +2.39754E-04*T**2-8.333E-09*T**3+4596375*T**(-1);
      6000.00  N !
$ ------------------------------------------------------------------------------
$ Ni
$
$ FCC_A1 and LIQUID modified in unary 5.0 (slight change in upper T interval)
$ BCT_A5 added in unary 5.0
$
 PAR  G(FCC_A1,NI:VA),,                 +GHSERNI;                3000 N 91Din !
 PAR  TC(FCC_A1,NI:VA),,                  633.00;                3000 N 91Din !
 PAR  BM(FCC_A1,NI:VA),,                    0.52;                3000 N 91Din !
 PAR  G(BCC_A2,NI:VA),,                 +GHSERNI
             +8715.084-3.556*T;                                  3000 N 91Din !
 PAR  TC(BCC_A2,NI:VA),,                  575.00;                3000 N 91Din !
 PAR  BM(BCC_A2,NI:VA),,                    0.85;                3000 N 91Din !
 PAR  G(HCP_A3,NI:VA),,                 +GHSERNI+1046+1.255*T;   3000 N 91Din !
 PAR  TC(HCP_A3,NI:VA),,                  633.00;                3000 N 91Din !
 PAR  BM(HCP_A3,NI:VA),,                    0.52;                3000 N 91Din !
$PAR  G(BCT_A5,NI),,                    +GHSERNI+10023-4.556*T;  3000 N 99Gho !
$PAR  G(CBCC_A12,NI:VA),,               +GHSERNI+3556;           3000 N 91Din !
$PAR  G(CUB_A13,NI:VA),,                +GHSERNI+2092;           3000 N 91Din !
 PAR  G(LIQUID,NI),,                    +GLIQNI;                 3000 N 91Din !
$
 FUNCTION GHSERNI   298.15  -5179.159+117.854*T-22.096*T*LN(T)-0.0048407*T**2;
      1728.00  Y  -27840.62+279.134977*T-43.1*T*LN(T)+1.12754E+31*T**(-9);
      3000.00  N !
 FUNCTION GLIQNI    298.15  +16414.686-9.397*T+GHSERNI-3.82318E-21*T**7;
      1728.00  Y  -9549.817+268.597977*T-43.1*T*LN(T);
      3000.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ From A. Fernandez Guillermet and K. Frisk 1991 (included in LB Vol. 4)
$
$ A. Fernandez Guillermet, K. Frisk, Int. J. Thermophys., 12, 417-31(1991).
$
$ Checked against LB and paper. Checked at 6000 K.
$
$ In LB this dataset is incorrectly attributed to 91Fri2.
$
 PAR  L(LIQUID,N,NI;0),,                +14981;,,                     N 91Fer !
$
 PAR  G(FCC_A1,NI:N),,                  +GHSERNI+GHSERNN
             +38680+143.09*T-10.9*T*LN(T)+0.00438*T**2;,,             N 91Fer !
$
 PAR  G(HCP_A3,NI:N),,                  +GHSERNI+0.5*GHSERNN
             -4409.6+72.93*T-7.36*T*LN(T)+0.00614*T**2;,,             N 91Fer !
$
 PAR  G(FE4N_L1,NI:N),,                 +4*GHSERNI+GHSERNN
             -5393+142.97*T-15.65*T*LN(T)+0.0154*T**2;,,              N 91Fer !
$
$ metastable
$
 PAR  G(BCC_A2,NI:N),,                  +GHSERNI+3*GHSERNN
             +200000+200*T;,,                                         N 91Fri2 !
 PAR  TC(BCC_A2,NI:N),,                  +575;,,                      N 91Fri2 !
 PAR  BM(BCC_A2,NI:N),,                    +0.85;,,                   N 91Fri2 !
$ ------------------------------------------------------------------------------
$
 ASSESSED_SYSTEM
  N-NI(TDB -* +LIQUID +GAS +FCC_A1 +HCP_A3
  ;P3 TMM:300/3000 DOR:GAS STP:.3/2400/-2) !
$
 LIST-OF-REFERENCE
 NUMBER  SOURCE
  Null   'Unknown source'
  Same   'Same or similar interaction as in the corresponding stable phase'
  REFLAV 'Laves phase convention: G(LAVES,X:X)=+3*GHSERXX+15000'
  91Din  'A.T. Dinsdale, Calphad, 15, 317-425(1991).'
  91Fer  'A. Fernandez Guillermet, K. Frisk,
          Int. J. Thermophys., 12, 417-31(1991); Ni-N'
  91Fri2 'K. Frisk, Z. Metallkd., 82, 59-66(1991); Fe-Ni-N'
  99Gho  'G. Ghosh, Metall. Mater. Trans. A, 30A, 1481-94(1999),
          SGTE unary database V.5.0, 2009; Ni'
!
