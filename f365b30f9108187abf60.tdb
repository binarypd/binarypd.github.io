$
$ Database for Ge-V from C.P. Wang et al. 2008
$
$ C.P. Wang, A.Q. Zheng, X.J. Liu, Intermetallics, 16, 544-49(2008).
$
$ Dataset created 2009.09.26 by Bengt Hallstedt.
$ 2021.01.03: Modified for use with GES6.
$
$ There is an inverse miscibility gap with a minimum at 5328 K and x(V)=0.69.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT GE   DIAMOND_A4                 72.59      4636.0     31.09   ! 
 ELEMENT V    BCC_A2                     50.9415    4507.0     30.89   ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : GE V : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : GE V : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : GE V% : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 % 2 1 0.5 !
 CONST HCP_A3 : GE V : VA : !
$
$ Prototype C (cF8, Fd-3m)
$
 PHASE DIAMOND_A4 % 1 1 !
 CONST DIAMOND_A4 : GE : !
$
$ Prototype Ge31V17 (tP192, P-4n2)
$
 PHASE GE31V17 % 2 31 17 !
 CONST GE31V17 : GE : V : !
$
$ Prototype Cr11Ge8 (oP76, Pnma)
$
 PHASE GE8V11 % 2 8 11 !
 CONST GE8V11 : GE : V : !
$
$ Prototype Si3W5 (tI32, I4/mcm)
$ Mn5Si3 type stabilised by impurities
$
 PHASE GE3V5_D8M % 2 3 5 !
 CONST GE3V5_D8M : GE : V : !
$
$ Prototype Cr3Si (cP8, Pm-3n)
$
 PHASE GEV3_A15 % 2 1 3 !
 CONST GEV3_A15 : GE : V : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Ge
$
$ BCT_A5 and RHOMBOHEDRAL_A7 added in unary 4.1
$
 PAR  G(DIAMOND_A4,GE),,                +GHSERGE;                3200 N 91Din !
 PAR  G(FCC_A1,GE:VA),,                 +GHSERGE+36000-22.3*T;   3200 N 91Din !
 PAR  G(BCC_A2,GE:VA),,                 +GHSERGE+34100-23.5*T;   3200 N 91Din !
 PAR  G(HCP_A3,GE:VA),,                 +GHSERGE+35000-21.5*T;   3200 N 91Din !
$PAR  G(BCT_A5,GE),,                    +GHSERGE+28800-16.5*T;   3600 N 99SGUN !
$PAR  G(RHOMBOHEDRAL_A7,GE),,           +GHSERGE+29800-16.5*T;   3600 N 99SGUN !
 PAR  G(LIQUID,GE),,                    +GLIQGE;                 3200 N 91Din !
$
 FUNCTION GHSERGE   298.15  -9486.153+165.635573*T-29.5337682*T*LN(T)
       +0.005568297*T**2-1.513694E-06*T**3+163298*T**(-1);
       900.00  Y  -5689.239+102.86087*T-19.8536239*T*LN(T)-0.003672527*T**2;
      1211.40  Y  -9548.204+156.708024*T-27.6144*T*LN(T)-8.59809E+28*T**(-9);
      3200.00  N !
 FUNCTION GLIQGE    298.15  +37141.489-30.687043*T+GHSERGE+8.56632E-21*T**7;
      1211.40  Y  +27243.473+126.324186*T-27.6144*T*LN(T);
      3200.00  N !
$ ------------------------------------------------------------------------------
$ V
$
 PAR  G(BCC_A2,V:VA),,                  +GHSERVV;                4000 N 91Din !
 PAR  G(FCC_A1,V:VA),,                  +GHSERVV+7500+1.7*T;     4000 N 91Din !
 PAR  G(HCP_A3,V:VA),,                  +GHSERVV+4000+2.4*T;     4000 N 91Din !
$PAR  G(CBCC_A12,V:VA),,                +GHSERVV+9000;           4000 N 91Din !
$PAR  G(CUB_A13,V:VA),,                 +GHSERVV+10000;          4000 N 91Din !
 PAR  G(LIQUID,V),,                     +GLIQVV;                 4000 N 91Din !
$
 FUNCTION GHSERVV   298.15  -7930.43+133.346053*T-24.134*T*LN(T)
       -0.003098*T**2+1.2175E-07*T**3+69460*T**(-1);
       790.00  Y  -7967.842+143.291093*T-25.9*T*LN(T)
       +6.25E-05*T**2-6.8E-07*T**3;
      2183.00  Y  -41689.864+321.140783*T-47.43*T*LN(T)+6.44389E+31*T**(-9);
      4000.00  N !
 FUNCTION GLIQVV    298.15  +20764.117-9.455552*T+GHSERVV-5.19136E-22*T**7;
      2183.00  Y  -19617.51+311.055983*T-47.43*T*LN(T);
      4000.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ C.P. Wang, A.Q. Zheng, X.J. Liu, Intermetallics, 16, 544-49(2008).
$
$ Checked against paper. Checked at 6000 K.
$
$ There is an inverse miscibility gap with a minimum at 5328 K and x(V)=0.69.
$
 PAR  L(LIQUID,GE,V;0),,                -125377+19.223*T;,,           N 08Wan !
 PAR  L(LIQUID,GE,V;1),,                +101606-35.95*T;,,            N 08Wan !
 PAR  L(LIQUID,GE,V;2),,                +51100-15.645*T;,,            N 08Wan !
 PAR  L(LIQUID,GE,V;3),,                -65500+18.52*T;,,             N 08Wan !
$
 PAR  L(BCC_A2,GE,V:VA;0),,             -92064+18.982*T;,,            N 08Wan !
 PAR  L(BCC_A2,GE,V:VA;1),,             +62000-18*T;,,                N 08Wan !
$
 PAR  G(GE31V17,GE:V),,                 +31*GHSERGE+17*GHSERVV
             -1555536+189.408*T;,,                                    N 08Wan !
 PAR  G(GE8V11,GE:V),,                  +8*GHSERGE+11*GHSERVV
             -883709+33.345*T;,,                                      N 08Wan !
 PAR  G(GE3V5_D8M,GE:V),,               +3*GHSERGE+5*GHSERVV
             -367616+189.6*T-23.92*T*LN(T);,,                         N 08Wan !
 PAR  G(GEV3_A15,GE:V),,                +GHSERGE+3*GHSERVV
             -142364+48.56*T-5.972*T*LN(T);,,                         N 08Wan !
$
$ metastable
$
 PAR  L(FCC_A1,GE,V:VA;0),,             -92064+18.982*T;,,            N Same !
 PAR  L(FCC_A1,GE,V:VA;1),,             +62000-18*T;,,                N Same !
 PAR  L(HCP_A3,GE,V:VA;0),,             -92064+18.982*T;,,            N Same !
 PAR  L(HCP_A3,GE,V:VA;1),,             +62000-18*T;,,                N Same !
