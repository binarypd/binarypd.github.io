 PAR  L(LIQUID,GE,K;0),,                -144204+39.985*T;,,           N 12Wan !
 PAR  L(LIQUID,GE,K;1),,                -43739+39.9*T;,,              N 12Wan !
$
 PAR  L(BCC_A2,GE,K:VA;0),,             +UN_ASS;,,                    N !
$
 PAR  G(GE4K,GE:K),,                    +4*GHSERGE+GHSERKK
             -82085-30*T;,,                                           N 12Wan !
 PAR  G(GEK,GE:K),,                     +GHSERGE+GHSERKK
             -41800-22.356*T;,,                                       N 12Wan !
 PAR  G(GEK3,GE:K),,                    +GHSERGE+3*GHSERKK
             -77260-29.448*T;,,                                       N 12Wan !
