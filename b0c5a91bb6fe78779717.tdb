$
 PAR  G(LIQUID,LA3SB2),,                +3*GHSERLA+2*GHSERSB
             -399723.017;,,                                           N 13Wan !
 PAR  L(LIQUID,LA,SB;0),,               -292000;,,                    N 13Wan !
 PAR  L(LIQUID,LA,SB;1),,               -16000;,,                     N 13Wan !
 PAR  L(LIQUID,LA,SB;2),,               +66000;,,                     N 13Wan !
 PAR  L(LIQUID,LA,LA3SB2;0),,           -94973.4386;,,                N 13Wan !
$
 PAR  L(FCC_A1,LA,SB:VA;0),,            +UN_ASS;,,                    N !
 PAR  L(BCC_A2,LA,SB:VA;0),,            +UN_ASS;,,                    N !
$
 PAR  G(LA2SB,LA:SB),,                  +2*GHSERLA+GHSERSB
             -291000+14.439*T;,,                                      N 13Wan !
 PAR  G(LA3SB2,LA:SB),,                 +3*GHSERLA+2*GHSERSB
             -570000+39.66*T;,,                                       N 13Wan !
 PAR  G(LASB_B1,LA:SB),,                +GHSERLA+GHSERSB
