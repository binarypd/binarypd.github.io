      1128.00  Y  -16335.232+268.625903*T-48.7854*T*LN(T);
      1799.00  Y  +9407.099-7.631185*T+GHSERND;
      1800.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ C.P. Wang, H.L. Zhang, A.T. Tang, F.S. Pan, X.J. Liu,
$ J. Alloys Compd., 502, 43-48(2010).
$
$ Checked against paper. Checked at 6000 K.
$
$ Invariant equilibria are up to 6 K different, possibly because of rounding
$ errors when converting from fractional compositions.
$
 PAR  L(LIQUID,BI,ND;0),,               -240000-10*T;,,               N 10Wan !
 PAR  L(LIQUID,BI,ND;1),,               +26500;,,                     N 10Wan !
 PAR  L(LIQUID,BI,ND;2),,               +30000+22*T;,,                N 10Wan !
 PAR  L(LIQUID,BI,ND;3),,               -20000;,,                     N 10Wan !
$
 PAR  L(BCC_A2,BI,ND:VA;0),,            -150000;,,                    N 10Wan !
 PAR  L(BCC_A2,BI,ND:VA;1),,            +50000;,,                     N 10Wan !
$
 PAR  L(DHCP,BI,ND:VA;0),,              +100000;,,                    N 10Wan !
$
 PAR  L(RHOMBOHEDRAL_A7,BI,ND;0),,      +50000;,,                     N 10Wan !
$
 PAR  G(BI2ND,BI:ND),,                  +2*GHSERBI+GHSERND
             -243600+29.49*T;,,                                       N 10Wan !
 PAR  G(BIND_B1,BI:ND),,                +GHSERBI+GHSERND
             -220000+14*T;,,                                          N 10Wan !
 PAR  G(BI3ND4_D73,BI:ND),,             +3*GHSERBI+4*GHSERND
             -774830+79.8*T;,,                                        N 10Wan !
 PAR  G(BI3ND5_D88,BI:ND),,             +3*GHSERBI+5*GHSERND
             -870513.6+119.2*T;,,                                     N 10Wan !
 PAR  G(BIND2,BI:ND),,                  +GHSERBI+2*GHSERND
             -321321+57.6*T;,,                                        N 10Wan !
$
$ Metastable
$
 PAR  L(HCP_A3,BI,ND:VA;0),,            -121500;,,                    N Same !
