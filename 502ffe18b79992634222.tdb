 PAR  L(ION,GA+3:TE-2,TE;0),,           +66623.8-52.8159*T;,,         N 92Oh !
 PAR  L(ION,GA+3:TE-2,TE;1),,           -3278.9-3.2009*T;,,           N 92Oh !
$
 PAR  G(GATE,GA:TE),,                   -94031.7+256.6211*T
             -49.8877*T*LN(T)-0.0085909*T**2;,,                       N 92Oh !
 PAR  G(GA3TE4,GA:TE),,                 -309927.3+758.7019*T
             -162.1256*T*LN(T);,,                                     N 92Oh !
 PAR  G(GA2TE3_B3,GA:TE),,              -232406.6+752.2623*T
