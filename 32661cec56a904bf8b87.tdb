 PAR  L(LIQUID,NI,SC;0),,               -183145.4-14.2658*T;,,        N 14Cao !
 PAR  L(LIQUID,NI,SC;1),,               -6622.2-15.7664*T;,,          N 14Cao !
 PAR  L(LIQUID,NI,SC;2),,               +27055.4;,,                   N 14Cao !
$
 PAR  L(FCC_A1,NI,SC:VA;0),,            -141315.3;,,                  N 14Cao !
$
 PAR  L(BCC_A2,NI,SC:VA;0),,            -81181.6-27.228*T;,,          N 14Cao !
 PAR  L(BCC_A2,NI,SC:VA;1),,            +8916.1;,,                    N 14Cao !
$
 PAR  L(A2_BCC,NI,SC:VA;0),,            -81181.6-27.228*T;,,          N 14Cao !
 PAR  L(A2_BCC,NI,SC:VA;1),,            +8916.1;,,                    N 14Cao !
$
 PAR  G(B2_BCC,NI:SC:VA),,              -56487.6+4.7696*T;,,          N 14Cao !
 PAR  G(B2_BCC,SC:NI:VA),,              -56487.6+4.7696*T;,,          N 14Cao !
 PAR  L(B2_BCC,NI,SC:NI:VA;0),,         +10220.5;,,                   N 14Cao !
 PAR  L(B2_BCC,NI:NI,SC:VA;0),,         +10220.5;,,                   N 14Cao !
 PAR  L(B2_BCC,NI,SC:SC:VA;0),,         +7357.5;,,                    N 14Cao !
 PAR  L(B2_BCC,SC:NI,SC:VA;0),,         +7357.5;,,                    N 14Cao !
$
 PAR  L(HCP_A3,NI,SC:VA;0),,            +ZERO;,,                      N 14Cao !
$
 PAR  G(NI5SC_D2D,NI:SC),,              +5*GHSERNI+GHSERSC
             -185955-4.5438*T;,,                                      N 14Cao !
 PAR  G(NI7SC2,NI:SC),,                 +7*GHSERNI+2*GHSERSC
             -331260.3-31.0716*T;,,                                   N 14Cao !
$
 PAR  G(C15_LAVES,NI:SC),,              +2*GHSERNI+GHSERSC
             -127082.1-21.9378*T;,,                                   N 14Cao !
