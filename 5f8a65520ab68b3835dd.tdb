 PAR  L(LIQUID,AU,CE;0),,               -269142+13.43*T;,,            N 16Mou !
 PAR  L(LIQUID,AU,CE;1),,               -114055+73.21*T;,,            N 16Mou !
 PAR  L(LIQUID,AU,CE;2),,               +49931.9-4.78*T;,,            N 16Mou !
$
 PAR  L(FCC_A1,AU,CE:VA;0),,            -152935;,,                    N 16Mou !
 PAR  L(BCC_A2,AU,CE:VA;0),,            -165689+1.5679*T;,,           N 16Mou !
$
 PAR  G(AU6CE,AU:CE),,                  +6*GHSERAU+GHSERCE
             -320908+81.84*T;,,                                       N 16Mou !
 PAR  G(AU51CE14_LT,AU:CE),,            +51*GHSERAU+14*GHSERCE
             -3635060+352.95*T;,,                                     N 16Mou !
 PAR  G(AU51CE14_HT,AU:CE),,            +51*GHSERAU+14*GHSERCE
             -3452670+200.85*T;,,                                     N 16Mou !
 PAR  G(AU2CE,AU:CE),,                  +2*GHSERAU+GHSERCE
             -198000+2.91*T;,,                                        N 16Mou !
 PAR  G(AU11CE9,AU:CE),,                +11*GHSERAU+9*GHSERCE
             -1518180+41.4*T;,,                                       N 16Mou !
$
 PAR  G(AUCE_B27,AU:CE),,               +GHSERAU+GHSERCE
             -157990+4.864*T;,,                                       N 16Mou !
 PAR  G(AUCE_B27,AU:AU,CE;0),,          -89600+15*T;,,                N 16Mou !
$
 PAR  G(AUCE_B33,AU:CE),,               +GHSERAU+GHSERCE
             -157200+3.96*T;,,                                        N 16Mou !
 PAR  G(AUCE_B33,AU:AU,CE;0),,          -90864+16.3*T;,,              N 16Mou !
$
 PAR  G(AUCE2_C37,AU:CE),,              +GHSERAU+2*GHSERCE
             -183156+13.68*T;,,                                       N 16Mou !
$
$ Metastable
$
 PAR  L(HCP_A3,AU,CE:VA;0),,            -152935;,,                    N Same !
