 PAR  G(HCP_A3,DY:H:H),,                +GHSERDY+1.5*GHSERH2
             -252710.72+195.99*T;,,                                   N 17Fu !
 PAR  L(HCP_A3,DY:H,VA:VA;0),,          -21425.17;,,                  N 17Fu !
 PAR  L(HCP_A3,DY:H,VA:H;0),,           +UN_ASS;,,                    N 17Fu !
 PAR  L(HCP_A3,DY:VA:H,VA;0),,          -23400.769+5.58*T;,,          N 17Fu !
 PAR  L(HCP_A3,DY:H:H,VA;0),,           -23400.769+5.58*T;,,          N 17Fu !
