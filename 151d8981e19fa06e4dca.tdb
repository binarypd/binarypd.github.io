             -158553+43.055*T;,,                                      N 99Du !
 PAR  G(ER2NI7,ER:NI),,                 +2*GHSERER+7*GHSERNI
             -325191+85.759*T;,,                                      N 99Du !
 PAR  G(ERNI4,ER:NI),,                  +GHSERER+4*GHSERNI
             -163912+40.943*T;,,                                      N 99Du !
 PAR  G(ER4NI17,ER:NI),,                +4*GHSERER+17*GHSERNI
             -657453+160.817*T;,,                                     N 99Du !
 PAR  G(ER5NI22,ER:NI),,                +5*GHSERER+22*GHSERNI
             -823178+199.197*T;,,                                     N 99Du !
 PAR  G(ERNI5_D2D,ER:NI),,              +GHSERER+5*GHSERNI
             -165724+38.72*T;,,                                       N 99Du !
 PAR  G(ER2NI17,ER:NI),,                +2*GHSERER+17*GHSERNI
             -341853+79.625*T;,,                                      N 99Du !
