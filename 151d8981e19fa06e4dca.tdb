 PAR  L(FCC_A1,ER,NI:VA;0),,            +100000;,,                    N 99Du !
 PAR  L(HCP_A3,ER,NI:VA;0),,            +100000;,,                    N 99Du !
$
 PAR  G(ER3NI_D011,ER:NI),,             +3*GHSERER+GHSERNI
             -66049+0.511*T;,,                                        N 99Du !
 PAR  G(ER3NI2,ER:NI),,                 +3*GHSERER+2*GHSERNI
             -132207+9.427*T;,,                                       N 99Du !
