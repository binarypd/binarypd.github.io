 PAR  L(LIQUID,GD,SR;0),,               +36500;,,                     N 23Lon !
 PAR  L(LIQUID,GD,SR;1),,               -300;,,                       N 23Lon !
 PAR  L(LIQUID,GD,SR;2),,               +5500;,,                      N 23Lon !
