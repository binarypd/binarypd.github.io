 PAR  L(LIQUID,GE,SB;0),,               +3289.7-0.5212*T;,,           N 11Liu !
$
 PAR  L(DIAMOND_A4,GE,SB;0),,           +79210.071-19.8*T;,,          N 09Wan !
 PAR  L(RHOMBOHEDRAL_A7,GE,SB;0),,      +10695-6.5572*T;,,            N 11Liu !
$
$ Metastable
$
 PAR  L(FCC_A1,GE,SB:VA;0),,            +10695-6.5572*T;,,            N Same !
 PAR  L(BCC_A2,GE,SB:VA;0),,            +10695-6.5572*T;,,            N Same !
 PAR  L(HCP_A3,GE,SB:VA;0),,            +10695-6.5572*T;,,            N Same !
