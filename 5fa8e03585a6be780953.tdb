 PAR  L(FCC_A1,FE,ND:VA;0),,            +114718.6-2.3022*T;,,         N 16Wan !
 PAR  L(BCC_A2,FE,ND:VA;0),,            +121536.1;,,                  N 16Wan !
 PAR  L(DHCP,FE,ND;0),,                 +29686.9+4.4826*T;,,          N 16Wan !
$
 PAR  G(FE17ND2,FE:ND),,                +17*GHSERFE+2*GHSERND
             -161745+83.921*T;,,                                      N 16Wan !
 PAR  TC(FE17ND2,FE:ND),,                 327.00;,,                   N 16Wan !
