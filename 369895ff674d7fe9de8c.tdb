 PAR  L(LIQUID,GESE2,SE;1),,            +7.019*T;,,                   N 22Che !
$
 PAR  G(GESE_B16,GE:SE),,               +2*GGESE+320;,,               N 22Che !
 PAR  G(GESE_B1,GE:SE),,                +GHSERGE+GHSERSE
             -33196+3.096*T+320;,,                                    N 22Che !
 PAR  G(GESE2_ALPHA,GE:SE),,            +GHSERGE+2*GHSERSE
             -69123+16.5*T+700;,,                                     N 22Che !
 PAR  G(GESE2_BETA,GE:SE),,             +GHSERGE+2*GHSERSE
             -68871+15.9*T+700;,,                                     N 22Che !
 PAR  G(GE3SE7,GE:SE),,                 +3*GHSERGE+7*GHSERSE
             -220650+63.63*T+2000;,,                                  N 22Che !
$
 FUNCTION GGESE     298.15  +0.5*GHSERGE+0.5*GHSERSE-23979+9.421*T
