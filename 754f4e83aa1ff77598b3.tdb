 PAR  G(B2_BCC,BE:CU:VA),,              -16529+5.3*T;,,               N 18Liu !
 PAR  G(B2_BCC,CU:BE:VA),,              -16529+5.3*T;,,               N 18Liu !
 PAR  L(B2_BCC,BE,CU:BE:VA;1),,         +3460;,,                      N 18Liu !
 PAR  L(B2_BCC,BE:BE,CU:VA;1),,         +3460;,,                      N 18Liu !
 PAR  L(B2_BCC,BE,CU:CU:VA;1),,         +3460;,,                      N 18Liu !
 PAR  L(B2_BCC,CU:BE,CU:VA;1),,         +3460;,,                      N 18Liu !
$
 PAR  G(C15_LAVES,BE:CU),,              +2*GHSERBE+GHSERCU
             -26650-7.24*T;,,                                         N 18Liu !
 PAR  G(C15_LAVES,CU:BE),,              +2*GHSERCU+GHSERBE
             +11650+7.24*T;,,                                         N 18Liu !
 PAR  L(C15_LAVES,BE,CU:BE;0),,         +ZERO;,,                      N 18Liu !
 PAR  L(C15_LAVES,BE,CU:CU;0),,         +65000-37.2*T;,,              N 18Liu !
 PAR  L(C15_LAVES,BE:BE,CU;0),,         -28800;,,                     N 18Liu !
 PAR  L(C15_LAVES,CU:BE,CU;0),,         +ZERO;,,                      N 18Liu !
$
 PAR  L(HCP_A3,BE,CU:VA;0),,            +30000-16.2*T;,,              N 18Liu !
 PAR  L(HCP_A3,BE,CU:VA;1),,            -41380;,,                     N 18Liu !
