 PAR  L(LIQUID,NI,TH;3),,               +27000;,,                     N 09Wan !
$
 PAR  L(FCC_A1,NI,TH:VA;0),,            +ZERO;,,                      N 09Wan !
 PAR  L(BCC_A2,NI,TH:VA;0),,            +ZERO;,,                      N 09Wan !
$
 PAR  G(NI17TH2,NI:TH),,                +19*GHSERNI+2*GHSERTH
             -558243+79.8*T;,,                                        N 09Wan !
 PAR  G(NI5TH_D2D,NI:TH),,              +5*GHSERNI+GHSERTH
             -241440+24.6*T;,,                                        N 09Wan !
$PAR  G(NI7TH2_ALPHA,NI:TH),,           +7*GHSERNI+2*GHSERTH
$            -382410+28.548*T;,,                                      N 09Wan !
 PAR  G(NI7TH2_ALPHA,NI:TH),,           +7*GHSERNI+2*GHSERTH
             -383886+28.548*T;,,                                      N 09Wan !
$PAR  G(NI7TH2_BETA,NI:TH),,            +7*GHSERNI+2*GHSERTH
$            -369810+19.98*T;,,                                       N 09Wan !
 PAR  G(NI7TH2_BETA,NI:TH),,            +7*GHSERNI+2*GHSERTH
             -371286+19.98*T;,,                                       N 09Wan !
 PAR  G(NI2TH_C32,NI:TH),,              +2*GHSERNI+GHSERTH
             -140250+9*T;,,                                           N 09Wan !
 PAR  G(NI7TH4,NI:TH),,                 +7*GHSERNI+4*GHSERTH
             -518760+29.7*T;,,                                        N 09Wan !
 PAR  G(NITH,NI:TH),,                   +GHSERNI+GHSERTH
