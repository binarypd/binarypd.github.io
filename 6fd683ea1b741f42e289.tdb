 PAR  L(LIQUID,CU3SN,SN;1),,            +2177;,,                      N 24Oik !
$
 PAR  L(FCC_A1,CU,SN:VA;0),,            -9500+0.2*T;,,                N 24Oik !
 PAR  L(FCC_A1,CU,SN:VA;1),,            -18500+6.8*T;,,               N 13Li !
$
 PAR  L(BCC_A2,CU,SN:VA;0),,            +5224;,,                      N 13Li !
 PAR  L(BCC_A2,CU,SN:VA;1),,            -95350+9.28*T;,,              N 13Li !
 PAR  L(BCC_A2,CU,SN:VA;2),,            +29870;,,                     N 13Li !
$
 PAR  L(A2_BCC,CU,SN:VA;0),,            +5224;,,                      N 13Li !
 PAR  L(A2_BCC,CU,SN:VA;1),,            -95350+9.28*T;,,              N 13Li !
 PAR  L(A2_BCC,CU,SN:VA;2),,            +29870;,,                     N 13Li !
$
 PAR  G(BCC_4SL,CU:CU:CU:SN:VA),,       +GBCU3SN;,,                   N 13Li !
 PAR  G(BCC_4SL,CU:CU:SN:CU:VA),,       +GBCU3SN;,,                   N 13Li !
 PAR  G(BCC_4SL,CU:SN:CU:CU:VA),,       +GBCU3SN;,,                   N 13Li !
 PAR  G(BCC_4SL,SN:CU:CU:CU:VA),,       +GBCU3SN;,,                   N 13Li !
