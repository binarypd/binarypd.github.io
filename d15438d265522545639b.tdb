 PAR  G(LIQUID,AU),,                    +GHSERAU
             +12552-9.385866*T;                                  3200 N 91Din !
$
 PAR  G(HCP_IM_A3,AU),,                 +GHCPAU;,,                    N 03Liu !
 PAR  G(AUZN_B2,AU:AU),,                +2*GBCCAU;,,                  N 03Liu !
$
