$
 PAR  L(LIQUID,RU,U;0),,                -173587+10.82*T;,,            N 11Ber !
 PAR  L(LIQUID,RU,U;1),,                -63720;,,                     N 11Ber !
$
 PAR  L(BCC_A2,RU,U:VA;0),,             -164020+43.68*T;,,            N 11Ber !
 PAR  L(BCC_A2,RU,U:VA;1),,             -19280;,,                     N 11Ber !
$
 PAR  L(HCP_A3,RU,U:VA;0),,             +UN_ASS;,,                    N !
 PAR  L(TETRAGONAL_U,RU,U;0),,          -105000;,,                    N 11Ber !
$
 PAR  G(RU3U_L12,RU:U),,                -186802+540.77*T
             -101.224*T*LN(T)-0.0092301*T**2+235907*T**(-1);,,        N 11Ber !
 PAR  G(RU5U3,RU:U),,                   +5*GHSERRU+3*GHSERUU
             -409237+52.69*T;,,                                       N 11Ber !
 PAR  G(RU4U3,RU:U),,                   +4*GHSERRU+3*GHSERUU
             -392680+68.8*T;,,                                        N 11Ber !
 PAR  G(RUU_LT,RU:U),,                  +0.48*GHSERRU+0.52*GHSERUU
             -61570+14.68*T;,,                                        N 11Ber !
 PAR  G(RUU_HT,RU:U),,                  +0.48*GHSERRU+0.52*GHSERUU
             -61569.47+14.6795*T;,,                                   N 11Ber !
 PAR  G(RUU2,RU:U),,                    +GHSERRU+2*GHSERUU
             -127824+25.58*T;,,                                       N 11Ber !
$
$ Metastable
$
 PAR  L(FCC_A1,RU,U:VA;0),,             +UN_ASS;,,                    N !
