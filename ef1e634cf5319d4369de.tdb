             -80143+19*T;,,                                           N 22Zhe !
 PAR  G(C14_LAVES,TI:MN),,              +GHSERMN+2*GHSERTI+131195;,,  N 22Zhe !
 PAR  L(C14_LAVES,MN,TI:MN;0),,         -48000-4.38*T;,,              N 22Zhe !
