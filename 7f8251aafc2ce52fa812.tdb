$
$ Database for Re-Ta from C. Guo et al. 2018.
$
$ C. Guo, T. Wu, C. Li, Z. Du, Calphad, 61, 33-40(2018).
$
$ Dataset created 2019.07.25 by Bengt Hallstedt.
$ 2021.02.05: Modified for use with GES6.
$
$ This dataset uses a 2-sl model for sigma and a (standard) 20:10:24 model
$ for chi.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT RE   HCP_A3                    186.207     5333.      36.4820 ! 
 ELEMENT TA   BCC_A2                    180.9479    5681.872   41.4718 ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : RE TA : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : RE TA : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : RE TA% : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 %A 2 1 0.5 !
 CONST HCP_A3 : RE% TA : VA : !
$
$ Prototype alpha-Mn (cI58, I-43m)
$
 PHASE CHI_A12 % 3 24 10 24 !
 CONST CHI_A12 : RE : RE TA : RE TA : !
$
$ Prototype CrFe (tP30, P4_2/mnm)
$
 PHASE SIGMA_D8B % 2 10 20 !
 CONST SIGMA_D8B : RE% TA : RE TA% : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Re
$
$ HCP_A3 and LIQUID revised in unary 3.0
$ BCC_A2 revised in unary 4.4
$
 PAR  G(HCP_A3,RE:VA),,                 +GHSERRE;,,                   N 96Arb !
 PAR  G(FCC_A1,RE:VA),,                 +GHSERRE+11000-1.5*T;,,       N 91Din !
 PAR  G(BCC_A2,RE:VA),,                 +GHSERRE+17000-3.7*T;,,       N 01SGUN !
$PAR  G(BCC_A2,RE:VA),,                 +GHSERRE+29200-6*T;,,         N 91Din !
 PAR  G(LIQUID,RE),,                    +GLIQRE;,,                    N 96Arb !
$
 PAR  G(CHI_A12,RE:RE:RE),,             +58*GHSERRE
             +256567-25.456*T;,,                                      N 13Liu !
 PAR  G(SIGMA_D8B,RE:RE),,              +30*GHSERRE+150000;,,         N 18Guo !
$
 FUNCTION GHSERRE   298.15  -7695.279+128.421589*T-24.348*T*LN(T)
       -0.00253505*T**2+1.92818E-07*T**3+32915*T**(-1);
      1200.00  Y  -15775.998+194.667426*T-33.586*T*LN(T)
       +0.00224565*T**2-2.81835E-07*T**3+1376270*T**(-1);
      2400.00  Y  -70882.739+462.110749*T-67.956*T*LN(T)
       +0.01184945*T**2-7.88955E-07*T**3+18075200*T**(-1);
      3458.00  Y  +346325.888-1211.37186*T+140.831655*T*LN(T)
       -0.033764567*T**2+1.053726E-06*T**3-1.34548866E+08*T**(-1);
      5000.00  Y  -78564.296+346.997842*T-49.519*T*LN(T);
      6000.00  N !
 FUNCTION GLIQRE    298.15  +23820.883-6.34538*T+GHSERRE;
      2000.00  Y  +568842.665-2527.83846*T+314.178898*T*LN(T)
       -0.08939817*T**2+3.92854E-06*T**3-1.63100987E+08*T**(-1);
      3458.00  Y  -39044.888+335.723691*T-49.519*T*LN(T);
      5000.00  Y  +39519.408-11.274151*T+GHSERRE;
      6000.00  N !
$ ------------------------------------------------------------------------------
$ Ta
$
 PAR  G(BCC_A2,TA:VA),,                 +GHSERTA;,,                   N 91Din !
 PAR  G(FCC_A1,TA:VA),,                 +GHSERTA+16000+1.7*T;,,       N 91Din !
 PAR  G(HCP_A3,TA:VA),,                 +GHSERTA+12000+2.4*T;,,       N 91Din !
 PAR  G(LIQUID,TA),,                    +GLIQTA;,,                    N 91Din !
$
 PAR  G(SIGMA_D8B,TA:TA),,              +30*GHSERTA+73.497*T;,,       N 10Wit !
$
 FUNCTION GHSERTA   298.15  -7285.889+119.139857*T-23.7592624*T*LN(T)
       -0.002623033*T**2+1.70109E-07*T**3-3293*T**(-1);
      1300.00  Y  -22389.955+243.88676*T-41.137088*T*LN(T)
       +0.006167572*T**2-6.55136E-07*T**3+2429586*T**(-1);
      2500.00  Y  +229382.886-722.59722*T+78.5244752*T*LN(T)
       -0.017983376*T**2+1.95033E-07*T**3-93813648*T**(-1);
      3290.00  Y  -1042384.01+2985.49125*T-362.159132*T*LN(T)
       +0.043117795*T**2-1.055148E-06*T**3+5.54714342E+08*T**(-1);
      6000.00  N !
 FUNCTION GLIQTA    298.15  +29160.975-7.578729*T+GHSERTA;
      1000.00  Y  +43884.339-61.981795*T+0.0279523*T*LN(T)
       -0.012330066*T**2+6.14599E-07*T**3-3523338*T**(-1);
      3290.00  Y  -6314.543+258.110873*T-41.84*T*LN(T);
      6000.00  N !
 FUNCTION GFCCTA    298.15  +GHSERTA+16000+1.7*T;                      6000 N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ C. Guo, T. Wu, C. Li, Z. Du, Calphad, 61, 33-40(2018).
$
$ Checked against paper. Checked at 6000 K.
$
$ This dataset uses a 2-sl model for sigma and a (standard) 20:10:24 model
$ for chi.
$
$ Liquid, bcc and hcp interactions are much weaker than in 00Liu.
$
 PAR  L(LIQUID,RE,TA;0),,               -34658;,,                     N 18Guo !
$
 PAR  L(BCC_A2,RE,TA:VA;0),,            -82693.8+18.3935*T;,,         N 18Guo !
 PAR  L(HCP_A3,RE,TA:VA;0),,            -9005.4;,,                    N 18Guo !
$
 PAR  G(CHI_A12,RE:RE:TA),,             +34*GHSERRE+24*GHSERTA
             +202016.3-23.8381*T;,,                                   N 18Guo !
 PAR  G(CHI_A12,RE:TA:RE),,             +48*GHSERRE+10*GHSERTA
             -485803.1-35.1696*T;,,                                   N 18Guo !
 PAR  G(CHI_A12,RE:TA:TA),,             +24*GHSERRE+34*GHSERTA
             -540353.8-33.5817*T;,,                                   N 18Guo !
 PAR  L(CHI_A12,RE:RE,TA:RE;0),,        -76189.2+146.6863*T;,,        N 18Guo !
 PAR  L(CHI_A12,RE:TA:RE,TA;0),,        -730256.7-11.2794*T;,,        N 18Guo !
$
 PAR  G(SIGMA_D8B,RE:TA),,              +10*GHSERRE+20*GHSERTA
             -283089-25.0771*T;,,                                     N 18Guo !
 PAR  G(SIGMA_D8B,TA:RE),,              +10*GHSERTA+20*GHSERRE
             +433089+98.5681*T;,,                                     N 18Guo !
 PAR  L(SIGMA_D8B,RE:RE,TA;0),,         -625206.2+18.1614*T;,,        N 18Guo !
$
$ metastable
$
 PAR  L(FCC_A1,RE,TA:VA;0),,            -9005.4;,,                    N Same !
$ ------------------------------------------------------------------------------
$
 ASSESSED_SYSTEM
  RE-TA(TDB -* +LIQUID +BCC_A2 +HCP_A3 +CHI_A12 +SIGMA_D8B
  ;P3 TMM:1000/4000 *) !
$
$
 LIST-OF-REFERENCE
 NUMBER  SOURCE
  Null   'Unknown source'
  Same   'Same or similar interaction as in the corresponding stable phase'
  REFLAV 'Laves phase convention: G(LAVES,X:X)=+3*GHSERXX+15000'
