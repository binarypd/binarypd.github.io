$PAR  G(HCP_A3,U:VA),,                  +GBCCUU+5000-0.008*T;    3000 N 99SGUN !
$PAR  G(HCP_A3,U:VA),,                  +GHSERUU+50000;          3000 N 04Che3 !
 PAR  G(HCP_A3,U:VA),,                  +GBCCUU+5000;            3000 N 10Kur1 !
 PAR  G(DHCP,U),,                       +GBCCUU+5000;            3000 N 10Kur2 !
$PAR  G(ORTHO_AC,U),,                   +GHSERUU+2814.7;         3000 N 10Kur2 !
$PAR  G(TETRAG_AD,U),,                  +GTETUU+260.4;           3000 N 10Kur2 !
