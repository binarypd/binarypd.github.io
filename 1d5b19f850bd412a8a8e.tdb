$PAR  L(CUB_A13,GE,MN:VA;0),,           -44144+3.35*T;,,              N 14Ber1 !
$PAR  L(CUB_A13,GE,MN:VA;1),,           +104625-100.06*T;,,           N 14Ber1 !
$PAR  L(CUB_A13,GE,MN:VA;2),,           -56031+18.08*T;,,             N 14Ber1 !
$
 PAR  L(CUB_A13,GE,MN:VA;0),,           -40000+3.19*T;,,              N 15Ber1 !
