$
$ Database for Ge-Ru from Z.H. Long et al. 2009.
$
$ Z.H. Long, H.S. Liu, Z.P. Jin, J. Alloys Compd., 479, 262-67(2009).
$
$ Dataset created 2018.01.14 by Bengt Hallstedt.
$ 2021.01.03: Modified for use with GES6.
$
$ They have modelled heat capacities of the compounds, which admittedly look
$ rather strange. The resulting heat capacity curves have very little in
$ common with any kind of "normal" heat capacity curve.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT GE   DIAMOND_A4                 72.59      4636.0     31.09   ! 
 ELEMENT RU   HCP_A3                    101.07      4602.4     28.6144 ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : GE RU : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : GE RU : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : GE RU : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 %A 2 1 0.5 !
 CONST HCP_A3 : GE RU% : VA : !
$
$ Prototype C (cF8, Fd-3m)
$
 PHASE DIAMOND_A4 % 1 1 !
 CONST DIAMOND_A4 : GE : !
$
$ Prototype Ge3Ru2 (oP40, Pbcn)
$
 PHASE GE3RU2_ALPHA % 2 3 2 !
 CONST GE3RU2_ALPHA : GE : RU : !
$
$ Prototype Ru2Sn3 (tP20, P-4c2)
$
 PHASE GE3RU2_BETA % 2 3 2 !
 CONST GE3RU2_BETA : GE : RU : !
$
$ Prototype FeSi (cP8, P2_13), low T
$ Prototypt CsCl (cP2, Pm-3m, B2), high T
$
 PHASE GERU % 2 1 1 !
 CONST GERU : GE : RU : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Ge
$
$ BCT_A5 and RHOMBOHEDRAL_A7 added in unary 4.1
$
 PAR  G(DIAMOND_A4,GE),,                +GHSERGE;                3200 N 91Din !
 PAR  G(FCC_A1,GE:VA),,                 +GHSERGE+36000-22.3*T;   3200 N 91Din !
 PAR  G(BCC_A2,GE:VA),,                 +GHSERGE+34100-23.5*T;   3200 N 91Din !
 PAR  G(HCP_A3,GE:VA),,                 +GHSERGE+35000-21.5*T;   3200 N 91Din !
$PAR  G(BCT_A5,GE),,                    +GHSERGE+28800-16.5*T;   3600 N 99SGUN !
$PAR  G(RHOMBOHEDRAL_A7,GE),,           +GHSERGE+29800-16.5*T;   3600 N 99SGUN !
 PAR  G(LIQUID,GE),,                    +GLIQGE;                 3200 N 91Din !
$
 FUNCTION GHSERGE   298.15  -9486.153+165.635573*T-29.5337682*T*LN(T)
       +0.005568297*T**2-1.513694E-06*T**3+163298*T**(-1);
       900.00  Y  -5689.239+102.86087*T-19.8536239*T*LN(T)-0.003672527*T**2;
      1211.40  Y  -9548.204+156.708024*T-27.6144*T*LN(T)-8.59809E+28*T**(-9);
      3200.00  N !
 FUNCTION GLIQGE    298.15  +37141.489-30.687043*T+GHSERGE+8.56632E-21*T**7;
      1211.40  Y  +27243.473+126.324186*T-27.6144*T*LN(T);
      3200.00  N !
$ ------------------------------------------------------------------------------
$ Ru
$
$ BCC_A2 revised in unary 4.4
$
 PAR  G(HCP_A3,RU:VA),,                 +GHSERRU;                4500 N 91Din !
 PAR  G(FCC_A1,RU:VA),,                 +GHSERRU+12500-2.4*T;    4500 N 91Din !
 PAR  G(BCC_A2,RU:VA),,                 +GHSERRU+21500-5.05*T;   4500 N 01SGUN !
$PAR  G(BCC_A2,RU:VA),,                 +GHSERRU+26500-6.2*T;    4500 N 91Din !
 PAR  G(LIQUID,RU),,                    +GLIQRU;                 4500 N 91Din !
$
 FUNCTION GHSERRU   298.15  -7561.873+127.866233*T-22.9143287*T*LN(T)
       -0.004062566*T**2+1.7641E-07*T**3+56377*T**(-1);
      1500.00  Y  -59448.103+489.516214*T-72.3241219*T*LN(T)
       +0.018726245*T**2-1.952433E-06*T**3+11063885*T**(-1);
      2607.00  Y  -38588773+168610.517*T-21329.705*T*LN(T)
       +5.221639*T**2-2.40245985E-04*T**3+1.30829926E+10*T**(-1);
      2740.00  Y  -55768.304+364.482314*T-51.8816*T*LN(T);
      4500.00  N !
 FUNCTION GLIQRU    298.15  +27480.616-8.398748*T+GHSERRU;
       800.00  Y  +50827.232-179.818561*T+19.539341*T*LN(T)
       -0.026524167*T**2+1.667839E-06*T**3-3861125*T**(-1);
      2607.00  Y  -17161.807+349.673561*T-51.8816*T*LN(T);
      4500.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ Z.H. Long, H.S. Liu, Z.P. Jin, J. Alloys Compd., 479, 262-67(2009).
$
$ Checked against paper. Checked at 6000 K.
$
$ They have modelled heat capacities of the compounds, which admittedly look
$ rather strange. The resulting heat capacity curves have very little in
$ common with any kind of "normal" heat capacity curve.
$
 PAR  L(LIQUID,GE,RU;0),,               -29462.1422-26.6615462*T;,,   N 09Lon !
 PAR  L(LIQUID,GE,RU;1),,               +1427.95887-5.6409538*T;,,    N 09Lon !
 PAR  L(LIQUID,GE,RU;2),,               -7455.00406+0.71486012*T;,,   N 09Lon !
$
 PAR  L(HCP_A3,GE,RU:VA;0),,            +28145.7276;,,                N 09Lon !
$
 PAR  G(GE3RU2_ALPHA,GE:RU),,           +5*GGE3RU2A;,,                N 09Lon !
 PAR  G(GE3RU2_BETA,GE:RU),,            +5*GGE3RU2B;,,                N 09Lon !
 PAR  G(GERU,GE:RU),,                   +2*GGERU;,,                   N 09Lon !
$
$ Metastable
$
 PAR  L(FCC_A1,GE,RU:VA;0),,            +28145.7276;,,                N Same !
 PAR  L(BCC_A2,GE,RU:VA;0),,            +28145.7276;,,                N Same !
$
 FUNCTION GGE3RU2A  298.15  -41000+98.7806066*T-19.1*T*LN(T)
       -0.00372*T**2-100000*T**(-1);
      6000.00  N !
 FUNCTION GGE3RU2B  298.15  -45429.1622+177.65*T-31.21*T*LN(T)
       +0.004985*T**2-9.63E-07*T**3+885940.885*T**(-1);
      6000.00  N !
 FUNCTION GGERU     298.15  -38542.4107+112.07*T-20.8718459*T*LN(T)
       -0.00406055275*T**2-42085.1577*T**(-1);
      6000.00  N !
