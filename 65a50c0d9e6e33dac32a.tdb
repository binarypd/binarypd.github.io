$
 PAR  L(FCC_A1,PD,SC:VA;0),,            -504164+76.1676*T;,,          N 02Du !
$
 PAR  G(PD3SC_L12,PD:SC),,              +3*GHSERPD+GHSERSC
             -328696.1+72.5546*T;,,                                   N 02Du !
 PAR  G(PD2SC,PD:SC),,                  +2*GHSERPD+GHSERSC
             -254365+48.5251*T;,,                                     N 02Du !
$
 PAR  G(PDSC_B2,PD:SC),,                +GHSERPD+GHSERSC
             -178497.5+27.228*T;,,                                    N 02Du !
 PAR  L(PDSC_B2,PD,VA:SC;0),,           +19547.8;,,                   N 02Du !
$
 PAR  G(PDSC2,PD:SC),,                  +GHSERPD+2*GHSERSC
             -237314.2+54.5888*T;,,                                   N 02Du !
 PAR  G(PDSC4,PD:SC),,                  +GHSERPD+4*GHSERSC
             -303852+97.3543*T;,,                                     N 02Du !
