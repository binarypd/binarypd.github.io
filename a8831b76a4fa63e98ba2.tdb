             -260096.1+14.598*T;,,                                    N 04Hua !
 PAR  L(SIGMA_OLD,CO:V:CO,V;0),,        -368781.96+246.3798*T;,,      N 04Hua !
$
 PAR  G(COV3_A15,CO:V),,                +GHSERCO+3*GHSERVV
             -33074.458;,,                                            N 04Hua !
