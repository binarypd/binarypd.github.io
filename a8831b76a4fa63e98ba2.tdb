 PAR  L(FCC_A1,CO,V:VA;0),,             -57151.224+21.699*T;,,        N 04Hua !
 PAR  L(FCC_A1,CO,V:VA;1),,             -4581.3012;,,                 N 04Hua !
$
 PAR  L(BCC_A2,CO,V:VA;0),,             -35366.216+17.435*T;,,        N 04Hua !
