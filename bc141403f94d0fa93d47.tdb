 PAR  L(TETRAGONAL_A6,IN,SN;1),,        -2217;,,                      N 03Moe !
$
 PAR  L(INSN_AF,IN,SN;0),,              -15715.5+19.3402*T;,,         N 96Lee !
 PAR  L(TET_ALPHA1_A6,IN,SN;0),,        +35.7-4.561*T;,,              N 03Moe !
