      1811.00  Y  -1713.815+0.940009*T+GHSERFE+4.9251E+30*T**(-9);
      6000.00  N !
 FUNCTION GHCPFE    298.15  -3705.78+12.591*T-1.15*T*LN(T)
       +6.4E-04*T**2+GHSERFE;
      1811.00  Y  -3957.195+5.249009*T+GHSERFE+4.9251E+30*T**(-9);
      6000.00  N !
 FUNCTION GLIQFE    298.15  +12040.17-6.55843*T+GHSERFE-3.67516E-21*T**7;
      1811.00  Y  -10838.83+291.302*T-46*T*LN(T);
      6000.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ L.M. Pan, unpublished research, 1991.
$
$ Checked at 6000 K.
$
$ The FCC_A1 phase becomes stable again above 5299 K at x(B)=0.46.
$
$ The B solubility in bcc-Fe and fcc-Fe is much higher than the most recent
$ experimental data from T.B. Cameron and J.E. Morrall 1986.
$
 PAR  L(LIQUID,B,FE;0),,                -140000+42.834*T;,,           N 91Pan !
 PAR  L(LIQUID,B,FE;1),,                -1500;,,                      N 91Pan !
 PAR  L(LIQUID,B,FE;2),,                +20000;,,                     N 91Pan !
$
 PAR  L(FCC_A1,B,FE:VA;0),,             +21000-15*T;,,                N 91Pan !
 PAR  L(BCC_A2,B,FE:VA;0),,             -1000-5*T;,,                  N 91Pan !
$
 PAR  G(M2B_C16,FE:B),,                 +0.666667*GHSERFE
             +0.333333*GHSERBB-24175+3.62*T;,,                        N 91Pan !
 PAR  TC(M2B_C16,FE:B),,                +1018;,,                      N 91Pan !
 PAR  BM(M2B_C16,FE:B),,                   +1.91;,,                   N 91Pan !
$
 PAR  G(MB_B27,FE:B),,                  +0.5*GHSERFE+0.5*GHSERBB
             -33000+6.35*T;,,                                         N 91Pan !
 PAR  TC(MB_B27,FE:B),,                  +600;,,                      N 91Pan !
 PAR  BM(MB_B27,FE:B),,                    +1.03;,,                   N 91Pan !
$
$ Metastable
$
 PAR  G(HCP_A3,B,FE:VA;0),,             +UN_ASS;,,                    N !
