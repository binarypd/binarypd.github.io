 PAR  L(LIQUID,GA,PT;1),,               +20000-T;,,                   N 09Wan !
 PAR  L(LIQUID,GA,PT;2),,               +60000-6.5*T;,,               N 09Wan !
 PAR  L(LIQUID,GA,PT;3),,               -30000+20*T;,,                N 09Wan !
$
 PAR  L(FCC_A1,GA,PT:VA;0),,            -189097+8.8*T;,,              N 06Li !
 PAR  L(FCC_A1,GA,PT:VA;1),,            -2000+0.084*T;,,              N 09Wan !
$
 PAR  G(GA6PT,GA:PT),,                  +6*GHSERGA+GHSERPT
             -216856.5+15.022*T;,,                                    N 09Wan !
 PAR  G(GA7PT3_D8F,GA:PT),,             +7*GHSERGA+3*GHSERPT
             -592262+37.32*T;,,                                       N 09Wan !
 PAR  G(GA2PT_C1,GA:PT),,               +2*GHSERGA+GHSERPT
             -181360.8+4.647*T;,,                                     N 09Wan !
 PAR  G(GA3PT2_D513,GA:PT),,            +3*GHSERGA+2*GHSERPT
             -325446+10.84*T;,,                                       N 09Wan !
 PAR  G(GAPT_B20,GA:PT),,               +GHSERGA+GHSERPT
             -136237.8+3.988*T;,,                                     N 09Wan !
 PAR  G(GA3PT5,GA:PT),,                 +3*GHSERGA+5*GHSERPT
             -493497.6+14.304*T;,,                                    N 09Wan !
 PAR  G(GAPT2,GA:PT),,                  +GHSERGA+2*GHSERPT
             -174300.6+5.592*T;,,                                     N 09Wan !
$
 PAR  G(GAPT3_L12,GA:PT),,              +GFCCGA+3*GHSERPT
             -187564.8+7.92*T;,,                                      N 09Wan !
 PAR  G(GAPT3_L12,PT:GA),,              +GHSERPT+3*GFCCGA
