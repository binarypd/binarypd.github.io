 PAR  L(LIQUID,CD,SB;0),,               -7896.21-5.09704*T
             +0.049516*T*LN(T);,,                                     N 93Zab !
 PAR  L(LIQUID,CD,SB;1),,               -2632.97-1.24446*T;,,         N 93Zab !
 PAR  L(LIQUID,CD,SB;2),,               +7826.03-4.87096*T;,,         N 93Zab !
 PAR  L(LIQUID,CD,SB;3),,               +5100.27-3.01441*T;,,         N 93Zab !
$
 PAR  L(HCP_A3,CD,SB:VA;0),,            +UN_ASS;,,                    N !
$
 PAR  G(CDSB_BE,CD:SB),,                -40923.36+366.32876*T
             -66.702912*T*LN(T);,,                                    N 93Zab !
$
$ Metastable
$
 PAR  L(FCC_A1,CD,SB:VA;0),,            +UN_ASS;,,                    N !
 PAR  L(BCC_A2,CD,SB:VA;0),,            +UN_ASS;,,                    N !
