$
$ Database for Fe-Ti from L.F.S. Dumitrescu et al. 1998 (included in TCFE99)
$
$ L.F.S. Dumitrescu, M. Hillert, N. Saunders,
$ J. Phase Equilib., 19, 441-48(1998).
$
$ Dataset created 2010.01.23 by Bengt Hallstedt.
$ 2021.01.02: Modified for use with GES6.
$
$ There are up to 10 K differences for invariant
$ equilibria involving the liquid phase.
$
$ FETI_B2 is modelled as a stoichiometric phase.
$
$ The phase diagram is similar to that of 98Har.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT FE   BCC_A2                     55.847     4489.0     27.2797 ! 
 ELEMENT TI   HCP_A3                     47.88      4824.      30.72   ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : FE TI : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : FE% TI : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : FE% TI% : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 %A 2 1 0.5 !
 CONST HCP_A3 : FE TI% : VA : !
$
$ Prototype CsCl (cP2, Pm-3m)
$
 PHASE FETI_B2 % 2 1 1 !
 CONST FETI_B2 : FE : TI : !
$
$ Prototype MgZn2 (hP12, P6_3/mmc)
$
 PHASE C14_LAVES % 2 2 1 !
 CONST C14_LAVES : FE% TI : FE TI% : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 DEFAULT_COM REJECT_PHASE C14_LAVES !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Fe
$
$ ORTHORHOMBIC_A20 and TETRAGONAL_U added im unary 4.1
$
 PAR  G(BCC_A2,FE:VA),,                 +GHSERFE;,,                   N 91Din !
 PAR  TC(BCC_A2,FE:VA),,                 1043.00;,,                   N 91Din !
 PAR  BM(BCC_A2,FE:VA),,                    2.22;,,                   N 91Din !
 PAR  G(FCC_A1,FE:VA),,                 +GFCCFE;,,                    N 91Din !
 PAR  TC(FCC_A1,FE:VA),,                 -201.00;,,                   N 91Din !
 PAR  BM(FCC_A1,FE:VA),,                   -2.10;,,                   N 91Din !
 PAR  G(HCP_A3,FE:VA),,                 +GHCPFE;,,                    N 91Din !
$PAR  G(CBCC_A12,FE:VA),,               +GHSERFE+4745;,,              N 91Din !
$PAR  G(CUB_A13,FE:VA),,                +GHSERFE+3745;,,              N 91Din !
$PAR  G(ORTHORHOMBIC_A20,FE),,          +GHSERFE+5000;,,              N 99SGUN !
$PAR  G(TETRAGONAL_U,FE),,              +GHSERFE+5000;,,              N 99SGUN !
 PAR  G(LIQUID,FE),,                    +GLIQFE;,,                    N 91Din !
$
 PAR  G(C14_LAVES,FE:FE),,              +3*GHSERFE+15000;,,           N REFLAV !
$
 FUNCTION GHSERFE   298.15  +1225.7+124.134*T-23.5143*T*LN(T)
       -0.00439752*T**2-5.8927E-08*T**3+77359*T**(-1);
      1811.00  Y  -25383.581+299.31255*T-46*T*LN(T)+2.29603E+31*T**(-9);
      6000.00  N !
 FUNCTION GFCCFE    298.15  -1462.4+8.282*T-1.15*T*LN(T)
       +6.4E-04*T**2+GHSERFE;
      1811.00  Y  -1713.815+0.940009*T+GHSERFE+4.9251E+30*T**(-9);
      6000.00  N !
 FUNCTION GHCPFE    298.15  -3705.78+12.591*T-1.15*T*LN(T)
       +6.4E-04*T**2+GHSERFE;
      1811.00  Y  -3957.195+5.249009*T+GHSERFE+4.9251E+30*T**(-9);
      6000.00  N !
 FUNCTION GLIQFE    298.15  +12040.17-6.55843*T+GHSERFE-3.67516E-21*T**7;
      1811.00  Y  -10838.83+291.302*T-46*T*LN(T);
      6000.00  N !
$ ------------------------------------------------------------------------------
$ Ti
$
$ BCT_A5 added in unary 3.0
$
 PAR  G(HCP_A3,TI:VA),,                 +GHSERTI;                4000 N 91Din !
 PAR  G(BCC_A2,TI:VA),,                 +GBCCTI;                 4000 N 91Din !
 PAR  G(FCC_A1,TI:VA),,                 +GHSERTI+6000-0.1*T;     4000 N 91Din !
$PAR  G(DIAMOND_A4,TI),,                +GHSERTI+25000;          4000 N 91Din !
$PAR  G(BCT_A5,TI),,                    +GHSERTI+4602.2;         3000 N SGCOST !
$PAR  G(CBCC_A12,TI:VA),,               +GHSERTI+4602.2;         4000 N 91Din !
$PAR  G(CUB_A13,TI:VA),,                +GHSERTI+7531.2;         4000 N 91Din !
 PAR  G(LIQUID,TI),,                    +GLIQTI;                 4000 N 91Din !
$
 PAR  G(C14_LAVES,TI:TI),,              +3*GHSERTI+15000;,,           N REFLAV !
$
 FUNCTION GHSERTI   298.15  -8059.921+133.615208*T-23.9933*T*LN(T)
       -0.004777975*T**2+1.06716E-07*T**3+72636*T**(-1);
       900.00  Y  -7811.815+132.988068*T-23.9887*T*LN(T)
       -0.0042033*T**2-9.0876E-08*T**3+42680*T**(-1);
      1155.00  Y  +908.837+66.976538*T-14.9466*T*LN(T)
       -0.0081465*T**2+2.02715E-07*T**3-1477660*T**(-1);
      1941.00  Y  -124526.786+638.806871*T-87.2182461*T*LN(T)
       +0.008204849*T**2-3.04747E-07*T**3+36699805*T**(-1);
      4000.00  N !
 FUNCTION GBCCTI    298.15  -1272.064+134.71418*T-25.5768*T*LN(T)
       -6.63845E-04*T**2-2.78803E-07*T**3+7208*T**(-1);
      1155.00  Y  +6667.385+105.366379*T-22.3771*T*LN(T)
       +0.00121707*T**2-8.4534E-07*T**3-2002750*T**(-1);
      1941.00  Y  +26483.26-182.426471*T+19.0900905*T*LN(T)
       -0.02200832*T**2+1.228863E-06*T**3+1400501*T**(-1);
      4000.00  N !
 FUNCTION GLIQTI    298.15  +12194.415-6.980938*T+GHSERTI;
      1300.00  Y  +369519.198-2554.0225*T+342.059267*T*LN(T)
       -0.163409355*T**2+1.2457117E-05*T**3-67034516*T**(-1);
      1941.00  Y  -19887.066+298.7367*T-46.29*T*LN(T);
      4000.00  N !
 FUNCTION GFCCTI    298.15  +GHSERTI+6000-0.1*T;                       4000 N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ From L.F.S. Dumitrescu et al. 1998 (included in TCFE99)
$
$ L.F.S. Dumitrescu, M. Hillert, N. Saunders,
$ J. Phase Equilib., 19, 441-48(1998).
$
$ Checked against paper. Checked at 6000 K.
$
$ There are up to 10 K differences for invariant
$ equilibria involving the liquid phase.
$
$ FETI_B2 is modelled as a stoichiometric phase.
$
$ The phase diagram is similar to that of 98Har.
$
 PAR  L(LIQUID,FE,TI;0),,               -71347+8.25*T;,,              N 98Dum !
 PAR  L(LIQUID,FE,TI;1),,               +7434-4.5*T;,,                N 98Dum !
 PAR  L(LIQUID,FE,TI;2),,               +12155+0.25*T;,,              N 98Dum !
$
 PAR  L(FCC_A1,FE,TI:VA;0),,            -51625+11*T;,,                N 98Dum !
 PAR  L(FCC_A1,FE,TI:VA;1),,            -1950-6*T;,,                  N 98Dum !
 PAR  L(FCC_A1,FE,TI:VA;2),,            +14875;,,                     N 98Dum !
$
 PAR  L(BCC_A2,FE,TI:VA;0),,            -59098+11.5*T;,,              N 98Dum !
 PAR  L(BCC_A2,FE,TI:VA;1),,            -1796+T;,,                    N 98Dum !
 PAR  L(BCC_A2,FE,TI:VA;2),,            +5602+3.5*T;,,                N 98Dum !
$
 PAR  L(HCP_A3,FE,TI:VA;0),,            -28750+11*T;,,                N 98Dum !
 PAR  L(HCP_A3,FE,TI:VA;1),,            -1700-6*T;,,                  N 98Dum !
 PAR  L(HCP_A3,FE,TI:VA;2),,            +15000;,,                     N 98Dum !
$
 PAR  G(FETI_B2,FE:TI),,                -60000+243*T-44.9929*T*LN(T)
             -0.00843*T**2+102000*T**(-1);,,                          N 98Dum !
$
 PAR  G(C14_LAVES,FE:TI),,              -90800+409*T-73.55318*T*LN(T)
             -0.01017*T**2+124200*T**(-1);,,                          N 98Dum !
 PAR  G(C14_LAVES,TI:FE),,              +GHSERFE+2*GHSERTI+15000;,,   N 98Dum !
 PAR  L(C14_LAVES,FE,TI:FE;0),,         -38000;,,                     N 98Dum !
 PAR  L(C14_LAVES,FE,TI:TI;0),,         -38000;,,                     N 98Dum !
 PAR  L(C14_LAVES,FE:FE,TI;0),,         +16000;,,                     N 98Dum !
 PAR  L(C14_LAVES,TI:FE,TI;0),,         +16000;,,                     N 98Dum !
$
$ Metastable
$
$PAR  G(MU_D85,FE:TI:FE),,              +7*GFCCFE+2*GBCCTI
$            +4*GHSERFE;,,                                            N 93Jin !
$PAR  G(MU_D85,FE:TI:TI),,              +7*GFCCFE+6*GBCCTI;,,         N 93Jin !
$
$PAR  G(SIGMA_OLD,FE:TI:FE),,           +8*GFCCFE+4*GBCCTI
$            +18*GHSERFE+UN_ASS;,,                                    N !
$PAR  G(SIGMA_OLD,FE:TI:TI),,           +8*GFCCFE+22*GBCCTI
$            +UN_ASS;,,                                               N !
