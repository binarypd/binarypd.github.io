 PAR  L(BCC_A2,MN,TI:VA;1),,            -1000;,,                     N 98Sau14 !
$
 PAR  L(HCP_A3,MN,TI:VA;0),,            +22100;,,                    N 98Sau14 !
$
 PAR  L(CBCC_A12,MN,TI:VA;0),,          -29500+20*T;,,               N 98Sau14 !
 PAR  L(CBCC_A12,MN,TI:VA;1),,          -3635-5*T;,,                 N 98Sau14 !
$
 PAR  L(CUB_A13,MN,TI:VA;0),,           -34000+20*T;,,               N 98Sau14 !
