 PAR  L(FCC_A1,CE,RH:VA;0),,            +ZERO;,,                      N 24Bou !
 PAR  L(BCC_A2,CE,RH:VA;0),,            +ZERO;,,                      N 24Bou !
$
 PAR  G(CE7RH3_D102,CE:RH),,            +7*GHSERCE+3*GHSERRH
             -501580-64.3*T;,,                                        N 24Bou !
 PAR  G(CE5RH3,CE:RH),,                 +5*GHSERCE+3*GHSERRH
             -479256-49.6*T;,,                                        N 24Bou !
 PAR  G(CE3RH2,CE:RH),,                 +3*GHSERCE+2*GHSERRH
             -315865-29.75*T;,,                                       N 24Bou !
 PAR  G(CE5RH4,CE:RH),,                 +5*GHSERCE+4*GHSERRH
             -622620-38.07*T;,,                                       N 24Bou !
 PAR  G(CERH_B33,CE:RH),,               +GHSERCE+GHSERRH
             -147344-6.44*T;,,                                        N 24Bou !
 PAR  G(CERH2_C15,CE:RH),,              +GHSERCE+2*GHSERRH
             -210111-17.1*T;,,                                        N 24Bou !
 PAR  G(CERH3_L12,CE:RH),,              +GHSERCE+3*GHSERRH
