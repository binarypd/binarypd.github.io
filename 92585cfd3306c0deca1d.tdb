 PAR  L(BCC_A2,FE,MO:VA;1),,            -362-5.724*T;,,               N 88And2 !
 PAR  TC(BCC_A2,FE,MO:VA;0),,            +335;,,                      N 82Fer !
 PAR  TC(BCC_A2,FE,MO:VA;1),,            +526;,,                      N 82Fer !
$
 PAR  G(FE2MO_C14,FE:MO),,              +2*GFCCFE+GHSERMO
             -10798-0.132*T;,,                                        N 88And2 !
