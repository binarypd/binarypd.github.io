 PAR  L(LIQUID,MN,Y;0),,                -26874.14+2.38277*T;,,        N 97Fla !
 PAR  L(LIQUID,MN,Y;1),,                -12579.78;,,                  N 97Fla !
$
 PAR  L(FCC_A1,MN,Y:VA;0),,             +5059.03-0.40472*T;,,         N 97Fla !
