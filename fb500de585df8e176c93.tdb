 PAR  G(NI5PR_D2D,NI:PR),,              +5*GHSERNI+GHSERPR
             -164616+33.342*T;,,                                      N 15Rah !
 PAR  G(NI7PR2,NI:PR),,                 +7*GHSERNI+2*GHSERPR
             -261747+42.561*T;,,                                      N 15Rah !
 PAR  G(NI3PR,NI:PR),,                  +3*GHSERNI+GHSERPR
             -117828+16.848*T;,,                                      N 15Rah !
 PAR  G(NI2PR_C15,NI:PR),,              +2*GHSERNI+GHSERPR
             -90885+8.946*T;,,                                        N 15Rah !
