 PAR  L(LIQUID,NI,PR;0),,               -126268+22.472*T;,,           N 15Rah !
 PAR  L(LIQUID,NI,PR;1),,               -82377+52.61*T;,,             N 15Rah !
$
 PAR  L(FCC_A1,NI,PR:VA;0),,            +100000;,,                    N 15Rah !
 PAR  L(BCC_A2,NI,PR:VA;0),,            +100000;,,                    N 15Rah !
 PAR  L(DHCP,NI,PR;0),,                 +100000;,,                    N 15Rah !
