       +0.008204849*T**2-3.04747E-07*T**3+36699805*T**(-1);
      4000.00  N !
 FUNCTION GBCCTI    298.15  -1272.064+134.71418*T-25.5768*T*LN(T)
       -6.63845E-04*T**2-2.78803E-07*T**3+7208*T**(-1);
      1155.00  Y  +6667.385+105.366379*T-22.3771*T*LN(T)
       +0.00121707*T**2-8.4534E-07*T**3-2002750*T**(-1);
      1941.00  Y  +26483.26-182.426471*T+19.0900905*T*LN(T)
       -0.02200832*T**2+1.228863E-06*T**3+1400501*T**(-1);
      4000.00  N !
 FUNCTION GLIQTI    298.15  +12194.415-6.980938*T+GHSERTI;
      1300.00  Y  +369519.198-2554.0225*T+342.059267*T*LN(T)
       -0.163409355*T**2+1.2457117E-05*T**3-67034516*T**(-1);
      1941.00  Y  -19887.066+298.7367*T-46.29*T*LN(T);
      4000.00  N !
 FUNCTION GFCCTI    298.15  +GHSERTI+6000-0.1*T;                       4000 N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ K. Wang, X. Kong, J. Du, C. Li, Z. Li, Z. Wu, Calphad, 34, 317-23(2010).
$
$ Checked against paper and author's tdb. Checked at 6000 K.
$
$ Non-standard models for fcc and bcc.
$
$ The sign of L(LIQUID,H,TIH2;1) had to be changed in the author's tdb (Pandat).
$
 PAR  G(LIQUID,TIH2),,                  +GHSERTI+GHSERH2
             +73032.4+80.2085*T;,,                                    N 10Wan !
 PAR  L(LIQUID,H,TIH2;0),,              -184222-152.063*T;,,          N 10Wan !
 PAR  L(LIQUID,H,TIH2;1),,              +154263+44.2163*T;,,          N 10Wan !
 PAR  L(LIQUID,TI,TIH2;0),,             -168706-11.8476*T;,,          N 10Wan !
$
 PAR  G(FCC_A1,TI:H),,                  +GFCCTI+GHSERH2
             -141748+138.601*T;,,                                     N 10Wan !
 PAR  L(FCC_A1,TI:H,VA;0),,             +8229.73;,,                   N 10Wan !
 PAR  L(FCC_A1,TI:H,VA;1),,             -17080.7;,,                   N 10Wan !
$
 PAR  G(BCC_A2,TI:H),,                  +GBCCTI+0.75*GHSERH2
             -95951.4+95.036*T;,,                                     N 10Wan !
 PAR  L(BCC_A2,TI:H,VA;0),,             +2970.28-6.5847*T;,,          N 10Wan !
 PAR  L(BCC_A2,TI:H,VA;1),,             -7380.82-0.3547*T;,,          N 10Wan !
$
 PAR  G(HCP_A3,TI:H),,                  +GHSERTI+0.25*GHSERH2
             -25108.3+22.1306*T;,,                                    N 10Wan !
 PAR  L(HCP_A3,TI:H,VA;0),,             +2218.31+0.3981*T;,,          N 10Wan !
