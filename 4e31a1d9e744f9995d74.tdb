 PAR  L(LIQUID,MG,MN;0),,               +25922.4+9.0357*T;,,          N 05Gro !
 PAR  L(LIQUID,MG,MN;1),,               -3470.8;,,                    N 05Gro !
$
 PAR  L(FCC_A1,MG,MN:VA;0),,            +85000;,,                     N 05Gro !
 PAR  L(BCC_A2,MG,MN:VA;0),,            +85000;,,                     N 05Gro !
 PAR  L(HCP_A3,MG,MN:VA;0),,            +37148.1-1.8103*T;,,          N 05Gro !
 PAR  L(CBCC_A12,MG,MN:VA;0),,          +85000;,,                     N 05Gro !
 PAR  L(CUB_A13,MG,MN:VA;0),,           +85000;,,                     N 05Gro !
$ ------------------------------------------------------------------------------
$
 ASSESSED_SYSTEM
  MG-MN(TDB -* +LIQUID +BCC_A2 +FCC_A1 +HCP_A3 +CBCC_A12 +CUB_A13
