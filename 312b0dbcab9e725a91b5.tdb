 PAR  L(LIQUID,MO,U;0),,                -46393.55+26.1448*T;,,        N 10Zha !
 PAR  L(LIQUID,MO,U;1),,                -2606.06+1.7078*T;,,          N 10Zha !
$
 PAR  L(FCC_A1,MO,U:VA;0),,             +UN_ASS;,,                    N !
$
 PAR  L(BCC_A2,MO,U:VA;0),,             +28739.2-217.1736*T
             +27.7696*T*LN(T)+0.0068435*T**2-785719.805*T**(-1);,,    N 10Zha !
 PAR  L(BCC_A2,MO,U:VA;1),,             +27365.2138-5.7719*T;,,       N 10Zha !
 PAR  L(BCC_A2,MO,U:VA;2),,             +20317.698-3.1805*T;,,        N 10Zha !
$
 PAR  G(ORTHORHOMBIC_A20,MO,U;0),,      -33595.3252+28.2996*T;,,      N 10Zha !
 PAR  G(TETRAGONAL_U,MO,U;0),,          -12520.8+3.5016*T;,,          N 10Zha !
$
 PAR  G(MOU2_C11B,MO:U),,                +GHSERMO+2*GHSERUU
             -19054.98+28.1718*T-2.7*T*LN(T);,,                       N 10Zha !
