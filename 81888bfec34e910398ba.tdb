$
$ Database for Hf-Ir from M. Idbenali et al. 2018.
$
$ M. Idbenali, C. Servant, M. Feddaoui, Metall. Res. Technol., 115, 503(2018).
$
$ Dataset created 2022.11.01 by Bengt Hallstedt.
$
$ The paper contains both linear and Kaptay expressions for the interactions.
$ In this dataset the linear version is used.
$
$ The liquid interaction has a very large temperature dependency.
$
$ There is an inverse miscibility gap with a minimum at 4511 K and x(Ir)=0.91.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT HF   HCP_A3                    178.49      5845.0     43.56   ! 
 ELEMENT IR   FCC_A1                    192.22      5267.656   35.5054 ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : HF IR : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : HF IR% : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : HF% IR : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 %A 2 1 0.5 !
 CONST HCP_A3 : HF% IR : VA : !
$
$ Prototype NiTi2 (cF96, Fd-3m)
$
 PHASE HF2IR % 2 2 1 !
 CONST HF2IR : HF : HF IR% : !
$
$ Prototype Ir3Zr5 (hP48, P6_122)
$
 PHASE HF5IR3 % 2 5 3 !
 CONST HF5IR3 : HF : IR : !
$
$ Pearson o**, Pmma
$
 PHASE HFIR_ALPHA % 2 0.52 0.48 !
 CONST HFIR_ALPHA : HF% IR : HF IR% : !
$
$ Prototype AuCu (tP4, P4/mmm)
$
 PHASE HFIR_L10 % 2 1 1 !
 CONST HFIR_L10 : HF% IR : HF IR% : !
$
$ Prototype CsCl (cP2, Pm-3m)
$
 PHASE HFIR_B2 % 2 1 1 !
 CONST HFIR_B2 : HF% IR : HF IR% : !
$
$ Prototype AuCu3 (cP4, Pm-3m)
$
 PHASE HFIR3_L12 % 2 1 3 !
 CONST HFIR3_L12 : HF% IR : HF IR% : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Hf
$
$ HCP_A3 and BCC_A2 modified in unary 5.0
$ (upper T interval added + trailing decimals added)
$
 PAR  G(HCP_A3,HF:VA),,                 +GHSERHF;                3001 N 91Din !
 PAR  G(BCC_A2,HF:VA),,                 +GBCCHF;                 3001 N 91Din !
 PAR  G(FCC_A1,HF:VA),,                 +GHSERHF+10000-2.2*T;    3001 N 91Din !
 PAR  G(LIQUID,HF),,                    +GLIQHF;                 3001 N 91Din !
$
 PAR  G(HF2IR,HF:HF),,                  +3*GHSERHF+15000;,,           N 18Idb !
 PAR  G(HFIR_ALPHA,HF:HF),,             +GHSERHF+5000;,,              N 18Idb !
 PAR  G(HFIR_L10,HF:HF),,               +2*GHSERHF+10000;,,           N 18Idb !
 PAR  G(HFIR_B2,HF:HF),,                +2*GHSERHF+10000;,,           N 18Idb !
 PAR  G(HFIR3_L12,HF:HF),,              +4*GHSERHF+20000;,,           N 18Idb !
$
 FUNCTION GHSERHF   298.15  -6987.297+110.744026*T-22.7075*T*LN(T)
       -0.004146145*T**2-4.77E-10*T**3-22590*T**(-1);
      2506.00  Y  -1446776.329+6193.609991*T-787.5363829*T*LN(T)
       +0.1735215*T**2-7.575759E-06*T**3+5.01742495E+08*T**(-1);
      3000.00  Y  -34274.698+277.882368*T-44*T*LN(T);
      3001.00  N !
 FUNCTION GBCCHF    298.15  +5370.703+103.836026*T-22.8995*T*LN(T)
       -0.004206605*T**2+8.71923E-07*T**3-22590*T**(-1)-1.446E-10*T**4;
      2506.00  Y  +1912456.771-8624.20573*T+1087.6141247*T*LN(T)
       -0.286857065*T**2+1.3427829E-05*T**3-6.10085091E+08*T**(-1);
      3000.00  Y  -32498.178+276.723247*T-44*T*LN(T);
      3001.00  N !
 FUNCTION GLIQHF    298.15  +27402.256-10.953093*T+GHSERHF;
      1000.00  Y  +49731.499-149.91739*T+12.116812*T*LN(T)
       -0.021262021*T**2+1.376466E-06*T**3-4449699*T**(-1);
      2506.00  Y  -4247.217+265.470523*T-44*T*LN(T);
      3001.00  N !
$ ------------------------------------------------------------------------------
$ Ir
$
 PAR  G(FCC_A1,IR:VA),,                 +GHSERIR;                4000 N 91Din !
 PAR  G(BCC_A2,IR:VA),,                 +GHSERIR+32000-6.9*T;    4000 N 91Din !
 PAR  G(HCP_A3,IR:VA),,                 +GHSERIR+4000-0.6*T;     4000 N 91Din !
 PAR  G(LIQUID,IR),,                    +GLIQIR;                 4000 N 91Din !
$
 PAR  G(HFIR_ALPHA,IR:IR),,             +GHSERIR+5000;,,              N 18Idb !
 PAR  G(HFIR_L10,IR:IR),,               +2*GHSERIR+10044;,,           N 18Idb !
 PAR  G(HFIR_B2,IR:IR),,                +2*GHSERIR+10000;,,           N 18Idb !
 PAR  G(HFIR3_L12,IR:IR),,              +4*GHSERIR+20000;,,           N 18Idb !
$
 FUNCTION GHSERIR   298.15  -6936.288+118.780119*T-22.7944*T*LN(T)
       -0.003091976*T**2-20083*T**(-1);
      1215.00  Y  -8123.73+140.066697*T-26.085*T*LN(T)-4.7969E-07*T**3;
      2719.00  Y  +290529.037-1258.35297*T+152.498874*T*LN(T)
       -0.047176402*T**2+1.844977E-06*T**3-92987250*T**(-1);
      4000.00  N !
 FUNCTION GLIQIR    298.15  +23455.244-6.312059*T+GHSERIR;
      1000.00  Y  +102217.789-587.632815*T+73.9517579*T*LN(T)
       -0.04638802*T**2+2.761831E-06*T**3-13382612*T**(-1);
      2719.00  Y  -38347.217+411.234043*T-59.418*T*LN(T);
      4000.00  N !
 FUNCTION GBCCIR    298.15  +GHSERIR+32000-6.9*T;                      4000 N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ M. Idbenali, C. Servant, M. Feddaoui, Metall. Res. Technol., 115, 503(2018).
$
$ Checked against paper. Checked at 6000 K.
$
$ The paper contains both linear and Kaptay expressions for the interactions.
$ In this dataset the linear version is used.
$
$ The liquid interaction has a very large temperature dependency.
$
$ There is an inverse miscibility gap with a minimum at 4511 K and x(Ir)=0.91.
$
 PAR  L(LIQUID,HF,IR;0),,               -373177.5059+45.37248857*T;,, N 18Idb !
 PAR  L(LIQUID,HF,IR;1),,               +61753.25576-20.461066*T;,,   N 18Idb !
 PAR  L(LIQUID,HF,IR;2),,               -22448.09351+28.52128647*T;,, N 18Idb !
$
 PAR  L(FCC_A1,HF,IR:VA;0),,            -225000+4.5*T;,,              N 18Idb !
 PAR  L(BCC_A2,HF,IR:VA;0),,            -210000+1.1*T;,,              N 18Idb !
 PAR  L(HCP_A3,HF,IR:VA;0),,            -166000+8*T;,,                N 18Idb !
$
 PAR  G(HF2IR,HF:IR),,                  +2*GHSERHF+GHSERIR
             -243000+22.95*T;,,                                       N 18Idb !
 PAR  L(HF2IR,HF:HF,IR;0),,             +15000;,,                     N 18Idb !
$
 PAR  G(HF5IR3,HF:IR),,                 +5*GHSERHF+3*GHSERIR
             -728000+74.4*T;,,                                        N 18Idb !
$
 PAR  G(HFIR_ALPHA,HF:IR),,             +0.52*GHSERHF+0.48*GHSERIR
             -118400+22.55*T;,,                                       N 18Idb !
 PAR  G(HFIR_ALPHA,IR:HF),,             +0.52*GHSERIR+0.48*GHSERHF
             +128400-22.55*T;,,                                       N 18Idb !
$
 PAR  G(HFIR_L10,HF:IR),,               +GHSERHF+GHSERIR
             -206700+17.382*T;,,                                      N 18Idb !
 PAR  G(HFIR_L10,IR:HF),,               +GHSERIR+GHSERHF
             +226700-17.382*T;,,                                      N 18Idb !
$
 PAR  G(HFIR_B2,HF:IR),,                +GHSERHF+GHSERIR
             -206600+17.3*T;,,                                        N 18Idb !
 PAR  G(HFIR_B2,IR:HF),,                +GHSERIR+GHSERHF
             +226600-17.3*T;,,                                        N 18Idb !
$
 PAR  G(HFIR3_L12,HF:IR),,              +GHSERHF+3*GHSERIR
             -280000+16.8*T;,,                                        N 18Idb !
 PAR  G(HFIR3_L12,IR:HF),,              +GHSERIR+3*GHSERHF
             +320000-16.8*T;,,                                        N 18Idb !
