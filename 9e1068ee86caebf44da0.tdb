 PAR  L(BCC_A2,NI,TI:VA;1),,            -32315.3;,,                   N 96Bel !
$
 PAR  L(A2_BCC,NI,TI:VA;0),,            -97427.4+12.112*T;,,          N 96Bel !
 PAR  L(A2_BCC,NI,TI:VA;1),,            -32315.3;,,                   N 96Bel !
$
 PAR  G(B2_BCC,NI:TI:VA;0),,            -33193.7+10.284*T;,,          N 96Bel !
