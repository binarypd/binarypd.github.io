 PAR  L(A2_BCC,MO,TA:VA;1),,            +LDB1MOTA;,,                  N 05Tur !
$
 PAR  G(B2_BCC,MO:TA:VA),,              +GB2MOTA;,,                   N 05Tur !
 PAR  G(B2_BCC,TA:MO:VA),,              +GB2MOTA;,,                   N 05Tur !
 PAR  L(B2_BCC,MO,TA:MO:VA;0),,         +L0BMOTA;,,                   N 05Tur !
