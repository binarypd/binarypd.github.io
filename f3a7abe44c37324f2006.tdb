 PAR  L(LIQUID,NI,SC;0),,               -162595.5+43.5436*T;,,        N 13Kar !
 PAR  L(LIQUID,NI,SC;1),,               -1330.2931-10.0219*T;,,       N 13Kar !
$
 PAR  L(FCC_A1,NI,SC:VA;0),,            -82714.549+26.933163*T;,,     N 13Kar !
