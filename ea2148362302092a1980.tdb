 PAR  G(AUPR_B27,AU:PR),,               +GHSERAU+GHSERPR
             -149632+26.3862*T;,,                                     N 04Du2 !
 PAR  G(AUPR_B33,AU:PR),,               +GHSERAU+GHSERPR
             -149275.6+25.8865*T;,,                                   N 04Du2 !
 PAR  G(AUPR_B2,AU:PR),,                +GHSERAU+GHSERPR
             -147744.3+24.9252*T;,,                                   N 04Du2 !
 PAR  G(AUPR2_C37,AU:PR),,              +GHSERAU+2*GHSERPR
