      1200.00  Y  +27786.061-156.162846*T+8.28608*T*LN(T)
       -0.01082725*T**2-1.112352E-06*T**3-6183850*T**(-1);
      1703.00  Y  -825364.662+4248.379056*T-558.9506818*T*LN(T)
       +0.139111904*T**2-6.824652E-06*T**3+2.19952973E+08*T**(-1);
      3000.00  Y  -17149.229+235.898104*T-43.932*T*LN(T);
      3001.00  N !
 FUNCTION GBCCHO    298.15  +3731.229-2.060317*T+GHSERHO;  
      1000.00  Y  +185512.056-1635.740669*T+218.9372486*T*LN(T)
       -0.13516576*T**2+1.2168911E-05*T**3-26729747*T**(-1);
      1703.00  Y  -28867.901+272.946988*T-48.116*T*LN(T);
      1745.00  Y  -152754.148+939.764197*T-134.7930638*T*LN(T)
       +0.025544089*T**2-1.287517E-06*T**3+32050889*T**(-1);
      3001.00  N !
 FUNCTION GLIQHO    298.15  +17262.172-9.992194*T+GHSERHO;  
      1000.00  Y  +124706.283-994.683024*T+127.9577778*T*LN(T)
       -0.088196514*T**2+8.008222E-06*T**3-15727191*T**(-1);
      1703.00  Y  -9809.781+230.793918*T-43.932*T*LN(T);
      3001.00  N !
$ ------------------------------------------------------------------------------
$ Pd
$
$ RHOMBOHEDRAL_A7 added in unary 5.0
$
 PAR  G(FCC_A1,PD:VA),,                 +GHSERPD;                4000 N 91Din !
 PAR  G(BCC_A2,PD:VA),,                 +GHSERPD+10500-1.8*T;    4000 N 91Din !
 PAR  G(HCP_A3,PD:VA),,                 +GHSERPD+2000+0.1*T;     4000 N 91Din !
$PAR  G(RHOMBOHEDRAL_A7,PD),,           +GHSERPD+4000;           4000 N 06Vre !
 PAR  G(LIQUID,PD),,                    +GLIQPD;                 4000 N 91Din !
$
 PAR  G(HOPD3_L12,PD:PD),,              +4*GHSERPD+10492;,,           N 00Du !
$
 FUNCTION GHSERPD   298.15  -10204.027+176.076315*T-32.211*T*LN(T)
       +0.007120975*T**2-1.919875E-06*T**3+168687*T**(-1);
       900.00  Y  +917.062+49.659892*T-13.5708*T*LN(T)
