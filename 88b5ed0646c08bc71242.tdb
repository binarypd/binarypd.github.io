 PAR  L(LIQUID,AL,BA;0),,               -49725-8.505*T;,,             N 19Ben !
 PAR  L(LIQUID,AL,BA;1),,               -43500+18.286*T;,,            N 19Ben !
 PAR  L(LIQUID,AL,BA;2),,               -14330+28.461*T;,,            N 19Ben !
$
 PAR  L(FCC_A1,AL,BA:VA;0),,            +UN_ASS;,,                    N !
 PAR  L(BCC_A2,AL,BA:VA;0),,            +UN_ASS;,,                    N !
$
 PAR  G(AL4BA_D13,AL:BA),,              +4*GHSERAL+GHSERBA
             -117370+11.255*T;,,                                      N 19Ben !
 PAR  G(AL13BA7,AL:BA),,                +13*GHSERAL+7*GHSERBA
             -477920+5.82*T;,,                                        N 19Ben !
 PAR  G(AL5BA3,AL:BA),,                 +5*GHSERAL+3*GHSERBA
