$
$ Database for Ag-Nd from S.L. Wang et al. 2009
$
$ S.L. Wang, C.P. Wang, X.J. Liu, K. Ishida,
$ J. Alloys Compd., 476, 245-52(2009).
$
$ Dataset created 2016.06.06 by Bengt Hallstedt.
$ 2020.12.20: Modified for use with GES6.
$
$ Invariant equilibria differ by up to 1.2 K, probably from changing from
$ fractional to integar compositions.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT AG   FCC_A1                    107.8682    5745.      42.55   ! 
 ELEMENT ND   DHCP                      144.24      7133.72    71.0862 ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : AG ND : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : AG% ND : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : AG ND% : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 %A 2 1 0.5 !
 CONST HCP_A3 : AG ND : VA : !
$
$ Prototype alpha-La (hP4, P6_3/mmc)
$
 PHASE DHCP % 1 1 !
 CONST DHCP : AG ND% : !
$
$ Prototype Ag51Gd14 (hP68, P6/m)
$
 PHASE AG51ND14 % 2 51 14 !
 CONST AG51ND14 : AG : ND : !
$
$ Prototype Hg2K (oI12, Imma)
$
 PHASE AG2ND_ALPHA % 2 2 1 !
 CONST AG2ND_ALPHA : AG : ND : !
$
$ unknown structure
$
 PHASE AG2ND_BETA % 2 2 1 !
 CONST AG2ND_BETA : AG : ND : !
$
$ Prototype CsCl (cP2, Pm-3m)
$
 PHASE AGND_B2 % 2 1 1 !
 CONST AGND_B2 : AG : ND : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Ag
$
$ CUB_A13 added in unary 3.0
$ CUB_A13 revised in unary 4.1
$ BCT_A5 added in unary 4.1
$
$ DHCP from 09Wan not included in unary
$
 PAR  G(FCC_A1,AG:VA),,                 +GHSERAG;                3000 N 91Din !
 PAR  G(HCP_A3,AG:VA),,                 +GHSERAG+300+0.3*T;      3000 N 91Din !
 PAR  G(BCC_A2,AG:VA),,                 +GHSERAG+3400-1.05*T;    3000 N 91Din !
$PAR  G(CUB_A13,AG:VA),,                +GHSERAG+3400-1.05*T;    3000 N 95Lim !
$PAR  G(CUB_A13,AG:VA),,                +GHSERAG
$            +3765.6-1.8826*T;                                   3000 N 96SGUN !
$PAR  G(BCT_A5,AG),,                    +GHSERAG+4184.1;         3000 N 96Oh !
 PAR  G(DHCP,AG),,                      +GHSERAG+1000;           3000 N 09Wan !
 PAR  G(LIQUID,AG),,                    +GLIQAG;                 3000 N 91Din !
$
 FUNCTION GHSERAG   298.15  -7209.512+118.202013*T-23.8463314*T*LN(T)
       -0.001790585*T**2-3.98587E-07*T**3-12011*T**(-1);
      1234.93  Y  -15095.252+190.266404*T-33.472*T*LN(T)+1.411773E+29*T**(-9);
      3000.00  N !
 FUNCTION GLIQAG    298.15  +11025.076-8.89102*T+GHSERAG-1.033905E-20*T**7;
      1234.93  Y  -3587.111+180.964656*T-33.472*T*LN(T);
      3000.00  N !
 FUNCTION GBCCAG    298.15  +GHSERAG+3400-1.05*T;                      6000 N !
$ ------------------------------------------------------------------------------
$ Nd
$
$ DHCP and LIQUID modified in unary 3.0 (upper/lower T interval added)
$ FCC_A1 added in unary 3.0
$ HCP_A3 added in unary 4.4
$
 PAR  G(DHCP,ND),,                      +GHSERND;                1800 N 91Din !
 PAR  G(BCC_A2,ND:VA),,                 +GBCCND;                 1800 N 91Din !
 PAR  G(FCC_A1,ND:VA),,                 +GHSERND+500;            1800 N SGCOST !
 PAR  G(HCP_A3,ND:VA),,                 +GHSERND
             +1500-0.415725*T;                                   1800 N 00Nor2 !
 PAR  G(LIQUID,ND),,                    +GLIQND;                 1800 N 91Din !
$
 FUNCTION GHSERND   298.15  -8402.93+111.10239*T-27.0858*T*LN(T)
       +5.56125E-04*T**2-2.6923E-06*T**3+34887*T**(-1);
       900.00  Y  -6984.083+83.662617*T-22.7536*T*LN(T)
       -0.00420402*T**2-1.802E-06*T**3;
      1128.00  Y  -225610.846+1673.04075*T-238.182873*T*LN(T)
       +0.078615997*T**2-6.048207E-06*T**3+38810350*T**(-1);
      1799.00  Y  -25742.331+276.257088*T-48.7854*T*LN(T);
      1800.00  N !
 FUNCTION GBCCND    298.15  +1437.295-0.546281*T+GHSERND;
       400.00  Y  +7312.2-153.033976*T+14.9956777*T*LN(T)
       -0.050479*T**2+7.287217E-06*T**3-831810*T**(-1);
      1128.00  Y  -18030.266+239.677322*T-44.5596*T*LN(T);
      1289.00  Y  +334513.017-2363.9199*T+311.409193*T*LN(T)
       -0.156030778*T**2+1.2408421E-05*T**3-64319604*T**(-1);
      1800.00  N !
 FUNCTION GLIQND    298.15  +5051.743-1.585076*T+GHSERND;
       300.00  Y  +5350.01-86.593963*T+5.357301*T*LN(T)
       -0.046955463*T**2+6.860782E-06*T**3-374380*T**(-1);
      1128.00  Y  -16335.232+268.625903*T-48.7854*T*LN(T);
      1799.00  Y  +9407.099-7.631185*T+GHSERND;
      1800.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ S.L. Wang, C.P. Wang, X.J. Liu, K. Ishida,
$ J. Alloys Compd., 476, 245-52(2009).
$
$ Checked against paper. Checked at 6000 K.
$
$ Invariant equilibria differ by up to 1.2 K, probably from changing from
$ fractional to integar compositions.
$
 PAR  L(LIQUID,AG,ND;0),,               -77546+57.877*T-5*T*LN(T);,,  N 09Wan !
 PAR  L(LIQUID,AG,ND;1),,               -23158-9*T;,,                 N 09Wan !
 PAR  L(LIQUID,AG,ND;2),,               -2.407*T;,,                   N 09Wan !
 PAR  L(LIQUID,AG,ND;3),,               +22754-10*T;,,                N 09Wan !
$
 PAR  L(FCC_A1,AG,ND:VA;0),,            +9784;,,                      N 09Wan !
 PAR  L(FCC_A1,AG,ND:VA;1),,            -41542;,,                     N 09Wan !
$
 PAR  L(BCC_A2,AG,ND:VA;0),,            -24587+4.736*T;,,             N 09Wan !
 PAR  L(DHCP,AG,ND;0),,                 -9772+6.093*T;,,              N 09Wan !
$
 PAR  G(AG51ND14,AG:ND),,               +51*GHSERAG+14*GHSERND
             -1157000+7.67*T;,,                                       N 09Wan !
 PAR  G(AG2ND_ALPHA,AG:ND),,            +2*GHSERAG+GHSERND
             -63366+6.597*T;,,                                        N 09Wan !
 PAR  G(AG2ND_BETA,AG:ND),,             +2*GHSERAG+GHSERND
             -63000+6.195*T;,,                                        N 09Wan !
 PAR  G(AGND_B2,AG:ND),,                +GHSERAG+GHSERND-39000;,,     N 09Wan !
$
$ Metastable
$
 PAR  L(HCP_A3,AG,ND:VA;0),,            +9784;,,                      N Same !
 PAR  L(HCP_A3,AG,ND:VA;1),,            -41542;,,                     N Same !
