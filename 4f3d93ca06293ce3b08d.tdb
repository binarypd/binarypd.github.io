 PAR  L(LIQUID,SI,Y;2),,                +59562.0693-5.84447*T;,,      N 19Xu !
$
 PAR  L(BCC_A2,SI,Y:VA;0),,             +80*T;,,                      N 19Xu !
 PAR  L(HCP_A3,SI,Y:VA;0),,             +63.11849*T;,,                N 19Xu !
$
 PAR  G(SI2Y_C32,SI:Y),,                +2*GHSERSI+GHSERYY
             -164000-13.040641*T;,,                                   N 19Xu !
 PAR  G(SI2Y_CC,SI:Y),,                 +2*GHSERSI+GHSERYY
             -163200-13.5644936*T;,,                                  N 19Xu !
 PAR  G(SI5Y3_RT,SI:Y),,                +5*GHSERSI+3*GHSERYY
             -495000-20.1777402*T;,,                                  N 19Xu !
 PAR  G(SI5Y3_C32,SI:Y),,               +5*GHSERSI+3*GHSERYY
             -490000-27.0911365*T;,,                                  N 19Xu !
 PAR  G(SIY_B33,SI:Y),,                 +GHSERSI+GHSERYY
             -163783.41+6.00267*T;,,                                  N 19Xu !
 PAR  G(SI4Y5,SI:Y),,                   +4*GHSERSI+5*GHSERYY
             -690000+13.7*T;,,                                        N 19Xu !
 PAR  G(SI3Y5_D88,SI:Y),,               +3*GHSERSI+5*GHSERYY
             -563000+9.2*T;,,                                         N 19Xu !
$
$ Metastable
$
 PAR  L(FCC_A1,SI,Y:VA;0),,             +63.11849*T;,,                N Same !
