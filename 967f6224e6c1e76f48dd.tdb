 PAR  G(LIQUID,MG),,                    +GLIQMG;                 3000 N 91Din !
$
 PAR  G(MGY_B2,MG:MG),,                 +2*GHSERMG+9891.48;,,         N 98Luk !
 PAR  G(MG24Y5_A12,MG:MG),,             +29*GHSERMG+44506.01;,,       N 98Luk !
$
