$
$ Database for Pt-Sb from C. Guo et al. 2014
$
$ C. Guo, C. Li, S. Shang, Z. Du, Int. J. Mater. Res, 105, 525-36(2014).
$
$ Dataset created 2014.01.03 by Bengt Hallstedt.
$ 2021.02.05: Modified for use with GES6.
$
$ There is an inverse miscibility gap with minimum at 5600 K and x(Sb)=0.42.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT PT   FCC_A1                    195.08      5723.712   41.6308 !
 ELEMENT SB   RHOMBOHEDRAL_A7           121.75      5870.152   45.5219 ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : PT SB : !
$
$ Fcc (cF4) and MeX (cF8)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : PT% SB : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : PT SB : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 %A 2 1 0.5 !
 CONST HCP_A3 : PT SB : VA : !
$
$ Prototype alpha-As (hR2, R-3m)
$
 PHASE RHOMBOHEDRAL_A7 % 1 1 !
 CONST RHOMBOHEDRAL_A7 : PT SB% : !
$
$ Prototype CuPt7 (cF32, Fm-3m)
$
 PHASE PT7SB % 2 7 1 !
 CONST PT7SB : PT : SB : !
$
$ Prototype AuCu3 (cP4, Pm-3m)
$
 PHASE PT5SB_L12 % 2 5 1 !
 CONST PT5SB_L12 : PT% SB : PT SB% : !
$
$ Prototype Al3Zr (tI16, I4/mmm)
$
 PHASE PT3SB_D023 % 2 3 1 !
 CONST PT3SB_D023 : PT : SB : !
$
$ Prototype Pt3Sb2 (oI20, Ibam)
$
 PHASE PT3SB2 % 2 3 2 !
 CONST PT3SB2 : PT : SB : !
$
$ Prototype NiAs (hP4, P6_3/mmc)
$
 PHASE PTSB_B81 % 2 1 1 !
 CONST PTSB_B81 : PT : SB : !
$
$ Prototype FeS2 (cP12, Pa-3)
$
 PHASE PTSB2_C2 % 2 1 2 !
 CONST PTSB2_C2 : PT : SB : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Pt
$
$ TETRAGONAL_A6 from 09Lia not included in unary
$ RHOMBOHEDRAL_A7 from 14Guo not included in unary
$
 PAR  G(FCC_A1,PT:VA),,                 +GHSERPT;                4000 N 91Din !
 PAR  G(BCC_A2,PT:VA),,                 +GHSERPT+15000-2.4*T;    4000 N 91Din !
 PAR  G(HCP_A3,PT:VA),,                 +GHSERPT+2500+0.1*T;     4000 N 91Din !
$PAR  G(TETRAGONAL_A6,PT),,             +GHSERPT+3000;           4000 N 09Lia !
 PAR  G(RHOMBOHEDRAL_A7,PT),,           +GHSERPT+5000;           4000 N 14Guo !
 PAR  G(LIQUID,PT),,                    +GLIQPT;                 4000 N 91Din !
$
 PAR  G(PT5SB_L12,PT:PT),,              +6*GHSERPT+30000;        4000 N 14Guo !
$
 FUNCTION GHSERPT   298.15  -7595.631+124.388275*T-24.5526*T*LN(T)
       -0.00248297*T**2-2.0138E-08*T**3+7974*T**(-1);
      1300.00  Y  -9253.174+161.529615*T-30.2527*T*LN(T)
       +0.002321665*T**2-6.56946E-07*T**3-272106*T**(-1);
      2041.50  Y  -222048.216+1019.35892*T-136.192996*T*LN(T)
       +0.020454938*T**2-7.59259E-07*T**3+71539020*T**(-1);
      4000.00  N !
 FUNCTION GLIQPT    298.15  +20114.016-9.275183*T+GHSERPT;
       600.00  Y  +19023.491+32.94182*T-12.3403769*T*LN(T)
       -0.011551507*T**2+9.31516E-07*T**3-601426*T**(-1);
      2041.50  Y  +1404.468+205.858962*T-36.5*T*LN(T);
      4000.00  N !
 FUNCTION GBCCPT    298.15  +GHSERPT+15000-2.4*T;                      6000 N !
$ ------------------------------------------------------------------------------
$ Sb
$
$ BCT_A5 revised in unary 3.0
$ TETRAGONAL_A6 from 03 Liu not included in unary
$
$ BCT_A5 the value from 91Din is used in LBall-v7+fun.tdb.
$ BCT_A5 the value from 96SGUN is used in SGSOL 5.0.
$ It is only needed in Sb-Sn.
$
$ DIAMOND_A4 from 89Che2 not included in unary
$
 PAR  G(RHOMBOHEDRAL_A7,SB),,           +GHSERSB;                2000 N 91Din !
 PAR  G(FCC_A1,SB:VA),,                 +GHSERSB+19874-13.7*T;   2000 N 91Din !
 PAR  G(BCC_A2,SB:VA),,                 +GHSERSB+19874-15.1*T;   2000 N 91Din !
 PAR  G(HCP_A3,SB:VA),,                 +GHSERSB+19874-13*T;     2000 N 91Din !
$PAR  G(BCT_A5,SB),,                    +GHSERSB+13000-8*T;      2000 N 96SGUN !
$PAR  G(BCT_A5,SB),,                    +GHSERSB+1000;           2000 N 91Din !
$PAR  G(DIAMOND_A4,SB),,                +GHSERSB+4184;           3200 N 89Che2 !
$PAR  G(TETRAGONAL_A6,SB),,             +GHSERSB+5575.4;         2000 N 03Liu1 !
 PAR  G(LIQUID,SB),,                    +GLIQSB;                 2000 N 91Din !
$
 PAR  G(PT5SB_L12,SB:SB),,              +6*GHSERSB+30000;        2000 N 14Guo !
$
 FUNCTION GHSERSB   298.15  -9242.858+156.154689*T-30.5130752*T*LN(T)
       +0.007748768*T**2-3.003415E-06*T**3+100625*T**(-1);
       903.78  Y  -11738.83+169.485872*T-31.38*T*LN(T)+1.616849E+27*T**(-9);
      2000.00  N !
 FUNCTION GLIQSB    298.15  +19822.328-21.923164*T+GHSERSB-1.74847E-20*T**7;
       903.78  Y  +8175.359+147.455986*T-31.38*T*LN(T);
      2000.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ C. Guo, C. Li, S. Shang, Z. Du, Int. J. Mater. Res, 105, 525-36(2014).
$
$ Checked against paper. Checked at 6000 K.
$
$ There is an inverse miscibility gap with minimum at 5600 K and x(Sb)=0.42.
$
 PAR  L(LIQUID,PT,SB;0),,               -181384.7+48.5239*T;,,        N 14Guo !
 PAR  L(LIQUID,PT,SB;1),,               -35595+8.2969*T;,,            N 14Guo !
$
 PAR  L(FCC_A1,PT,SB:VA;0),,            -13004.2+60.7336*T;,,         N 14Guo !
 PAR  L(FCC_A1,PT,SB:VA;1),,            -222530.2;,,                  N 14Guo !
$
 PAR  G(PT7SB,PT:SB),,                  +7*GHSERPT+GHSERSB
             -151994.9+28.4508*T;,,                                   N 14Guo !
$
 PAR  G(PT5SB_L12,PT:SB),,              +5*GHSERPT+GHSERSB
             -128869.2+27.1388*T;,,                                   N 14Guo !
 PAR  G(PT5SB_L12,SB:PT),,              +5*GHSERSB+GHSERPT
             +188869.2-27.1388*T;,,                                   N 14Guo !
 PAR  L(PT5SB_L12,PT,SB:PT;0),,         -573983.2;,,                  N 14Guo !
 PAR  L(PT5SB_L12,PT,SB:SB;0),,         -573983.2;,,                  N 14Guo !
 PAR  L(PT5SB_L12,PT,SB:PT;1),,         +199906.4;,,                  N 14Guo !
 PAR  L(PT5SB_L12,PT,SB:SB;1),,         +199906.4;,,                  N 14Guo !
 PAR  L(PT5SB_L12,PT:PT,SB;0),,         -99036.9;,,                   N 14Guo !
 PAR  L(PT5SB_L12,SB:PT,SB;0),,         -99036.9;,,                   N 14Guo !
$
 PAR  G(PT3SB_D023,PT:SB),,             +3*GHSERPT+GHSERSB
             -105717.8+7.9864*T;,,                                    N 14Guo !
 PAR  G(PT3SB2,PT:SB),,                 +3*GHSERPT+2*GHSERSB
             -174648.5+6.3313*T;,,                                    N 14Guo !
 PAR  G(PTSB_B81,PT:SB),,               +GHSERPT+GHSERSB
             -81200.9+4.391*T;,,                                      N 14Guo !
 PAR  G(PTSB2_C2,PT:SB),,               +GHSERPT+2*GHSERSB
             -149619.8-26.2545*T+7.078251*T*LN(T);,,                  N 14Guo !
$
$ Metastable
$
 PAR  L(BCC_A2,PT,SB:VA;0),,            -13004.2+60.7336*T;,,         N Same !
 PAR  L(BCC_A2,PT,SB:VA;1),,            -222530.2;,,                  N Same !
$
 PAR  L(HCP_A3,PT,SB:VA;0),,            -13004.2+60.7336*T;,,         N Same !
 PAR  L(HCP_A3,PT,SB:VA;1),,            -222530.2;,,                  N Same !
$
$PAR  G(L12_FCC,PT:SB:VA),,             +5000;,,                      N 14Guo !
