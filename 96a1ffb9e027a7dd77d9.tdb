 PAR  G(SI2W_C11B,SI:W),,               +2*GSICOMP+GHSERWW
             -80100-15.51*T+1.5*T*LN(T)+7.5E-08*T**3-30000*T**(-1);,, N 13Li !
$
 PAR  G(SI3W5_D8M,W:SI:SI),,            +4*GSICOMP+4*GHSERWW
             -136000-19.2*T;,,                                        N 13Li !
 PAR  G(SI3W5_D8M,W:SI:W),,             +GSICOMP+7*GHSERWW+240000;,,  N 13Li !
 PAR  G(SI3W5_D8M,W:W:SI),,             +3*GSICOMP+5*GHSERWW
             -136000-20.4*T-2.4E-07*T**3;,,                           N 13Li !
 PAR  L(SI3W5_D8M,W:SI:SI,W;0),,        -80000;,,                     N 13Li !
 PAR  L(SI3W5_D8M,W:W:SI,W;0),,         -40000;,,                     N 13Li !
$
$ Metastable
