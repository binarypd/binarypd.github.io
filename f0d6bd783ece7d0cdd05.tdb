$
 PAR  L(LIQUID,CO,GA;0),,               -61807+7.985*T;,,             N 10Cha !
 PAR  L(LIQUID,CO,GA;1),,               +12605.5;,,                   N 10Cha !
$
 PAR  L(FCC_A1,CO,GA:VA;0),,            -125202+54.131*T;,,           N 10Cha !
 PAR  L(FCC_A1,CO,GA:VA;1),,            +30657-25.625*T;,,            N 10Cha !
$
 PAR  L(HCP_A3,CO,GA:VA;0),,            -87051+22.438*T;,,            N 10Cha !
$
 PAR  G(COGA_B2,GA:CO),,                +0.5*GHSERGA+0.5*GHSERCO
             -42125+9.519*T;,,                                        N 10Cha !
 PAR  L(COGA_B2,CO,GA:CO;0),,           -11752+3.505*T;,,             N 10Cha !
 PAR  L(COGA_B2,CO,GA:VA;0),,           -7557-0.3907*T;,,             N 10Cha !
 PAR  L(COGA_B2,GA:CO,VA;0),,           -24462+9.677*T;,,             N 10Cha !
$
 PAR  G(COGA3,CO:GA),,                  +GHSERCO+3*GHSERGA
             -123080+12.172*T;,,                                      N 10Cha !
$
$ Metastable
$
 PAR  L(BCC_A2,CO,GA:VA;0),,            -125202+54.131*T;,,           N Same !
 PAR  L(BCC_A2,CO,GA:VA;1),,            +30657-25.625*T;,,            N Same !
