 PAR  L(LIQUID,BI,SI;0),,               +46370+2.26*T;,,              N 85Ole !
$
 PAR  L(DIAMOND_A4,BI,SI;0),,           +95000;,,                     N 04Sun !
$
$ Metastable
$
 PAR  L(FCC_A1,BI,SI:VA;0),,            +50000;,,                     N Null !
 PAR  L(BCC_A2,BI,SI:VA;0),,            +50000;,,                     N Null !
 PAR  L(HCP_A3,BI,SI:VA;0),,            +50000;,,                     N Null !
