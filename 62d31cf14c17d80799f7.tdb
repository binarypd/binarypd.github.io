 PAR  L(PD5SB3,PD,SB:SB;0),,            -344444.1+145.001*T;,,        N 10Han !
 PAR  L(PD5SB3,PD:PD,SB;0),,            -397728+130.183*T;,,          N 10Han !
 PAR  L(PD5SB3,SB:PD,SB;0),,            -397728+130.183*T;,,          N 10Han !
$
 PAR  G(PDSB_B81,PD:SB),,               +GHSERPD+GHSERSB
             -95313.7+13.768*T;,,                                     N 10Han !
 PAR  G(PDSB2_C2,PD:SB),,               +GHSERPD+2*GHSERSB
