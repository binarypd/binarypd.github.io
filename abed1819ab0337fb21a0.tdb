 PAR  G(LIQUID,INLI),,                  +GLIQIN+GLIQLI
             -41511.85+3.7319*T;,,                                    N 22Gie !
 PAR  L(LIQUID,IN,INLI;0),,             -9400.39-15.3466*T;,,         N 22Gie !
 PAR  L(LIQUID,INLI,LI;0),,             -50379.64+20.279*T;,,         N 22Gie !
 PAR  L(LIQUID,INLI,LI;1),,             +3820.32-1.1837*T;,,          N 22Gie !
