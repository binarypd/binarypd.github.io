 PAR  L(FCC_A1,CR,SI:VA;0),,            -109560+4.98075*T;,,          N 95Lee !
 PAR  L(FCC_A1,CR,SI:VA;1),,            -59585+19.20022*T;,,          N 95Lee !
$
 PAR  G(M5SI3_D88,CR:SI),,              +GCR5SI3+16000;,,             N 95Lee !
