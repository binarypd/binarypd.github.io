 PAR  L(HCP_A3,MG,PB:VA;0),,            -30072.14;,,                  N 21Deb !
 PAR  L(HCP_A3,MG,PB:VA;1),,            -11061.87;,,                  N 21Deb !
$
 PAR  G(MG2PB_C1,MG:PB),,               +2*GHSERMG+GHSERPB
             -83415.42-2.4317*T+0.114*T*LN(T)+0.19534*T**2
             -2.63E-04*T**3+1.21E-07*T**4+702260*T**(-1);,,           N 21Deb !
