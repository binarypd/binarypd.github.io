$
 PAR  L(FCC_A1,AL,V:VA;0),,             -69947.2+12.33*T;,,           N 17Kro !
$
 PAR  L(BCC_A2,AL,V:VA;0),,             -89400+16.46*T;,,             N 17Kro !
 PAR  L(BCC_A2,AL,V:VA;1),,             -6000;,,                      N 17Kro !
