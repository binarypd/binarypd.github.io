$ Adjusted parameter below
 PAR  G(AL23V4,AL:V),,                  +23*GHSERAL+4*GHSERVV
             -432073+72.1548*T;,,                                     N 17Kro !
 PAR  G(AL3M_D022,AL:V),,               +3*GHSERAL+GHSERVV
             -108189.52+20.5856*T;,,                                  N 17Kro !
$
 PAR  G(AL8V5_D82,AL:AL:AL:V),,         +11*GHSERAL+2*GHSERVV
             -152100;,,                                               N 17Kro !
 PAR  G(AL8V5_D82,AL:AL:V:V),,          +8*GHSERAL+5*GHSERVV
             -304070+4.836*T;,,                                       N 17Kro !
 PAR  G(AL8V5_D82,AL:V:AL:V),,          +9*GHSERAL+4*GHSERVV
             -65000;,,                                                N 17Kro !
 PAR  G(AL8V5_D82,AL:V:V:V),,           +6*GHSERAL+7*GHSERVV
