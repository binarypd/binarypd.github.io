$
 PAR  G(SIGMA_D8B,NI:V:NI),,            +26*GHSERNI+4*GHSERVV
             +79717-127.17*T;,,                                       N 02Sun !
 PAR  G(SIGMA_D8B,NI:V:V),,             +10*GHSERNI+20*GHSERVV
             -450217+137.12*T;,,                                      N 02Sun !
 PAR  G(SIGMA_D8B,V:V:NI),,             +16*GHSERNI+14*GHSERVV
             +100000;,,                                               N 02Sun !
$
$ metastable
$
 PAR  L(HCP_A3,NI,V:VA;0),,             -36365.6+3.75677*T;,,         N Same !
 PAR  L(HCP_A3,NI,V:VA;1),,             +11860.7-9.0302*T;,,          N Same !
 PAR  L(HCP_A3,NI,V:VA;2),,             -10647.5+7.00954*T;,,         N Same !
