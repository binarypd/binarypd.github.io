 PAR  L(FCC_A1,NI,V:VA;2),,             -10647.5+7.00954*T;,,         N 98Kor !
$
 PAR  G(NI3V_D022,NI:V),,               -45524.96+529.01852*T
             -99.74166*T*LOG(T)-0.00824*T**2-13753.332*T**(-1);,,     N 98Kor !
 PAR  G(NI2V,NI:V),,                    -38032.065+337.2614*T
             -64.6973*T*LOG(T)-0.01512*T**2-13.75326*T**(-1);,,       N 98Kor !
 PAR  G(NI2V7_A15,NI:V),,               -190634.14+1333.90548*T
             -233.55668*T*LOG(T)-0.00482*T**2-5.196308E-06*T**3
             +844557.21*T**(-1);,,                                    N 98Kor !
$
 PAR  G(SIGMA_OLD,NI:V:NI),,             +26*GHSERNI+4*GHSERVV
             +4734.82-64.7436*T;,,                                    N 98Kor !
 PAR  G(SIGMA_OLD,NI:V:V),,              +8*GHSERNI+22*GHSERVV
             -447428+136.3269*T;,,                                    N 98Kor !
$
$PAR  G(SIGMA_OLD,NI:V:NI),,             -161645.05+3532.8443*T
$            -671.032*T*LN(T)-0.1382502*T**2+4.87E-07*T**3
$            +277840*T**(-1); 790.00 Y -161794.7+3572.6245*T
$            -678.096*T*LN(T)-0.1256082*T**2-2.72E-06*T**3;      1728 N 98Kor !
$PAR  G(SIGMA_OLD,NI:V:V),,              -663330.65+4012.7719*T
