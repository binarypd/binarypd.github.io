 ELEMENT PT   FCC_A1                    195.08      5723.712   41.6308 !
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : IN PT : !
$
$ Fcc (cF4) and MeX (cF8)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : IN PT : VA : !
$
$ Disordered part of L12_FCC, identical to FCC_A1
$
 PHASE A1_FCC %A 2 1 1 !
 CONST A1_FCC : IN PT : VA : !
$
$ Prototype AuCu3 (L1_2, cP4, Pm-3m)
$
 PHASE L12_FCC %AY 3 0.25 0.75 1 !
 CONST L12_FCC : IN% PT : IN PT% : VA :  !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : IN PT : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 %A 2 1 0.5 !
 CONST HCP_A3 : IN PT : VA : !
$
$ Prototype In (tI2, I4/mmm)
$
 PHASE TETRAGONAL_A6 % 1 1 !
 CONST TETRAGONAL_A6 : IN% PT : !
$
$ Prototype Ru3Sn7 (cI40, Im-3m)
$
 PHASE IN7PT3_D8F % 2 7 3 !
 CONST IN7PT3_D8F : IN : PT : !
$
$ Prototype CaF2 (cF12, Fm-3m)
$
 PHASE IN2PT_C1 % 2 2 1 !
 CONST IN2PT_C1 : IN : PT : !
$
$ Prototype Al3Ni2 (hP5, P-3m1)
$
 PHASE IN3PT2_D513 % 2 3 2 !
 CONST IN3PT2_D513 : IN : PT : !
$
$ Unknown structure
$
 PHASE INPT_HT % 2 1 1 !
 CONST INPT_HT : IN% PT : IN PT% : !
$
$ Prototype AlCu (mC20, C2/m)
$
 PHASE IN5PT6 % 2 5 6 !
 CONST IN5PT6 : IN% PT : IN PT% : !
$
$ Prototype Ga9Ni13 (mC44, C2/m)
$
 PHASE IN9PT13 % 2 9 13 !
 CONST IN9PT13 : IN : IN PT% : !
$
$ Prototype Pt3Tl2 (hP20, P6_3/mmc)
$
 PHASE IN2PT3_ALPHA % 2 2 3 !
 CONST IN2PT3_ALPHA : IN : PT : !
$
$ Prototype InNi2 (hP6, P6_3/mmc)
$
 PHASE IN2PT3_B82 % 2 2 3 !
 CONST IN2PT3_B82 : IN% PT : IN PT% : !
$
$ Prototype Ga3Pt5 (oC16, Cmmm)
$
 PHASE INPT2 % 2 1 2 !
 CONST INPT2 : IN : PT : !
