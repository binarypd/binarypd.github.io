$ In some works HfB has been found to have an NaCl type structure, but this
$ is probably caused by impurities.
$
 PAR  L(LIQUID,B,HF;0),,                -170625-4.85541*T;,,          N 97Bit !
 PAR  L(LIQUID,B,HF;1),,                -39128;,,                     N 97Bit !
 PAR  L(LIQUID,B,HF;2),,                +16331.1;,,                   N 97Bit !
$
 PAR  G(BCC_A2,HF:B),,                  +GBCCHF+3*GHSERBB
             -137926;,,                                               N 97Bit !
$
 PAR  G(HCP_A3,HF:B),,                  +GHSERHF+0.5*GHSERBB
             -44765.7;,,                                              N 97Bit !
$
 PAR  G(MB_B27,HF:B),,                  +GHSERHF+GHSERBB
             -159810+4.98602*T;,,                                     N 97Bit !
 PAR  G(MB2_C32,HF:B),,                 -351445+469.146*T
             -73.868*T*LN(T)-0.0036789*T**2+1517800*T**(-1)
             -5.2E+09*T**(-3);,,                                      N 97Bit !
