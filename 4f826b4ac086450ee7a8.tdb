$
$ Database for Pt-V from C.P. Wang et al. 2008
$
$ C.P. Wang, A.Q. Zheng, X.J. Liu, Intermetallics, 16, 544-49(2008).
$
$ Dataset created 2018.02.25 by Bengt Hallstedt.
$ 2021.02.05: Modified for use with GES6.
$
$ Invariant equilibria differ by up to 4 K.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT PT   FCC_A1                    195.08      5723.712   41.6308 !
 ELEMENT V    BCC_A2                     50.9415    4507.0     30.89   ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : PT V : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : PT% V : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : PT V% : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 % 2 1 0.5 !
 CONST HCP_A3 : PT V : VA : !
$
$ Prototype Al3Ti (tI8, I4/mmm)
$
 PHASE PT3V_D022 % 2 3 1 !
 CONST PT3V_D022 : PT% V : PT V% : !
$
$ Prototype MoPt2 (oI6, Immm)
$
 PHASE PT2V % 2 2 1 !
 CONST PT2V : PT% V : PT V% : !
$
$ Prototype AuCd (oP4, Pmma)
$
 PHASE PTV_B19 % 2 1 1 !
 CONST PTV_B19 : PT% V : PT V% : !
$
$ Prototype Cr3Si (cP8, Pm-3n)
$
 PHASE PTV3_A15 % 2 1 3 !
 CONST PTV3_A15 : PT% V : PT V% : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Pt
$
 PAR  G(FCC_A1,PT:VA),,                 +GHSERPT;                4000 N 91Din !
 PAR  G(BCC_A2,PT:VA),,                 +GHSERPT+15000-2.4*T;    4000 N 91Din !
 PAR  G(HCP_A3,PT:VA),,                 +GHSERPT+2500+0.1*T;     4000 N 91Din !
 PAR  G(LIQUID,PT),,                    +GLIQPT;                 4000 N 91Din !
$
 PAR  G(PT3V_D022,PT:PT),,              +4*GHSERPT+42000-2*T;,,       N 08Wan !
 PAR  G(PT2V,PT:PT),,                   +3*GHSERPT+1500;,,            N 08Wan !
 PAR  G(PTV_B19,PT:PT),,                +2*GHSERPT+3000;,,            N 08Wan !
 PAR  G(PTV3_A15,PT:PT),,               +4*GHSERPT+8120;,,            N 08Wan !
$
 FUNCTION GHSERPT   298.15  -7595.631+124.388275*T-24.5526*T*LN(T)
       -0.00248297*T**2-2.0138E-08*T**3+7974*T**(-1);
      1300.00  Y  -9253.174+161.529615*T-30.2527*T*LN(T)
       +0.002321665*T**2-6.56946E-07*T**3-272106*T**(-1);
      2041.50  Y  -222048.216+1019.35892*T-136.192996*T*LN(T)
       +0.020454938*T**2-7.59259E-07*T**3+71539020*T**(-1);
      4000.00  N !
 FUNCTION GLIQPT    298.15  +20114.016-9.275183*T+GHSERPT;
       600.00  Y  +19023.491+32.94182*T-12.3403769*T*LN(T)
       -0.011551507*T**2+9.31516E-07*T**3-601426*T**(-1);
      2041.50  Y  +1404.468+205.858962*T-36.5*T*LN(T);
      4000.00  N !
 FUNCTION GBCCPT    298.15  +GHSERPT+15000-2.4*T;                      6000 N !
$ ------------------------------------------------------------------------------
$ V
$
 PAR  G(BCC_A2,V:VA),,                  +GHSERVV;                4000 N 91Din !
 PAR  G(FCC_A1,V:VA),,                  +GHSERVV+7500+1.7*T;     4000 N 91Din !
 PAR  G(HCP_A3,V:VA),,                  +GHSERVV+4000+2.4*T;     4000 N 91Din !
$PAR  G(CBCC_A12,V:VA),,                +GHSERVV+9000;           4000 N 91Din !
$PAR  G(CUB_A13,V:VA),,                 +GHSERVV+10000;          4000 N 91Din !
 PAR  G(LIQUID,V),,                     +GLIQVV;                 4000 N 91Din !
$
 PAR  G(PT3V_D022,V:V),,                +4*GHSERVV+60000;,,           N 08Wan !
 PAR  G(PT2V,V:V),,                     +3*GHSERVV+10;,,              N 08Wan !
 PAR  G(PTV_B19,V:V),,                  +2*GHSERVV+6000;,,            N 08Wan !
 PAR  G(PTV3_A15,V:V),,                 +4*GHSERVV+10100;,,           N 08Wan !
$
 FUNCTION GHSERVV   298.15  -7930.43+133.346053*T-24.134*T*LN(T)
       -0.003098*T**2+1.2175E-07*T**3+69460*T**(-1);
       790.00  Y  -7967.842+143.291093*T-25.9*T*LN(T)
       +6.25E-05*T**2-6.8E-07*T**3;
      2183.00  Y  -41689.864+321.140783*T-47.43*T*LN(T)+6.44389E+31*T**(-9);
      4000.00  N !
 FUNCTION GLIQVV    298.15  +20764.117-9.455552*T+GHSERVV-5.19136E-22*T**7;
      2183.00  Y  -19617.51+311.055983*T-47.43*T*LN(T);
      4000.00  N !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ C.P. Wang, A.Q. Zheng, X.J. Liu, Intermetallics, 16, 544-49(2008).
$
$ Checked against paper. Checked at 6000 K.
$
$ Invariant equilibria differ by up to 4 K.
$
$ Hcp becomes stable in the centre of the system when the interaction is the
$ same as for fcc. +10 kJ/mol is added to the hcp interaction.
$
 PAR  L(LIQUID,PT,V;0),,                -70800-19.78*T;,,             N 08Wan !
 PAR  L(LIQUID,PT,V;1),,                -90700+16.54*T;,,             N 08Wan !
$
 PAR  L(FCC_A1,PT,V:VA;0),,             -131860+1.08*T;,,             N 08Wan !
 PAR  L(FCC_A1,PT,V:VA;1),,             -48733-0.838*T;,,             N 08Wan !
$
 PAR  L(BCC_A2,PT,V:VA;0),,             -69500-4.6*T;,,               N 08Wan !
 PAR  L(BCC_A2,PT,V:VA;1),,             +20000-9.8*T;,,               N 08Wan !
 PAR  L(BCC_A2,PT,V:VA;2),,             +8000-2.5*T;,,                N 08Wan !
$
 PAR  G(PT3V_D022,PT:V),,               +3*GHSERPT+GHSERVV
             -123140-5.772*T;,,                                       N 08Wan !
 PAR  G(PT3V_D022,V:PT),,               +3*GHSERVV+GHSERPT
             +86920+5.772*T;,,                                        N 08Wan !
 PAR  L(PT3V_D022,PT,V:V;0),,           -299200+78.436*T;,,           N 08Wan !
 PAR  L(PT3V_D022,PT,V:V;1),,           -123200+99.736*T;,,           N 08Wan !
 PAR  L(PT3V_D022,PT:PT,V;0),,          -83200+10.748*T;,,            N 08Wan !
$
 PAR  G(PT2V,PT:V),,                    +2*GHSERPT+GHSERVV
             -112815+2.19*T;,,                                        N 08Wan !
 PAR  G(PT2V,V:PT),,                    +2*GHSERVV+GHSERPT
             +114300-2.19*T;,,                                        N 08Wan !
 PAR  L(PT2V,PT:PT,V;0),,               +6000-30*T;,,                 N 08Wan !
$
 PAR  G(PTV_B19,PT:V),,                 +GHSERPT+GHSERVV
             -73100-0.916*T;,,                                        N 08Wan !
 PAR  G(PTV_B19,V:PT),,                 +GHSERVV+GHSERPT
             +73100+0.916*T;,,                                        N 08Wan !
 PAR  L(PTV_B19,PT,V:V;0),,             +19400;,,                     N 08Wan !
$
 PAR  G(PTV3_A15,PT:V),,                +GHSERPT+3*GHSERVV
             -82000-4.76*T;,,                                         N 08Wan !
 PAR  G(PTV3_A15,V:PT),,                +GHSERVV+3*GHSERPT
             +46000+4.76*T;,,                                         N 08Wan !
 PAR  L(PTV3_A15,PT:PT,V;0),,           -204000-4.364*T;,,            N 08Wan !
$
$ Metastable
$
 PAR  L(HCP_A3,PT,V:VA;0),,             -131860+1.08*T+10000;,,       N Same !
 PAR  L(HCP_A3,PT,V:VA;1),,             -48733-0.838*T;,,             N Same !
