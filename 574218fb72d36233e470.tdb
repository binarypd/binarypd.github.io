$
 PAR  L(LIQUID,AG,OS;0),,               +55975;,,                     N 86Kar !
 PAR  L(LIQUID,AG,OS;1),,               -3375;,,                      N 86Kar !
$
 PAR  L(FCC_A1,AG,OS:VA;0),,            +80*T;,,                      N !
 PAR  L(HCP_A3,AG,OS:VA;0),,            +80*T;,,                      N !
