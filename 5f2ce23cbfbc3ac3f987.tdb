$
$ Database for Re-Si from G. Shao 2001
$
$ G. Shao, Intermetallics, 9, 1063-68(2001).
$
$ Dataset created 2017.07.01 by Bengt Hallstedt.
$ 2021.02.05: Modified for use with GES6.
$
$ Old unary data for Re are used. If current data are used the phase diagram
$ is not reproduced.
$
$ ------------------------------------------------------------------------------
 TEMP-LIM 298.15 6000.00 !
$
$ELEMENT NAME  REF. STATE               ATOMIC MASS H298-H0    S298    !
$
 ELEMENT VA   VACUUM                      0.0          0.0      0.0    ! 
 ELEMENT RE   HCP_A3                    186.207     5333.      36.4820 ! 
 ELEMENT SI   DIAMOND_A4                 28.0855    3217.      18.81   ! 
$ ------------------------------------------------------------------------------
$ Phase definitions
$
 PHASE LIQUID:L % 1 1 !
 CONST LIQUID:L : RE SI : !
$
$ Fcc (cF4, Fm-3m) and MeX (cF8, Fm-3m)
$
 PHASE FCC_A1 %A 2 1 1 !
 CONST FCC_A1 : RE SI : VA : !
$
$ Bcc (cI2, Im-3m)
$
 PHASE BCC_A2 %B 2 1 3 !
 CONST BCC_A2 : RE SI : VA : !
$
$ Hcp (hP2, P6_3/mmc) and Me2X (NiAs-type, hP4, P6_3/mmc, B8_1)
$
 PHASE HCP_A3 %A 2 1 0.5 !
 CONST HCP_A3 : RE% SI : VA : !
$
$ Prototype C (cF8, Fd-3m)
$
 PHASE DIAMOND_A4 % 1 1 !
 CONST DIAMOND_A4 : SI : !
$
$ Prototype Re2Si (mP24, P2_1/c)
$
 PHASE RE2SI % 2 2 1 !
 CONST RE2SI : RE : SI : !
$
$ Prototype FeSi (cP8, P2_13)
$
 PHASE RESI_B20 % 2 1 1 !
 CONST RESI_B20 : RE : SI : !
$
$ Prototype Re4Si7 (mC44, Cm)
$
 PHASE RESI2 % 2 1 1.8 !
 CONST RESI2 : RE : SI : !
$ ------------------------------------------------------------------------------
$ Defaults
$
 DEFINE-SYSTEM-DEFAULT ELEMENT 2 !
 DEFAULT-COM DEFINE_SYSTEM_ELEMENT VA !
 TYPE-DEF % SEQ * !
 TYPE-DEF A GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -3 0.28 !
 TYPE-DEF B GES AMEND_PHASE_DESCRIPTION @ MAGNETIC -1 0.4 !
 FUNCTION ZERO      298.15  0;                                         6000 N !
 FUNCTION UN_ASS    298.15  0;                                         6000 N !
 FUNCTION R         298.15  +8.31451;                                  6000 N !
$ ------------------------------------------------------------------------------
$ Element data
$ ------------------------------------------------------------------------------
$ Re
$
$ HCP_A3 and LIQUID revised in unary 3.0
$ BCC_A2 revised in unary 4.4
$
$ Note! Old unary data for Re are used.
$
 PAR  G(HCP_A3,RE:VA),,                 +GHSERRE;,,                   N 91Din !
 PAR  G(FCC_A1,RE:VA),,                 +GHSERRE+11000-1.5*T;,,       N 91Din !
 PAR  G(BCC_A2,RE:VA),,                 +GHSERRE+17000-3.7*T;,,       N 01SGUN !
$PAR  G(BCC_A2,RE:VA),,                 +GHSERRE+29200-6*T;,,         N 91Din !
 PAR  G(LIQUID,RE),,                    +GLIQRE;,,                    N 91Din !
$
 FUNCTION GHSERRE   298.15  -7305.045+123.758773*T-23.6892641*T*LN(T)
       -0.002723387*T**2;
      3459.00  Y  +38514.17-65.920606*T+.0645635*T*LN(T)
       -0.008050184*T**2+2.19551E-07*T**3-15938253*T**(-1);
      6000.00  N !
 FUNCTION GLIQRE    298.15  +45296.708-11.184298*T+GHSERRE;  
      1000.00  Y  +66060.979-113.540121*T+7.4580354*T*LN(T)
       -0.016183804*T**2+7.69496E-07*T**3-4421721*T**(-1);
      3459.00  Y  -5301.43+328.158541*T-50*T*LN(T);
      6000.00  N !
$ ------------------------------------------------------------------------------
$ Si
$
 PAR  G(DIAMOND_A4,SI),,                +GHSERSI;                3600 N 91Din !
 PAR  G(FCC_A1,SI:VA),,                 +GHSERSI+51000-21.8*T;   3600 N 91Din !
 PAR  G(BCC_A2,SI:VA),,                 +GHSERSI+47000-22.5*T;   3600 N 91Din !
 PAR  G(HCP_A3,SI:VA),,                 +GHSERSI+49200-20.8*T;   3600 N 91Din !
$PAR  G(CBCC_A12,SI:VA),,               +GHSERSI+50208-20.377*T; 3600 N 91Din !
$PAR  G(CUB_A13,SI:VA),,                +GHSERSI+47279-20.377*T; 3600 N 91Din !
 PAR  G(LIQUID,SI),,                    +GLIQSI;                 3600 N 91Din !
$
 FUNCTION GHSERSI   298.15  -8162.609+137.236859*T-22.8317533*T*LN(T)
       -0.001912904*T**2-3.552E-09*T**3+176667*T**(-1);
      1687.00  Y  -9457.642+167.281367*T-27.196*T*LN(T)-4.20369E+30*T**(-9);
      3600.00  N !
 FUNCTION GLIQSI    298.15  +50696.36-30.099439*T+GHSERSI+2.09307E-21*T**7;
      1687.00  Y  +40370.523+137.722298*T-27.196*T*LN(T);
      3600.00  N !
 FUNCTION GBCCSI    298.15  +GHSERSI+47000-22.5*T;               3600 N 91Din !
$ ------------------------------------------------------------------------------
$ Binary system
$
$ G. Shao, Intermetallics, 9, 1063-68(2001).
$
$ Checked against paper. Checked at 6000 K.
$
$ Old unary data for Re are used. If current data are used the phase diagram
$ is not reproduced.
$
 PAR  L(LIQUID,RE,SI;0),,               -96700;,,                     N 01Sha !
 PAR  L(LIQUID,RE,SI;1),,               -45000;,,                     N 01Sha !
 PAR  L(LIQUID,RE,SI;2),,               -60000;,,                     N 01Sha !
$
 PAR  L(HCP_A3,RE,SI:VA;0),,            -48000;,,                     N 01Sha !
 PAR  L(HCP_A3,RE,SI:VA;1),,            -3000;,,                      N 01Sha !
$
 PAR  G(RE2SI,RE:SI),,                  +2*GHSERRE+GHSERSI
             -68100-5.58*T;,,                                         N 01Sha !
 PAR  G(RESI_B20,RE:SI),,               +GHSERRE+GHSERSI
             -42960-10*T;,,                                           N 01Sha !
 PAR  G(RESI2,RE:SI),,                  +GHSERRE+1.8*GHSERSI
             -87360-5.88*T;,,                                         N 01Sha !
$
$ metastable
$
 PAR  L(FCC_A1,RE,SI:VA;0),,            -48000;,,                     N Same !
 PAR  L(FCC_A1,RE,SI:VA;1),,            -3000;,,                      N Same !
$
 PAR  L(BCC_A2,RE,SI:VA;0),,            -48000;,,                     N Same !
 PAR  L(BCC_A2,RE,SI:VA;1),,            -3000;,,                      N Same !
$ ------------------------------------------------------------------------------
$
$
 LIST-OF-REFERENCE
 NUMBER  SOURCE
  Null   'Unknown source'
  Same   'Same or similar interaction as in the corresponding stable phase'
  REFLAV 'Laves phase convention: G(LAVES,X:X)=+3*GHSERXX+15000'
  91Din  'A.T. Dinsdale, Calphad, 15, 317-425(1991).'
  96Arb  'J.W. Arblaster, Calphad, 20, 343-52(1996),
          SGTE unary database V.3.0, 1996; Re'
  01Sha  'G. Shao, Intermetallics, 9, 1063-68(2001); Re-Si'
  01SGUN 'SGTE unary database V.4.4, 2001.'
!
