 PAR  L(LIQUID,BE,FE;0),,               -73300-1.8*T;,,               N 04Oht !
 PAR  L(LIQUID,BE,FE;1),,               -8500+5.57*T;,,               N 04Oht !
$
 PAR  L(FCC_A1,BE,FE:VA;0),,            -71900+8.1*T;,,               N 04Oht !
