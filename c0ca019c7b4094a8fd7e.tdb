 PAR  G(BCC_B2,BE:FE:VA),,              +0.5*GBCCBE+0.5*GHSERFE
             -37100+9*T;,,                                            N 04Oht !
 PAR  G(BCC_B2,FE:BE:VA),,              +0.5*GBCCBE+0.5*GHSERFE
             -37100+9*T;,,                                            N 04Oht !
 PAR  L(BCC_B2,BE,FE:BE:VA;0),,         -4*T;,,                       N 04Oht !
