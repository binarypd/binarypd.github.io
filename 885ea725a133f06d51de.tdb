 PAR  G(AGZN_BB,ZN:AG),,                +GBCCZN+2*GBCCAG-27200;,,     N 02Fri !
 PAR  L(AGZN_BB,ZN:AG,ZN;0),,           -32300;,,                     N 02Fri !
$
 PAR  G(AG5ZN8_D82,ZN:AG:AG:AG),,       +11*GBCCAG+2*GBCCZN
             +GAGZNK0+6*GAGZNK4;,,                                    N 02Fri !
 PAR  G(AG5ZN8_D82,AG:AG:ZN:AG),,       +10*GBCCAG+3*GBCCZN
             +GAGZNK0+2*GAGZNK1+3*GAGZNK3+6*GAGZNK4;,,                N 02Fri !
 PAR  G(AG5ZN8_D82,ZN:AG:ZN:AG),,       +8*GBCCAG+5*GBCCZN
             +GAGZNK0+3*GAGZNK3+6*GAGZNK4;,,                          N 02Fri !
 PAR  G(AG5ZN8_D82,AG:AG:AG:ZN),,       +7*GBCCAG+6*GBCCZN
             +GAGZNK0+2*GAGZNK1;,,                                    N 02Fri !
 PAR  G(AG5ZN8_D82,ZN:AG:AG:ZN),,       +5*GBCCAG+8*GBCCZN+GAGZNK0;,, N 02Fri !
 PAR  G(AG5ZN8_D82,AG:AG:ZN:ZN),,       +4*GBCCAG+9*GBCCZN
             +GAGZNK0+2*GAGZNK1+3*GAGZNK3;,,                          N 02Fri !
